[32m+[m[32m    jQuery.swap = function (elem, options, callback, args) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret, name,[m[41m[m
[32m+[m			[32mold = {};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Remember the old values, and insert the new ones[m[41m[m
[32m+[m[32m        for (name in options) {[m[41m[m
[32m+[m[32m            old[name] = elem.style[name];[m[41m[m
[32m+[m[32m            elem.style[name] = options[name];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ret = callback.apply(elem, args || []);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Revert the old values[m[41m[m
[32m+[m[32m        for (name in options) {[m[41m[m
[32m+[m[32m            elem.style[name] = old[name];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return ret;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.text = function (elem) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var node,[m[41m[m
[32m+[m[32m            ret = "",[m[41m[m
[32m+[m[32m            i = 0,[m[41m[m
[32m+[m[32m            nodeType = elem.nodeType;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!nodeType) {[m[41m[m
[32m+[m[32m            // If no nodeType, this is expected to be an array[m[41m[m
[32m+[m[32m            for (; (node = elem[i]) ; i++) {[m[41m[m
[32m+[m[32m                // Do not traverse comment nodes[m[41m[m
[32m+[m[32m                ret += getText(node);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        } else if (nodeType === 1 || nodeType === 9 || nodeType === 11) {[m[41m[m
[32m+[m[32m            // Use textContent for elements[m[41m[m
[32m+[m[32m            // innerText usage removed for consistency of new lines (see #11153)[m[41m[m
[32m+[m[32m            if (typeof elem.textContent === "string") {[m[41m[m
[32m+[m[32m                return elem.textContent;[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                // Traverse its children[m[41m[m
[32m+[m[32m                for (elem = elem.firstChild; elem; elem = elem.nextSibling) {[m[41m[m
[32m+[m[32m                    ret += getText(elem);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        } else if (nodeType === 3 || nodeType === 4) {[m[41m[m
[32m+[m[32m            return elem.nodeValue;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        // Do not include comment or processing instruction nodes[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return ret;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.trim = function (text) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Remove the whitespace from the beginning and end of a string.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="text" type="String">[m[41m[m
[32m+[m[32m        ///     The string to trim.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="String" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return text == null ?[m[41m[m
[32m+[m[32m            "" :[m[41m[m
[32m+[m[32m            core_trim.call(text);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.type = function (obj) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Determine the internal JavaScript [[Class]] of an object.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="obj" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     Object to get the internal JavaScript [[Class]] of.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="String" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (obj == null) {[m[41m[m
[32m+[m[32m            return String(obj);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return typeof obj === "object" || typeof obj === "function" ?[m[41m[m
[32m+[m			[32mclass2type[core_toString.call(obj)] || "object" :[m[41m[m
[32m+[m			[32mtypeof obj;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.unique = function (results) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Sorts an array of DOM elements, in place, with the duplicates removed. Note that this only works on arrays of DOM elements, not strings or numbers.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="results" type="Array">[m[41m[m
[32m+[m[32m        ///     The Array of DOM elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Array" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var elem,[m[41m[m
[32m+[m[32m            duplicates = [],[m[41m[m
[32m+[m[32m            i = 1,[m[41m[m
[32m+[m[32m            j = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Unless we *know* we can detect duplicates, assume their presence[m[41m[m
[32m+[m[32m        hasDuplicate = !support.detectDuplicates;[m[41m[m
[32m+[m[32m        results.sort(sortOrder);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (hasDuplicate) {[m[41m[m
[32m+[m[32m            for (; (elem = results[i]) ; i++) {[m[41m[m
[32m+[m[32m                if (elem === results[i - 1]) {[m[41m[m
[32m+[m[32m                    j = duplicates.push(i);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            while (j--) {[m[41m[m
[32m+[m[32m                results.splice(duplicates[j], 1);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return results;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.valHooks = {[m[41m[m
[32m+[m[32m        "option": {},[m[41m[m
[32m+[m[32m        "select": {},[m[41m[m
[32m+[m[32m        "radio": {},[m[41m[m
[32m+[m[32m        "checkbox": {}[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.when = function (subordinate /* , ..., subordinateN */) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Provides a way to execute callback functions based on one or more objects, usually Deferred objects that represent asynchronous events.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="subordinate/*" type="Deferred">[m[41m[m
[32m+[m[32m        ///     One or more Deferred objects, or plain JavaScript objects.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Promise" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var i = 0,[m[41m[m
[32m+[m			[32mresolveValues = core_slice.call(arguments),[m[41m[m
[32m+[m			[32mlength = resolveValues.length,[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// the count of uncompleted subordinates[m[41m[m
[32m+[m			[32mremaining = length !== 1 || (subordinate && jQuery.isFunction(subordinate.promise)) ? length : 0,[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// the master Deferred. If resolveValues consist of only a single Deferred, just use that.[m[41m[m
[32m+[m			[32mdeferred = remaining === 1 ? subordinate : jQuery.Deferred(),[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// Update function for both resolve and progress values[m[41m[m
[32m+[m			[32mupdateFunc = function (i, contexts, values) {[m[41m[m
[32m+[m			[32m    return function (value) {[m[41m[m
[32m+[m			[32m        contexts[i] = this;[m[41m[m
[32m+[m			[32m        values[i] = arguments.length > 1 ? core_slice.call(arguments) : value;[m[41m[m
[32m+[m			[32m        if (values === progressValues) {[m[41m[m
[32m+[m			[32m            deferred.notifyWith(contexts, values);[m[41m[m
[32m+[m			[32m        } else if (!(--remaining)) {[m[41m[m
[32m+[m			[32m            deferred.resolveWith(contexts, values);[m[41m[m
[32m+[m			[32m        }[m[41m[m
[32m+[m			[32m    };[m[41m[m
[32m+[m			[32m},[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mprogressValues, progressContexts, resolveContexts;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // add listeners to Deferred subordinates; treat others as resolved[m[41m[m
[32m+[m[32m        if (length > 1) {[m[41m[m
[32m+[m[32m            progressValues = new Array(length);[m[41m[m
[32m+[m[32m            progressContexts = new Array(length);[m[41m[m
[32m+[m[32m            resolveContexts = new Array(length);[m[41m[m
[32m+[m[32m            for (; i < length; i++) {[m[41m[m
[32m+[m[32m                if (resolveValues[i] && jQuery.isFunction(resolveValues[i].promise)) {[m[41m[m
[32m+[m[32m                    resolveValues[i].promise()[m[41m[m
[32m+[m						[32m.done(updateFunc(i, resolveContexts, resolveValues))[m[41m[m
[32m+[m						[32m.fail(deferred.reject)[m[41m[m
[32m+[m						[32m.progress(updateFunc(i, progressContexts, progressValues));[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    --remaining;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // if we're not waiting on anything, resolve the master[m[41m[m
[32m+[m[32m        if (!remaining) {[m[41m[m
[32m+[m[32m            deferred.resolveWith(resolveContexts, resolveValues);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return deferred.promise();[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Event.prototype.isDefaultPrevented = function returnFalse() {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Returns whether event.preventDefault() was ever called on this event object.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return false;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Event.prototype.isImmediatePropagationStopped = function returnFalse() {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Returns whether event.stopImmediatePropagation() was ever called on this event object.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return false;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Event.prototype.isPropagationStopped = function returnFalse() {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Returns whether event.stopPropagation() was ever called on this event object.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return false;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Event.prototype.preventDefault = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     If this method is called, the default action of the event will not be triggered.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="undefined" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var e = this.originalEvent;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        this.isDefaultPrevented = returnTrue;[m[41m[m
[32m+[m[32m        if (!e) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If preventDefault exists, run it on the original event[m[41m[m
[32m+[m[32m        if (e.preventDefault) {[m[41m[m
[32m+[m[32m            e.preventDefault();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Support: IE[m[41m[m
[32m+[m[32m            // Otherwise set the returnValue property of the original event to false[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            e.returnValue = false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Event.prototype.stopImmediatePropagation = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Keeps the rest of the handlers from being executed and prevents the event from bubbling up the DOM tree.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        this.isImmediatePropagationStopped = returnTrue;[m[41m[m
[32m+[m[32m        this.stopPropagation();[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Event.prototype.stopPropagation = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Prevents the event from bubbling up the DOM tree, preventing any parent handlers from being notified of the event.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var e = this.originalEvent;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        this.isPropagationStopped = returnTrue;[m[41m[m
[32m+[m[32m        if (!e) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        // If stopPropagation exists, run it on the original event[m[41m[m
[32m+[m[32m        if (e.stopPropagation) {[m[41m[m
[32m+[m[32m            e.stopPropagation();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Support: IE[m[41m[m
[32m+[m[32m        // Set the cancelBubble property of the original event to true[m[41m[m
[32m+[m[32m        e.cancelBubble = true;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.add = function (selector, context) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Add elements to the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - add(selector)[m[41m [m
[32m+[m[32m        ///     &#10;2 - add(elements)[m[41m [m
[32m+[m[32m        ///     &#10;3 - add(html)[m[41m [m
[32m+[m[32m        ///     &#10;4 - add(jQuery object)[m[41m [m
[32m+[m[32m        ///     &#10;5 - add(selector, context)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string representing a selector expression to find additional elements to add to the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="context" domElement="true">[m[41m[m
[32m+[m[32m        ///     The point in the document at which the selector should begin matching; similar to the context argument of the $(selector, context) method.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var set = typeof selector === "string" ?[m[41m[m
[32m+[m				[32mjQuery(selector, context) :[m[41m[m
[32m+[m				[32mjQuery.makeArray(selector && selector.nodeType ? [selector] : selector),[m[41m[m
[32m+[m			[32mall = jQuery.merge(this.get(), set);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(jQuery.unique(all));[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.addBack = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Add the previous set of elements on the stack to the current set, optionally filtered by a selector.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match the current set of elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.add(selector == null ?[m[41m[m
[32m+[m			[32mthis.prevObject : this.prevObject.filter(selector)[m[41m[m
[32m+[m		[32m);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.addClass = function (value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Adds the specified class(es) to each of the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - addClass(className)[m[41m [m
[32m+[m[32m        ///     &#10;2 - addClass(function(index, currentClass))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="value" type="String">[m[41m[m
[32m+[m[32m        ///     One or more space-separated classes to be added to the class attribute of each matched element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var classes, elem, cur, clazz, j,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mlen = this.length,[m[41m[m
[32m+[m			[32mproceed = typeof value === "string" && value;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (jQuery.isFunction(value)) {[m[41m[m
[32m+[m[32m            return this.each(function (j) {[m[41m[m
[32m+[m[32m                jQuery(this).addClass(value.call(this, j, this.className));[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (proceed) {[m[41m[m
[32m+[m[32m            // The disjunction here is for better compressibility (see removeClass)[m[41m[m
[32m+[m[32m            classes = (value || "").match(core_rnotwhite) || [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (; i < len; i++) {[m[41m[m
[32m+[m[32m                elem = this[i];[m[41m[m
[32m+[m[32m                cur = elem.nodeType === 1 && (elem.className ?[m[41m[m
[32m+[m					[32m(" " + elem.className + " ").replace(rclass, " ") :[m[41m[m
[32m+[m					[32m" "[m[41m[m
[32m+[m				[32m);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (cur) {[m[41m[m
[32m+[m[32m                    j = 0;[m[41m[m
[32m+[m[32m                    while ((clazz = classes[j++])) {[m[41m[m
[32m+[m[32m                        if (cur.indexOf(" " + clazz + " ") < 0) {[m[41m[m
[32m+[m[32m                            cur += clazz + " ";[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    elem.className = jQuery.trim(cur);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.after = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Insert content, specified by the parameter, after each element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - after(content, content)[m[41m [m
[32m+[m[32m        ///     &#10;2 - after(function(index))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="" type="jQuery">[m[41m[m
[32m+[m[32m        ///     HTML string, DOM element, or jQuery object to insert after each element in the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="" type="jQuery">[m[41m[m
[32m+[m[32m        ///     One or more additional DOM elements, arrays of elements, HTML strings, or jQuery objects to insert after each element in the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.domManip(arguments, false, function (elem) {[m[41m[m
[32m+[m[32m            if (this.parentNode) {[m[41m[m
[32m+[m[32m                this.parentNode.insertBefore(elem, this.nextSibling);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
