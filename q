[32m+[m[32m    jQuery.prototype.next = function (until, selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the immediately following sibling of each element in the set of matched elements. If a selector is provided, it retrieves the next sibling only if it matches that selector.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="until" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret = jQuery.map(this, fn, until);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!runtil.test(name)) {[m[41m[m
[32m+[m[32m            selector = until;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (selector && typeof selector === "string") {[m[41m[m
[32m+[m[32m            ret = jQuery.filter(selector, ret);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ret = this.length > 1 && !guaranteedUnique[name] ? jQuery.unique(ret) : ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (this.length > 1 && rparentsprev.test(name)) {[m[41m[m
[32m+[m[32m            ret = ret.reverse();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.nextAll = function (until, selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get all following siblings of each element in the set of matched elements, optionally filtered by a selector.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="until" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret = jQuery.map(this, fn, until);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!runtil.test(name)) {[m[41m[m
[32m+[m[32m            selector = until;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (selector && typeof selector === "string") {[m[41m[m
[32m+[m[32m            ret = jQuery.filter(selector, ret);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ret = this.length > 1 && !guaranteedUnique[name] ? jQuery.unique(ret) : ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (this.length > 1 && rparentsprev.test(name)) {[m[41m[m
[32m+[m[32m            ret = ret.reverse();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.nextUntil = function (until, selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get all following siblings of each element up to but not including the element matched by the selector, DOM node, or jQuery object passed.[m[41m[m
[32m+[m[32m        ///     &#10;1 - nextUntil(selector, filter)[m[41m [m
[32m+[m[32m        ///     &#10;2 - nextUntil(element, filter)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="until" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to indicate where to stop matching following sibling elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret = jQuery.map(this, fn, until);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!runtil.test(name)) {[m[41m[m
[32m+[m[32m            selector = until;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (selector && typeof selector === "string") {[m[41m[m
[32m+[m[32m            ret = jQuery.filter(selector, ret);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ret = this.length > 1 && !guaranteedUnique[name] ? jQuery.unique(ret) : ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (this.length > 1 && rparentsprev.test(name)) {[m[41m[m
[32m+[m[32m            ret = ret.reverse();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.not = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Remove elements from the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - not(selector)[m[41m [m
[32m+[m[32m        ///     &#10;2 - not(elements)[m[41m [m
[32m+[m[32m        ///     &#10;3 - not(function(index))[m[41m [m
[32m+[m[32m        ///     &#10;4 - not(jQuery object)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(winnow(this, selector, false));[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.off = function (types, selector, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Remove an event handler.[m[41m[m
[32m+[m[32m        ///     &#10;1 - off(events, selector, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - off(events, selector)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="types" type="String">[m[41m[m
[32m+[m[32m        ///     One or more space-separated event types and optional namespaces, or just namespaces, such as "click", "keydown.myPlugin", or ".myPlugin".[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A selector which should match the one originally passed to .on() when attaching event handlers.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A handler function previously attached for the event(s), or the special value false.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var handleObj, type;[m[41m[m
[32m+[m[32m        if (types && types.preventDefault && types.handleObj) {[m[41m[m
[32m+[m[32m            // ( event )  dispatched jQuery.Event[m[41m[m
[32m+[m[32m            handleObj = types.handleObj;[m[41m[m
[32m+[m[32m            jQuery(types.delegateTarget).off([m[41m[m
[32m+[m				[32mhandleObj.namespace ? handleObj.origType + "." + handleObj.namespace : handleObj.origType,[m[41m[m
[32m+[m				[32mhandleObj.selector,[m[41m[m
[32m+[m				[32mhandleObj.handler[m[41m[m
[32m+[m			[32m);[m[41m[m
[32m+[m[32m            return this;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        if (typeof types === "object") {[m[41m[m
[32m+[m[32m            // ( types-object [, selector] )[m[41m[m
[32m+[m[32m            for (type in types) {[m[41m[m
[32m+[m[32m                this.off(type, selector, types[type]);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return this;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        if (selector === false || typeof selector === "function") {[m[41m[m
[32m+[m[32m            // ( types [, fn] )[m[41m[m
[32m+[m[32m            fn = selector;[m[41m[m
[32m+[m[32m            selector = undefined;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        if (fn === false) {[m[41m[m
[32m+[m[32m            fn = returnFalse;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return this.each(function () {[m[41m[m
[32m+[m[32m            jQuery.event.remove(this, types, fn, selector);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.offset = function (options) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Get the current coordinates of the first element in the set of matched elements, relative to the document.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - offset()[m[41m[m
[32m+[m[32m        ///     &#10;2: Set the current coordinates of every element in the set of matched elements, relative to the document.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - offset(coordinates)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - offset(function(index, coords))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="options" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     An object containing the properties top and left, which are integers indicating the new top and left coordinates for the elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (arguments.length) {[m[41m[m
[32m+[m[32m            return options === undefined ?[m[41m[m
[32m+[m[32m                this :[m[41m[m
[32m+[m[32m                this.each(function (i) {[m[41m[m
[32m+[m[32m                    jQuery.offset.setOffset(this, options, i);[m[41m[m
[32m+[m[32m                });[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var docElem, win,[m[41m[m
[32m+[m[32m            box = { top: 0, left: 0 },[m[41m[m
[32m+[m[32m            elem = this[0],[m[41m[m
[32m+[m[32m            doc = elem && elem.ownerDocument;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!doc) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        docElem = doc.documentElement;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Make sure it's not a disconnected DOM node[m[41m[m
[32m+[m[32m        if (!jQuery.contains(docElem, elem)) {[m[41m[m
[32m+[m[32m            return box;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If we don't have gBCR, just use 0,0 rather than error[m[41m[m
[32m+[m[32m        // BlackBerry 5, iOS 3 (original iPhone)[m[41m[m
[32m+[m[32m        if (typeof elem.getBoundingClientRect !== core_strundefined) {[m[41m[m
[32m+[m[32m            box = elem.getBoundingClientRect();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        win = getWindow(doc);[m[41m[m
[32m+[m[32m        return {[m[41m[m
[32m+[m[32m            top: box.top + (win.pageYOffset || docElem.scrollTop) - (docElem.clientTop || 0),[m[41m[m
[32m+[m[32m            left: box.left + (win.pageXOffset || docElem.scrollLeft) - (docElem.clientLeft || 0)[m[41m[m
[32m+[m[32m        };[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.offsetParent = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the closest ancestor element that is positioned.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.map(function () {[m[41m[m
[32m+[m[32m            var offsetParent = this.offsetParent || document.documentElement;[m[41m[m
[32m+[m[32m            while (offsetParent && (!jQuery.nodeName(offsetParent, "html") && jQuery.css(offsetParent, "position") === "static")) {[m[41m[m
[32m+[m[32m                offsetParent = offsetParent.offsetParent;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return offsetParent || document.documentElement;[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.on = function (types, selector, data, fn, /*INTERNAL*/ one) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Attach an event handler function for one or more events to the selected elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - on(events, selector, data, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - on(events, selector, data)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="types" type="String">[m[41m[m
[32m+[m[32m        ///     One or more space-separated event types and optional namespaces, such as "click" or "keydown.myPlugin".[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A selector string to filter the descendants of the selected elements that trigger the event. If the selector is null or omitted, the event is always triggered when it reaches the selected element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Anything">[m[41m[m
[32m+[m[32m        ///     Data to be passed to the handler in event.data when an event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute when the event is triggered. The value false is also allowed as a shorthand for a function that simply does return false.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var type, origFn;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Types can be a map of types/handlers[m[41m[m
[32m+[m[32m        if (typeof types === "object") {[m[41m[m
[32m+[m[32m            // ( types-Object, selector, data )[m[41m[m
[32m+[m[32m            if (typeof selector !== "string") {[m[41m[m
[32m+[m[32m                // ( types-Object, data )[m[41m[m
[32m+[m[32m                data = data || selector;[m[41m[m
[32m+[m[32m                selector = undefined;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            for (type in types) {[m[41m[m
[32m+[m[32m                this.on(type, selector, data, types[type], one);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return this;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (data == null && fn == null) {[m[41m[m
[32m+[m[32m            // ( types, fn )[m[41m[m
[32m+[m[32m            fn = selector;[m[41m[m
[32m+[m[32m            data = selector = undefined;[m[41m[m
[32m+[m[32m        } else if (fn == null) {[m[41m[m
[32m+[m[32m            if (typeof selector === "string") {[m[41m[m
[32m+[m[32m                // ( types, selector, fn )[m[41m[m
[32m+[m[32m                fn = data;[m[41m[m
[32m+[m[32m                data = undefined;[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                // ( types, data, fn )[m[41m[m
[32m+[m[32m                fn = data;[m[41m[m
[32m+[m[32m                data = selector;[m[41m[m
[32m+[m[32m                selector = undefined;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        if (fn === false) {[m[41m[m
[32m+[m[32m            fn = returnFalse;[m[41m[m
[32m+[m[32m        } else if (!fn) {[m[41m[m
[32m+[m[32m            return this;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (one === 1) {[m[41m[m
[32m+[m[32m            origFn = fn;[m[41m[m
[32m+[m[32m            fn = function (event) {[m[41m[m
[32m+[m[32m                // Can use an empty set, since event contains the info[m[41m[m
[32m+[m[32m                jQuery().off(event);[m[41m[m
[32m+[m[32m                return origFn.apply(this, arguments);[m[41m[m
[32m+[m[32m            };[m[41m[m
[32m+[m[32m            // Use same guid so caller can remove using origFn[m[41m[m
[32m+[m[32m            fn.guid = origFn.guid || (origFn.guid = jQuery.guid++);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return this.each(function () {[m[41m[m
[32m+[m[32m            jQuery.event.add(this, types, fn, data, selector);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.one = function (types, selector, data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Attach a handler to an event for the elements. The handler is executed at most once per element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - one(events, data, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - one(events, selector, data, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;3 - one(events, selector, data)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="types" type="String">[m[41m[m
[32m+[m[32m        ///     One or more space-separated event types and optional namespaces, such as "click" or "keydown.myPlugin".[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A selector string to filter the descendants of the selected elements that trigger the event. If the selector is null or omitted, the event is always triggered when it reaches the selected element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Anything">[m[41m[m
[32m+[m[32m        ///     Data to be passed to the handler in event.data when an event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute when the event is triggered. The value false is also allowed as a shorthand for a function that simply does return false.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.on(types, selector, data, fn, 1);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.outerHeight = function (margin, value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the current computed height for the first element in the set of matched elements, including padding, border, and optionally margin. Returns an integer (without "px") representation of the value or null if called on an empty set of elements.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="margin" type="Boolean">[m[41m[m
[32m+[m[32m        ///     A Boolean indicating whether to include the element's margin in the calculation.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Number" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var chainable = arguments.length && (defaultExtra || typeof margin !== "boolean"),[m[41m[m
[32m+[m[32m            extra = defaultExtra || (margin === true || value === true ? "margin" : "border");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, function (elem, type, value) {[m[41m[m
[32m+[m[32m            var doc;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (jQuery.isWindow(elem)) {[m[41m[m
[32m+[m[32m                // As of 5/8/2012 this will yield incorrect results for Mobile Safari, but there[m[41m[m
[32m+[m[32m                // isn't a whole lot we can do. See pull request at this URL for discussion:[m[41m[m
[32m+[m[32m                // https://github.com/jquery/jquery/pull/764[m[41m[m
[32m+[m[32m                return elem.document.documentElement["client" + name];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Get document width or height[m[41m[m
[32m+[m[32m            if (elem.nodeType === 9) {[m[41m[m
[32m+[m[32m                doc = elem.documentElement;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Either scroll[Width/Height] or offset[Width/Height] or client[Width/Height], whichever is greatest[m[41m[m
[32m+[m[32m                // unfortunately, this causes bug #3838 in IE6/8 only, but there is currently no good, small way to fix it.[m[41m[m
[32m+[m[32m                return Math.max([m[41m[m
[32m+[m[32m                    elem.body["scroll" + name], doc["scroll" + name],[m[41m[m
[32m+[m[32m                    elem.body["offset" + name], doc["offset" + name],[m[41m[m
[32m+[m[32m                    doc["client" + name][m[41m[m
[32m+[m[32m                );[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return value === undefined ?[m[41m[m
[32m+[m[32m                // Get width or height on the element, requesting but not forcing parseFloat[m[41m[m
[32m+[m[32m                jQuery.css(elem, type, extra) :[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Set width or height on the element[m[41m[m
[32m+[m[32m                jQuery.style(elem, type, value, extra);[m[41m[m
[32m+[m[32m        }, type, chainable ? margin : undefined, chainable, null);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.outerWidth = function (margin, value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the current computed width for the first element in the set of matched elements, including padding and border.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="margin" type="Boolean">[m[41m[m
[32m+[m[32m        ///     A Boolean indicating whether to include the element's margin in the calculation.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Number" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var chainable = arguments.length && (defaultExtra || typeof margin !== "boolean"),[m[41m[m
[32m+[m[32m            extra = defaultExtra || (margin === true || value === true ? "margin" : "border");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, function (elem, type, value) {[m[41m[m
[32m+[m[32m            var doc;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (jQuery.isWindow(elem)) {[m[41m[m
[32m+[m[32m                // As of 5/8/2012 this will yield incorrect results for Mobile Safari, but there[m[41m[m
[32m+[m[32m                // isn't a whole lot we can do. See pull request at this URL for discussion:[m[41m[m
[32m+[m[32m                // https://github.com/jquery/jquery/pull/764[m[41m[m
[32m+[m[32m                return elem.document.documentElement["client" + name];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Get document width or height[m[41m[m
[32m+[m[32m            if (elem.nodeType === 9) {[m[41m[m
[32m+[m[32m                doc = elem.documentElement;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Either scroll[Width/Height] or offset[Width/Height] or client[Width/Height], whichever is greatest[m[41m[m
[32m+[m[32m                // unfortunately, this causes bug #3838 in IE6/8 only, but there is currently no good, small way to fix it.[m[41m[m
[32m+[m[32m                return Math.max([m[41m[m
[32m+[m[32m                    elem.body["scroll" + name], doc["scroll" + name],[m[41m[m
[32m+[m[32m                    elem.body["offset" + name], doc["offset" + name],[m[41m[m
[32m+[m[32m                    doc["client" + name][m[41m[m
[32m+[m[32m                );[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return value === undefined ?[m[41m[m
[32m+[m[32m                // Get width or height on the element, requesting but not forcing parseFloat[m[41m[m
[32m+[m[32m                jQuery.css(elem, type, extra) :[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Set width or height on the element[m[41m[m
[32m+[m[32m                jQuery.style(elem, type, value, extra);[m[41m[m
[32m+[m[32m        }, type, chainable ? margin : undefined, chainable, null);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.parent = function (until, selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the parent of each element in the current set of matched elements, optionally filtered by a selector.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="until" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret = jQuery.map(this, fn, until);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!runtil.test(name)) {[m[41m[m
[32m+[m[32m            selector = until;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (selector && typeof selector === "string") {[m[41m[m
[32m+[m[32m            ret = jQuery.filter(selector, ret);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ret = this.length > 1 && !guaranteedUnique[name] ? jQuery.unique(ret) : ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (this.length > 1 && rparentsprev.test(name)) {[m[41m[m
[32m+[m[32m            ret = ret.reverse();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.parents = function (until, selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the ancestors of each element in the current set of matched elements, optionally filtered by a selector.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="until" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret = jQuery.map(this, fn, until);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!runtil.test(name)) {[m[41m[m
[32m+[m[32m            selector = until;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (selector && typeof selector === "string") {[m[41m[m
[32m+[m[32m            ret = jQuery.filter(selector, ret);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ret = this.length > 1 && !guaranteedUnique[name] ? jQuery.unique(ret) : ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (this.length > 1 && rparentsprev.test(name)) {[m[41m[m
[32m+[m[32m            ret = ret.reverse();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.parentsUntil = function (until, selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the ancestors of each element in the current set of matched elements, up to but not including the element matched by the selector, DOM node, or jQuery object.[m[41m[m
[32m+[m[32m        ///     &#10;1 - parentsUntil(selector, filter)[m[41m [m
[32m+[m[32m        ///     &#10;2 - parentsUntil(element, filter)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="until" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to indicate where to stop matching ancestor elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret = jQuery.map(this, fn, until);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!runtil.test(name)) {[m[41m[m
[32m+[m[32m            selector = until;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (selector && typeof selector === "string") {[m[41m[m
[32m+[m[32m            ret = jQuery.filter(selector, ret);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ret = this.length > 1 && !guaranteedUnique[name] ? jQuery.unique(ret) : ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (this.length > 1 && rparentsprev.test(name)) {[m[41m[m
[32m+[m[32m            ret = ret.reverse();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.position = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the current coordinates of the first element in the set of matched elements, relative to the offset parent.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="Object" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!this[0]) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var offsetParent, offset,[m[41m[m
[32m+[m			[32mparentOffset = { top: 0, left: 0 },[m[41m[m
[32m+[m			[32melem = this[0];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // fixed elements are offset from window (parentOffset = {top:0, left: 0}, because it is it's only offset parent[m[41m[m
[32m+[m[32m        if (jQuery.css(elem, "position") === "fixed") {[m[41m[m
[32m+[m[32m            // we assume that getBoundingClientRect is available when computed position is fixed[m[41m[m
[32m+[m[32m            offset = elem.getBoundingClientRect();[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            // Get *real* offsetParent[m[41m[m
[32m+[m[32m            offsetParent = this.offsetParent();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Get correct offsets[m[41m[m
[32m+[m[32m            offset = this.offset();[m[41m[m
[32m+[m[32m            if (!jQuery.nodeName(offsetParent[0], "html")) {[m[41m[m
[32m+[m[32m                parentOffset = offsetParent.offset();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Add offsetParent borders[m[41m[m
[32m+[m[32m            parentOffset.top += jQuery.css(offsetParent[0], "borderTopWidth", true);[m[41m[m
[32m+[m[32m            parentOffset.left += jQuery.css(offsetParent[0], "borderLeftWidth", true);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Subtract parent offsets and element margins[m[41m[m
[32m+[m[32m        // note: when an element has margin: auto the offsetLeft and marginLeft[m[41m[m
[32m+[m[32m        // are the same in Safari causing offset.left to incorrectly be 0[m[41m[m
[32m+[m[32m        return {[m[41m[m
[32m+[m[32m            top: offset.top - parentOffset.top - jQuery.css(elem, "marginTop", true),[m[41m[m
[32m+[m[32m            left: offset.left - parentOffset.left - jQuery.css(elem, "marginLeft", true)[m[41m[m
[32m+[m[32m        };[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.prepend = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Insert content, specified by the parameter, to the beginning of each element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - prepend(content, content)[m[41m [m
[32m+[m[32m        ///     &#10;2 - prepend(function(index, html))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="" type="jQuery">[m[41m[m
[32m+[m[32m        ///     DOM element, array of elements, HTML string, or jQuery object to insert at the beginning of each element in the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="" type="jQuery">[m[41m[m
[32m+[m[32m        ///     One or more additional DOM elements, arrays of elements, HTML strings, or jQuery objects to insert at the beginning of each element in the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.domManip(arguments, true, function (elem) {[m[41m[m
[32m+[m[32m            if (this.nodeType === 1 || this.nodeType === 11 || this.nodeType === 9) {[m[41m[m
[32m+[m[32m                this.insertBefore(elem, this.firstChild);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.prependTo = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Insert every element in the set of matched elements to the beginning of the target.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="jQuery">[m[41m[m
[32m+[m[32m        ///     A selector, element, HTML string, or jQuery object; the matched set of elements will be inserted at the beginning of the element(s) specified by this parameter.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var elems,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mret = [],[m[41m[m
[32m+[m			[32minsert = jQuery(selector),[m[41m[m
[32m+[m			[32mlast = insert.length - 1;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; i <= last; i++) {[m[41m[m
[32m+[m[32m            elems = i === last ? this : this.clone(true);[m[41m[m
[32m+[m[32m            jQuery(insert[i])[original](elems);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Modern browsers can apply jQuery collections as arrays, but oldIE needs a .get()[m[41m[m
[32m+[m[32m            core_push.apply(ret, elems.get());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.prev = function (until, selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the immediately preceding sibling of each element in the set of matched elements, optionally filtered by a selector.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="until" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret = jQuery.map(this, fn, until);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!runtil.test(name)) {[m[41m[m
[32m+[m[32m            selector = until;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (selector && typeof selector === "string") {[m[41m[m
[32m+[m[32m            ret = jQuery.filter(se