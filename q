[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="undefined" />[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.now = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Return a number representing the current time.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="Number" />[m[41m[m
