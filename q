[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.fadeTo = function (speed, to, easing, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Adjust the opacity of the matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - fadeTo(duration, opacity, complete)[m[41m [m
[32m+[m[32m        ///     &#10;2 - fadeTo(duration, opacity, easing, complete)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="speed" type="Number">[m[41m[m
[32m+[m[32m        ///     A string or number determining how long the animation will run.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="to" type="Number">[m[41m[m
[32m+[m[32m        ///     A number between 0 and 1 denoting the target opacity.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="easing" type="String">[m[41m[m
[32m+[m[32m        ///     A string indicating which easing function to use for the transition.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to call once the animation is complete.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // show any hidden elements after setting opacity to 0[m[41m[m
[32m+[m[32m        return this.filter(isHidden).css("opacity", 0).show()[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// animate to the value specified[m[41m[m
[32m+[m			[32m.end().animate({ opacity: to }, speed, easing, callback);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.fadeToggle = function (speed, easing, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Display or hide the matched elements by animating their opacity.[m[41m[m
[32m+[m[32m        ///     &#10;1 - fadeToggle(duration, easing, complete)[m[41m [m
