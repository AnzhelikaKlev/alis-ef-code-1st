[32m+[m[32mï»¿/// <reference path="jquery-1.7.1.js" />[m[41m[m
[32m+[m[32m/// <reference path="jquery.validate.js" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/*![m[41m[m
[32m+[m[32m** Unobtrusive validation support library for jQuery and jQuery Validate[m[41m[m
