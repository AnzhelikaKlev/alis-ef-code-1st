[32m+[m[41m[m
[32m+[m[32m(function($) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$.extend($.fn, {[m[41m[m
[32m+[m	[32m// http://docs.jquery.com/Plugins/Validation/validate[m[41m[m
[32m+[m	[32mvalidate: function( options ) {[m[41m[m
[32m+[m		[32m/// <summary>[m[41m[m
[32m+[m		[32m/// Validates the selected form. This method sets up event handlers for submit, focus,[m[41m[m
[32m+[m		[32m/// keyup, blur and click to trigger validation of the entire form or individual[m[41m[m
[32m+[m		[32m/// elements. Each one can be disabled, see the onxxx options (onsubmit, onfocusout,[m[41m[m
[32m+[m		[32m/// onkeyup, onclick). focusInvalid focuses elements when submitting a invalid form.[m[41m[m
[32m+[m		[32m/// </summary>[m[41m[m
[32m+[m		[32m/// <param name="options" type="Object">[m[41m[m
[32m+[m		[32m/// A set of key/value pairs that configure the validate. All options are optional.[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m// if nothing is selected, return nothing; can't chain anyway[m[41m[m
[32m+[m		[32mif (!this.length) {[m[41m[m
[32m+[m			[32moptions && options.debug && window.console && console.warn( "nothing selected, can't validate, returning nothing" );[m[41m[m
[32m+[m			[32mreturn;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m// check if a validator for this form was already created[m[41m[m
[32m+[m		[32mvar validator = $.data(this[0], 'validator');[m[41m[m
[32m+[m		[32mif ( validator ) {[m[41m[m
[32m+[m			[32mreturn validator;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mvalidator = new $.validator( options, this[0] );[m[41m[m
[32m+[m		[32m$.data(this[0], 'validator', validator);[m[41m [m
[32m+[m[41m		[m
[32m+[m		[32mif ( validator.settings.onsubmit ) {[m[41m[m
[32m+[m[41m		[m
[32m+[m			[32m// allow suppresing validation by adding a cancel class to the submit button[m[41m[m
[32m+[m			[32mthis.find("input, button").filter(".cancel").click(function() {[m[41m[m
[32m+[m				[32mvalidator.cancelSubmit = true;[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32m// when a submitHandler is used, capture the submitting button[m[41m[m
[32m+[m			[32mif (validator.settings.submitHandler) {[m[41m[m
[32m+[m				[32mthis.find("input, button").filter(":submit").click(function() {[m[41m[m
[32m+[m					[32mvalidator.submitButton = this;[m[41m[m
[32m+[m				[32m});[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m			[32m// validate the form on submit[m[41m[m
[32m+[m			[32mthis.submit( function( event ) {[m[41m[m
[32m+[m				[32mif ( validator.settings.debug )[m[41m[m
[32m+[m					[32m// prevent form submit to be able to see console output[m[41m[m
[32m+[m					[32mevent.preventDefault();[m[41m[m
[32m+[m[41m					[m
[32m+[m				[32mfunction handle() {[m[41m[m
[32m+[m					[32mif ( validator.settings.submitHandler ) {[m[41m[m
[32m+[m						[32mif (validator.submitButton) {[m[41m[m
[32m+[m							[32m// insert a hidden input as a replacement for the missing submit button[m[41m[m
[32m+[m							[32mvar hidden = $("<input type='hidden'/>").attr("name", validator.submitButton.name).val(validator.submitButton.value).appendTo(validator.currentForm);[m[41m[m
[32m+[m						[32m}[m[41m[m
[32m+[m						[32mvalidator.settings.submitHandler.call( validator, validator.currentForm );[m[41m[m
[32m+[m						[32mif (validator.submitButton) {[m[41m[m
[32m+[m							[32m// and clean up afterwards; thanks to no-block-scope, hidden can be referenced[m[41m[m
[32m+[m							[32mhidden.remove();[m[41m[m
[32m+[m						[32m}[m[41m[m
[32m+[m						[32mreturn false;[m[41m[m
[32m+[m					[32m}[m[41m[m
[32m+[m					[32mreturn true;[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m[41m					[m
[32m+[m				[32m// prevent submit for invalid forms or custom submit handlers[m[41m[m
[32m+[m				[32mif ( validator.cancelSubmit ) {[m[41m[m
[32m+[m					[32mvalidator.cancelSubmit = false;[m[41m[m
[32m+[m					[32mreturn handle();[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m				[32mif ( validator.form() ) {[m[41m[m
[32m+[m					[32mif ( validator.pendingRequest ) {[m[41m[m
[32m+[m						[32mvalidator.formSubmitted = true;[m[41m[m
[32m+[m						[32mreturn false;[m[41m[m
[32m+[m					[32m}[m[41m[m
[32m+[m					[32mreturn handle();[m[41m[m
[32m+[m				[32m} else {[m[41m[m
[32m+[m					[32mvalidator.focusInvalid();[m[41m[m
[32m+[m					[32mreturn false;[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mreturn validator;[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m	[32m// http://docs.jquery.com/Plugins/Validation/valid[m[41m[m
[32m+[m	[32mvalid: function() {[m[41m[m
[32m+[m		[32m/// <summary>[m[41m[m
[32m+[m		[32m/// Checks if the selected form is valid or if all selected elements are valid.[m[41m[m
[32m+[m		[32m/// validate() needs to be called on the form before checking it using this method.[m[41m[m
[32m+[m		[32m/// </summary>[m[41m[m
[32m+[m		[32m/// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if ( $(this[0]).is('form')) {[m[41m[m
[32m+[m[32m            return this.validate().form();[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            var valid = true;[m[41m[m
[32m+[m[32m            var validator = $(this[0].form).validate();[m[41m[m
[32m+[m[32m            this.each(function() {[m[41m[m
[32m+[m				[32mvalid &= validator.element(this);[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[32m            return valid;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m	[32m// attributes: space seperated list of attributes to retrieve and remove[m[41m[m
[32m+[m	[32mremoveAttrs: function(attributes) {[m[41m[m
[32m+[m		[32m/// <summary>[m[41m[m
[32m+[m		[32m/// Remove the specified attributes from the first matched element and return them.[m[41m[m
[32m+[m		[32m/// </summary>[m[41m[m
[32m+[m		[32m/// <param name="attributes" type="String">[m[41m[m
[32m+[m		[32m/// A space-seperated list of attribute names to remove.[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mvar result = {},[m[41m[m
[32m+[m			[32m$element = this;[m[41m[m
[32m+[m		[32m$.each(attributes.split(/\s/), function(index, value) {[m[41m[m
[32m+[m			[32mresult[value] = $element.attr(value);[m[41m[m
[32m+[m			[32m$element.removeAttr(value);[m[41m[m
[32m+[m		[32m});[m[41m[m
[32m+[m		[32mreturn result;[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m	[32m// http://docs.jquery.com/Plugins/Validation/rules[m[41m[m
[32m+[m	[32mrules: function(command, argument) {[m[41m[m
[32m+[m		[32m/// <summary>[m[41m[m
[32m+[m		[32m/// Return the validations rules for the first selected element.[m[41m[m
[32m+[m		[32m/// </summary>[m[41m[m
[32m+[m		[32m/// <param name="command" type="String">[m[41m[m
[32m+[m		[32m/// Can be either "add" or "remove".[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m		[32m/// <param name="argument" type="">[m[41m[m
[32m+[m		[32m/// A list of rules to add or remove.[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mvar element = this[0];[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mif (command) {[m[41m[m
[32m+[m			[32mvar settings = $.data(element.form, 'validator').settings;[m[41m[m
[32m+[m			[32mvar staticRules = settings.rules;[m[41m[m
[32m+[m			[32mvar existingRules = $.validator.staticRules(element);[m[41m[m
[32m+[m			[32mswitch(command) {[m[41m[m
[32m+[m			[32mcase "add":[m[41m[m
[32m+[m				[32m$.extend(existingRules, $.validator.normalizeRule(argument));[m[41m[m
[32m+[m				[32mstaticRules[element.name] = existingRules;[m[41m[m
[32m+[m				[32mif (argument.messages)[m[41m[m
[32m+[m					[32msettings.messages[element.name] = $.extend( settings.messages[element.name], argument.messages );[m[41m[m
[32m+[m				[32mbreak;[m[41m[m
[32m+[m			[32mcase "remove":[m[41m[m
[32m+[m				[32mif (!argument) {[m[41m[m
[32m+[m					[32mdelete staticRules[element.name];[m[41m[m
[32m+[m					[32mreturn existingRules;[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m				[32mvar filtered = {};[m[41m[m
[32m+[m				[32m$.each(argument.split(/\s/), function(index, method) {[m[41m[m
[32m+[m					[32mfiltered[method] = existingRules[method];[m[41m[m
[32m+[m					[32mdelete existingRules[method];[m[41m[m
[32m+[m				[32m});[m[41m[m
[32m+[m				[32mreturn filtered;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mvar data = $.validator.normalizeRules([m[41m[m
[32m+[m		[32m$.extend([m[41m[m
[32m+[m			[32m{},[m[41m[m
[32m+[m			[32m$.validator.metadataRules(element),[m[41m[m
[32m+[m			[32m$.validator.classRules(element),[m[41m[m
[32m+[m			[32m$.validator.attributeRules(element),[m[41m[m
[32m+[m			[32m$.validator.staticRules(element)[m[41m[m
[32m+[m		[32m), element);[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// make sure required is at front[m[41m[m
[32m+[m		[32mif (data.required) {[m[41m[m
[32m+[m			[32mvar param = data.required;[m[41m[m
[32m+[m			[32mdelete data.required;[m[41m[m
[32m+[m			[32mdata = $.extend({required: param}, data);[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mreturn data;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[32m});[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Custom selectors[m[41m[m
[32m+[m[32m$.extend($.expr[":"], {[m[41m[m
[32m+[m	[32m// http://docs.jquery.com/Plugins/Validation/blank[m[41m[m
[32m+[m	[32mblank: function(a) {return !$.trim("" + a.value);},[m[41m[m
[32m+[m	[32m// http://docs.jquery.com/Plugins/Validation/filled[m[41m[m
[32m+[m	[32mfilled: function(a) {return !!$.trim("" + a.value);},[m[41m[m
[32m+[m	[32m// http://docs.jquery.com/Plugins/Validation/unchecked[m[41m[m
[32m+[m	[32munchecked: function(a) {return !a.checked;}[m[41m[m
[32m+[m[32m});[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// constructor for validator[m[41m[m
[32m+[m[32m$.validator = function( options, form ) {[m[41m[m
[32m+[m	[32mthis.settings = $.extend( true, {}, $.validator.defaults, options );[m[41m[m
[32m+[m	[32mthis.currentForm = form;[m[41m[m
[32m+[m	[32mthis.init();[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$.validator.format = function(source, params) {[m[41m[m
[32m+[m	[32m/// <summary>[m[41m[m
[32m+[m	[32m/// Replaces {n} placeholders with arguments.[m[41m[m
[32m+[m	[32m/// One or more arguments can be passed, in addition to the string template itself, to insert[m[41m[m
[32m+[m	[32m/// into the string.[m[41m[m
[32m+[m	[32m/// </summary>[m[41m[m
[32m+[m	[32m/// <param name="source" type="String">[m[41m[m
[32m+[m	[32m/// The string to format.[m[41m[m
[32m+[m	[32m/// </param>[m[41m[m
[32m+[m	[32m/// <param name="params" type="String">[m[41m[m
[32m+[m	[32m/// The first argument to insert, or an array of Strings to insert[m[41m[m
[32m+[m	[32m/// </param>[m[41m[m
[32m+[m	[32m/// <returns type="String" />[m[41m[m
[32m+[m[41m[m
[32m+[m	[32mif ( arguments.length == 1 )[m[41m [m
[32m+[m		[32mreturn function() {[m[41m[m
[32m+[m			[32mvar args = $.makeArray(arguments);[m[41m[m
[32m+[m			[32margs.unshift(source);[m[41m[m
[32m+[m			[32mreturn $.validator.format.apply( this, args );[m[41m[m
[32m+[m		[32m};[m[41m[m
[32m+[m	[32mif ( arguments.length > 2 && params.constructor != Array  ) {[m[41m[m
[32m+[m		[32mparams = $.makeArray(arguments).slice(1);[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mif ( params.constructor != Array ) {[m[41m[m
[32m+[m		[32mparams = [ params ];[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32m$.each(params, function(i, n) {[m[41m[m
[32m+[m		[32msource = source.replace(new RegExp("\\{" + i + "\\}", "g"), n);[m[41m[m
[32m+[m	[32m});[m[41m[m
[32m+[m	[32mreturn source;[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$.extend($.validator, {[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mdefaults: {[m[41m[m
[32m+[m		[32mmessages: {},[m[41m[m
[32m+[m		[32mgroups: {},[m[41m[m
[32m+[m		[32mrules: {},[m[41m[m
[32m+[m		[32merrorClass: "error",[m[41m[m
[32m+[m		[32mvalidClass: "valid",[m[41m[m
[32m+[m		[32merrorElement: "label",[m[41m[m
[32m+[m		[32mfocusInvalid: true,[m[41m[m
[32m+[m		[32merrorContainer: $( [] ),[m[41m[m
[32m+[m		[32merrorLabelContainer: $( [] ),[m[41m[m
[32m+[m		[32monsubmit: true,[m[41m[m
[32m+[m		[32mignore: [],[m[41m[m
[32m+[m		[32mignoreTitle: false,[m[41m[m
[32m+[m		[32monfocusin: function(element) {[m[41m[m
[32m+[m			[32mthis.lastActive = element;[m[41m[m
[32m+[m[41m				[m
[32m+[m			[32m// hide error label and remove error class on focus if enabled[m[41m[m
[32m+[m			[32mif ( this.settings.focusCleanup && !this.blockFocusCleanup ) {[m[41m[m
[32m+[m				[32mthis.settings.unhighlight && this.settings.unhighlight.call( this, element, this.settings.errorClass, this.settings.validClass );[m[41m[m
[32m+[m				[32mthis.addWrapper(this.errorsFor(element)).hide();[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m		[32monfocusout: function(element) {[m[41m[m
[32m+[m			[32mif ( !this.checkable(element) && (element.name in this.submitted || !this.optional(element)) ) {[m[41m[m
[32m+[m				[32mthis.element(element);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m		[32monkeyup: function(element) {[m[41m[m
[32m+[m			[32mif ( element.name in this.submitted || element == this.lastElement ) {[m[41m[m
[32m+[m				[32mthis.element(element);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m		[32monclick: function(element) {[m[41m[m
[32m+[m			[32m// click on selects, radiobuttons and checkboxes[m[41m[m
[32m+[m			[32mif ( element.name in this.submitted )[m[41m[m
[32m+[m				[32mthis.element(element);[m[41m[m
[32m+[m			[32m// or option elements, check parent select in that case[m[41m[m
[32m+[m			[32melse if (element.parentNode.name in this.submitted)[m[41m[m
[32m+[m				[32mthis.element(element.parentNode);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m		[32mhighlight: function( element, errorClass, validClass ) {[m[41m[m
[32m+[m			[32m$(element).addClass(errorClass).removeClass(validClass);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m		[32munhighlight: function( element, errorClass, validClass ) {[m[41m[m
[32m+[m			[32m$(element).removeClass(errorClass).addClass(validClass);[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m[m
[32m+[m	[32m// http://docs.jquery.com/Plugins/Validation/Validator/setDefaults[m[41m[m
[32m+[m	[32msetDefaults: function(settings) {[m[41m[m
[32m+[m		[32m/// <summary>[m[41m[m
[32m+[m		[32m/// Modify default settings for validation.[m[41m[m
[32m+[m		[32m/// Accepts everything that Plugins/Validation/validate accepts.[m[41m[m
[32m+[m		[32m/// </summary>[m[41m[m
[32m+[m		[32m/// <param name="settings" type="Options">[m[41m[m
[32m+[m		[32m/// Options to set as default.[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m$.extend( $.validator.defaults, settings );[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m[m
[32m+[m	[32mmessages: {[m[41m[m
[32m+[m		[32mrequired: "This field is required.",[m[41m[m
[32m+[m		[32mremote: "Please fix this field.",[m[41m[m
[32m+[m		[32memail: "Please enter a valid email address.",[m[41m[m
[32m+[m		[32murl: "Please enter a valid URL.",[m[41m[m
[32m+[m		[32mdate: "Please enter a valid date.",[m[41m[m
[32m+[m		[32mdateISO: "Please enter a valid date (ISO).",[m[41m[m
[32m+[m		[32mnumber: "Please enter a valid number.",[m[41m[m
[32m+[m		[32mdigits: "Please enter only digits.",[m[41m[m
[32m+[m		[32mcreditcard: "Please enter a valid credit card number.",[m[41m[m
[32m+[m		[32mequalTo: "Please enter the same value again.",[m[41m[m
[32m+[m		[32maccept: "Please enter a value with a valid extension.",[m[41m[m
[32m+[m		[32mmaxlength: $.validator.format("Please enter no more than {0} characters."),[m[41m[m
[32m+[m		[32mminlength: $.validator.format("Please enter at least {0} characters."),[m[41m[m
[32m+[m		[32mrangelength: $.validator.format("Please enter a value between {0} and {1} characters long."),[m[41m[m
[32m+[m		[32mrange: $.validator.format("Please enter a value between {0} and {1}."),[m[41m[m
[32m+[m		[32mmax: $.validator.format("Please enter a value less than or equal to {0}."),[m[41m[m
[32m+[m		[32mmin: $.validator.format("Please enter a value greater than or equal to {0}.")[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mautoCreateRanges: false,[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mprototype: {[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32minit: function() {[m[41m[m
[32m+[m			[32mthis.labelContainer = $(this.settings.errorLabelContainer);[m[41m[m
[32m+[m			[32mthis.errorContext = this.labelContainer.length && this.labelContainer || $(this.currentForm);[m[41m[m
[32m+[m			[32mthis.containers = $(this.settings.errorContainer).add( this.settings.errorLabelContainer );[m[41m[m
[32m+[m			[32mthis.submitted = {};[m[41m[m
[32m+[m			[32mthis.valueCache = {};[m[41m[m
[32m+[m			[32mthis.pendingRequest = 0;[m[41m[m
[32m+[m			[32mthis.pending = {};[m[41m[m
[32m+[m			[32mthis.invalid = {};[m[41m[m
[32m+[m			[32mthis.reset();[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mvar groups = (this.groups = {});[m[41m[m
[32m+[m			[32m$.each(this.settings.groups, function(key, value) {[m[41m[m
[32m+[m				[32m$.each(value.split(/\s/), function(index, name) {[m[41m[m
[32m+[m					[32mgroups[name] = key;[m[41m[m
[32m+[m				[32m});[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m			[32mvar rules = this.settings.rules;[m[41m[m
[32m+[m			[32m$.each(rules, function(key, value) {[m[41m[m
[32m+[m				[32mrules[key] = $.validator.normalizeRule(value);[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mfunction delegate(event) {[m[41m[m
[32m+[m				[32mvar validator = $.data(this[0].form, "validator"),[m[41m[m
[32m+[m					[32meventType = "on" + event.type.replace(/^validate/, "");[m[41m[m
[32m+[m				[32mvalidator.settings[eventType] && validator.settings[eventType].call(validator, this[0] );[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32m$(this.currentForm)[m[41m[m
[32m+[m				[32m.validateDelegate(":text, :password, :file, select, textarea", "focusin focusout keyup", delegate)[m[41m[m
[32m+[m				[32m.validateDelegate(":radio, :checkbox, select, option", "click", delegate);[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mif (this.settings.invalidHandler)[m[41m[m
[32m+[m				[32m$(this.currentForm).bind("invalid-form.validate", this.settings.invalidHandler);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Validator/form[m[41m[m
[32m+[m		[32mform: function() {[m[41m[m
[32m+[m			[32m/// <summary>[m[41m[m
[32m+[m			[32m/// Validates the form, returns true if it is valid, false otherwise.[m[41m[m
[32m+[m			[32m/// This behaves as a normal submit event, but returns the result.[m[41m[m
[32m+[m			[32m/// </summary>[m[41m[m
[32m+[m			[32m/// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mthis.checkForm();[m[41m[m
[32m+[m			[32m$.extend(this.submitted, this.errorMap);[m[41m[m
[32m+[m			[32mthis.invalid = $.extend({}, this.errorMap);[m[41m[m
[32m+[m			[32mif (!this.valid())[m[41m[m
[32m+[m				[32m$(this.currentForm).triggerHandler("invalid-form", [this]);[m[41m[m
[32m+[m			[32mthis.showErrors();[m[41m[m
[32m+[m			[32mreturn this.valid();[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mcheckForm: function() {[m[41m[m
[32m+[m			[32mthis.prepareForm();[m[41m[m
[32m+[m			[32mfor ( var i = 0, elements = (this.currentElements = this.elements()); elements[i]; i++ ) {[m[41m[m
[32m+[m				[32mthis.check( elements[i] );[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mreturn this.valid();[m[41m [m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Validator/element[m[41m[m
[32m+[m		[32melement: function( element ) {[m[41m[m
[32m+[m			[32m/// <summary>[m[41m[m
[32m+[m			[32m/// Validates a single element, returns true if it is valid, false otherwise.[m[41m[m
[32m+[m			[32m/// This behaves as validation on blur or keyup, but returns the result.[m[41m[m
[32m+[m			[32m/// </summary>[m[41m[m
[32m+[m			[32m/// <param name="element" type="Selector">[m[41m[m
[32m+[m			[32m/// An element to validate, must be inside the validated form.[m[41m[m
[32m+[m			[32m/// </param>[m[41m[m
[32m+[m			[32m/// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m			[32melement = this.clean( element );[m[41m[m
[32m+[m			[32mthis.lastElement = element;[m[41m[m
[32m+[m			[32mthis.prepareElement( element );[m[41m[m
[32m+[m			[32mthis.currentElements = $(element);[m[41m[m
[32m+[m			[32mvar result = this.check( element );[m[41m[m
[32m+[m			[32mif ( result ) {[m[41m[m
[32m+[m				[32mdelete this.invalid[element.name];[m[41m[m
[32m+[m			[32m} else {[m[41m[m
[32m+[m				[32mthis.invalid[element.name] = true;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif ( !this.numberOfInvalids() ) {[m[41m[m
[32m+[m				[32m// Hide error containers on last error[m[41m[m
[32m+[m				[32mthis.toHide = this.toHide.add( this.containers );[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mthis.showErrors();[m[41m[m
[32m+[m			[32mreturn result;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Validator/showErrors[m[41m[m
[32m+[m		[32mshowErrors: function(errors) {[m[41m[m
[32m+[m			[32m/// <summary>[m[41m[m
[32m+[m			[32m/// Show the specified messages.[m[41m[m
[32m+[m			[32m/// Keys have to refer to the names of elements, values are displayed for those elements, using the configured error placement.[m[41m[m
[32m+[m			[32m/// </summary>[m[41m[m
[32m+[m			[32m/// <param name="errors" type="Object">[m[41m[m
[32m+[m			[32m/// One or more key/value pairs of input names and messages.[m[41m[m
[32m+[m			[32m/// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mif(errors) {[m[41m[m
[32m+[m				[32m// add items to error list and map[m[41m[m
[32m+[m				[32m$.extend( this.errorMap, errors );[m[41m[m
[32m+[m				[32mthis.errorList = [];[m[41m[m
[32m+[m				[32mfor ( var name in errors ) {[m[41m[m
[32m+[m					[32mthis.errorList.push({[m[41m[m
[32m+[m						[32mmessage: errors[name],[m[41m[m
[32m+[m						[32melement: this.findByName(name)[0][m[41m[m
[32m+[m					[32m});[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m				[32m// remove items from success list[m[41m[m
[32m+[m				[32mthis.successList = $.grep( this.successList, function(element) {[m[41m[m
[32m+[m					[32mreturn !(element.name in errors);[m[41m[m
[32m+[m				[32m});[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mthis.settings.showErrors[m[41m[m
[32m+[m				[32m? this.settings.showErrors.call( this, this.errorMap, this.errorList )[m[41m[m
[32m+[m				[32m: this.defaultShowErrors();[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Validator/resetForm[m[41m[m
[32m+[m		[32mresetForm: function() {[m[41m[m
[32m+[m			[32m/// <summary>[m[41m[m
[32m+[m			[32m/// Resets the controlled form.[m[41m[m
[32m+[m			[32m/// Resets input fields to their original value (requires form plugin), removes classes[m[41m[m
[32m+[m			[32m/// indicating invalid elements and hides error messages.[m[41m[m
[32m+[m			[32m/// </summary>[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mif ( $.fn.resetForm )[m[41m[m
[32m+[m				[32m$( this.currentForm ).resetForm();[m[41m[m
[32m+[m			[32mthis.submitted = {};[m[41m[m
[32m+[m			[32mthis.prepareForm();[m[41m[m
[32m+[m			[32mthis.hideErrors();[m[41m[m
[32m+[m			[32mthis.elements().removeClass( this.settings.errorClass );[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mnumberOfInvalids: function() {[m[41m[m
[32m+[m			[32m/// <summary>[m[41m[m
[32m+[m			[32m/// Returns the number of invalid fields.[m[41m[m
[32m+[m			[32m/// This depends on the internal validator state. It covers all fields only after[m[41m[m
[32m+[m			[32m/// validating the complete form (on submit or via $("form").valid()). After validating[m[41m[m
[32m+[m			[32m/// a single element, only that element is counted. Most useful in combination with the[m[41m[m
[32m+[m			[32m/// invalidHandler-option.[m[41m[m
[32m+[m			[32m/// </summary>[m[41m[m
[32m+[m			[32m/// <returns type="Number" />[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mreturn this.objectLength(this.invalid);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mobjectLength: function( obj ) {[m[41m[m
[32m+[m			[32mvar count = 0;[m[41m[m
[32m+[m			[32mfor ( var i in obj )[m[41m[m
[32m+[m				[32mcount++;[m[41m[m
[32m+[m			[32mreturn count;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mhideErrors: function() {[m[41m[m
[32m+[m			[32mthis.addWrapper( this.toHide ).hide();[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mvalid: function() {[m[41m[m
[32m+[m			[32mreturn this.size() == 0;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32msize: function() {[m[41m[m
[32m+[m			[32mreturn this.errorList.length;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mfocusInvalid: function() {[m[41m[m
[32m+[m			[32mif( this.settings.focusInvalid ) {[m[41m[m
[32m+[m				[32mtry {[m[41m[m
[32m+[m					[32m$(this.findLastActive() || this.errorList.length && this.errorList[0].element || [])[m[41m[m
[32m+[m					[32m.filter(":visible")[m[41m[m
[32m+[m					[32m.focus()[m[41m[m
[32m+[m					[32m// manually trigger focusin event; without it, focusin handler isn't called, findLastActive won't have anything to find[m[41m[m
[32m+[m					[32m.trigger("focusin");[m[41m[m
[32m+[m				[32m} catch(e) {[m[41m[m
[32m+[m					[32m// ignore IE throwing errors when focusing hidden elements[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mfindLastActive: function() {[m[41m[m
[32m+[m			[32mvar lastActive = this.lastActive;[m[41m[m
[32m+[m			[32mreturn lastActive && $.grep(this.errorList, function(n) {[m[41m[m
[32m+[m				[32mreturn n.element.name == lastActive.name;[m[41m[m
[32m+[m			[32m}).length == 1 && lastActive;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32melements: function() {[m[41m[m
[32m+[m			[32mvar validator = this,[m[41m[m
[32m+[m				[32mrulesCache = {};[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32m// select all valid inputs inside the form (no submit or reset buttons)[m[41m[m
[32m+[m			[32m// workaround $Query([]).add until http://dev.jquery.com/ticket/2114 is solved[m[41m[m
[32m+[m			[32mreturn $([]).add(this.currentForm.elements)[m[41m[m
[32m+[m			[32m.filter(":input")[m[41m[m
[32m+[m			[32m.not(":submit, :reset, :image, [disabled]")[m[41m[m
[32m+[m			[32m.not( this.settings.ignore )[m[41m[m
[32m+[m			[32m.filter(function() {[m[41m[m
[32m+[m				[32m!this.name && validator.settings.debug && window.console && console.error( "%o has no name assigned", this);[m[41m[m
[32m+[m[41m			[m
[32m+[m				[32m// select only the first element for each name, and only those with rules specified[m[41m[m
[32m+[m				[32mif ( this.name in rulesCache || !validator.objectLength($(this).rules()) )[m[41m[m
[32m+[m					[32mreturn false;[m[41m[m
[32m+[m[41m				[m
[32m+[m				[32mrulesCache[this.name] = true;[m[41m[m
[32m+[m				[32mreturn true;[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mclean: function( selector ) {[m[41m[m
[32m+[m			[32mreturn $( selector )[0];[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32merrors: function() {[m[41m[m
[32m+[m			[32mreturn $( this.settings.errorElement + "." + this.settings.errorClass, this.errorContext );[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mreset: function() {[m[41m[m
[32m+[m			[32mthis.successList = [];[m[41m[m
[32m+[m			[32mthis.errorList = [];[m[41m[m
[32m+[m			[32mthis.errorMap = {};[m[41m[m
[32m+[m			[32mthis.toShow = $([]);[m[41m[m
[32m+[m			[32mthis.toHide = $([]);[m[41m[m
[32m+[m			[32mthis.currentElements = $([]);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mprepareForm: function() {[m[41m[m
[32m+[m			[32mthis.reset();[m[41m[m
[32m+[m			[32mthis.toHide = this.errors().add( this.containers );[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mprepareElement: function( element ) {[m[41m[m
[32m+[m			[32mthis.reset();[m[41m[m
[32m+[m			[32mthis.toHide = this.errorsFor(element);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m		[32mcheck: function( element ) {[m[41m[m
[32m+[m			[32melement = this.clean( element );[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32m// if radio/checkbox, validate first element in group instead[m[41m[m
[32m+[m			[32mif (this.checkable(element)) {[m[41m[m
[32m+[m			[32m    element = this.findByName(element.name).not(this.settings.ignore)[0];[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mvar rules = $(element).rules();[m[41m[m
[32m+[m			[32mvar dependencyMismatch = false;[m[41m[m
[32m+[m			[32mfor (var method in rules) {[m[41m[m
[32m+[m				[32mvar rule = { method: method, parameters: rules[method] };[m[41m[m
[32m+[m				[32mtry {[m[41m[m
[32m+[m					[32mvar result = $.validator.methods[method].call( this, element.value.replace(/\r/g, ""), element, rule.parameters );[m[41m[m
[32m+[m[41m					[m
[32m+[m					[32m// if a method indicates that the field is optional and therefore valid,[m[41m[m
[32m+[m					[32m// don't mark it as valid when there are no other rules[m[41m[m
[32m+[m					[32mif ( result == "dependency-mismatch" ) {[m[41m[m
[32m+[m						[32mdependencyMismatch = true;[m[41m[m
[32m+[m						[32mcontinue;[m[41m[m
[32m+[m					[32m}[m[41m[m
[32m+[m					[32mdependencyMismatch = false;[m[41m[m
[32m+[m[41m					[m
[32m+[m					[32mif ( result == "pending" ) {[m[41m[m
[32m+[m						[32mthis.toHide = this.toHide.not( this.errorsFor(element) );[m[41m[m
[32m+[m						[32mreturn;[m[41m[m
[32m+[m					[32m}[m[41m[m
[32m+[m[41m					[m
[32m+[m					[32mif( !result ) {[m[41m[m
[32m+[m						[32mthis.formatAndAdd( element, rule );[m[41m[m
[32m+[m						[32mreturn false;[m[41m[m
[32m+[m					[32m}[m[41m[m
[32m+[m				[32m} catch(e) {[m[41m[m
[32m+[m					[32mthis.settings.debug && window.console && console.log("exception occured when checking element " + element.id[m[41m[m
[32m+[m						[32m + ", check the '" + rule.method + "' method", e);[m[41m[m
[32m+[m					[32mthrow e;[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif (dependencyMismatch)[m[41m[m
[32m+[m				[32mreturn;[m[41m[m
[32m+[m			[32mif ( this.objectLength(rules) )[m[41m[m
[32m+[m				[32mthis.successList.push(element);[m[41m[m
[32m+[m			[32mreturn true;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// return the custom message for the given element and validation method[m[41m[m
[32m+[m		[32m// specified in the element's "messages" metadata[m[41m[m
[32m+[m		[32mcustomMetaMessage: function(element, method) {[m[41m[m
[32m+[m			[32mif (!$.metadata)[m[41m[m
[32m+[m				[32mreturn;[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mvar meta = this.settings.meta[m[41m[m
[32m+[m				[32m? $(element).metadata()[this.settings.meta][m[41m[m
[32m+[m				[32m: $(element).metadata();[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mreturn meta && meta.messages && meta.messages[method];[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// return the custom message for the given element name and validation method[m[41m[m
[32m+[m		[32mcustomMessage: function( name, method ) {[m[41m[m
[32m+[m			[32mvar m = this.settings.messages[name];[m[41m[m
[32m+[m			[32mreturn m && (m.constructor == String[m[41m[m
[32m+[m				[32m? m[m[41m[m
[32m+[m				[32m: m[method]);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// return the first defined argument, allowing empty strings[m[41m[m
[32m+[m		[32mfindDefined: function() {[m[41m[m
[32m+[m			[32mfor(var i = 0; i < arguments.length; i++) {[m[41m[m
[32m+[m				[32mif (arguments[i] !== undefined)[m[41m[m
[32m+[m					[32mreturn arguments[i];[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mreturn undefined;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mdefaultMessage: function( element, method) {[m[41m[m
[32m+[m			[32mreturn this.findDefined([m[41m[m
[32m+[m				[32mthis.customMessage( element.name, method ),[m[41m[m
[32m+[m				[32mthis.customMetaMessage( element, method ),[m[41m[m
[32m+[m				[32m// title is never undefined, so handle empty string as undefined[m[41m[m
[32m+[m				[32m!this.settings.ignoreTitle && element.title || undefined,[m[41m[m
[32m+[m				[32m$.validator.messages[method],[m[41m[m
[32m+[m				[32m"<strong>Warning: No message defined for " + element.name + "</strong>"[m[41m[m
[32m+[m			[32m);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mformatAndAdd: function( element, rule ) {[m[41m[m
[32m+[m			[32mvar message = this.defaultMessage( element, rule.method ),[m[41m[m
[32m+[m				[32mtheregex = /\$?\{(\d+)\}/g;[m[41m[m
[32m+[m			[32mif ( typeof message == "function" ) {[m[41m[m
[32m+[m				[32mmessage = message.call(this, rule.parameters, element);[m[41m[m
[32m+[m			[32m} else if (theregex.test(message)) {[m[41m[m
[32m+[m				[32mmessage = jQuery.format(message.replace(theregex, '{$1}'), rule.parameters);[m[41m[m
[32m+[m			[32m}[m[41m			[m
[32m+[m			[32mthis.errorList.push({[m[41m[m
[32m+[m				[32mmessage: message,[m[41m[m
[32m+[m				[32melement: element[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mthis.errorMap[element.name] = message;[m[41m[m
[32m+[m			[32mthis.submitted[element.name] = message;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32maddWrapper: function(toToggle) {[m[41m[m
[32m+[m			[32mif ( this.settings.wrapper )[m[41m[m
[32m+[m				[32mtoToggle = toToggle.add( toToggle.parent( this.settings.wrapper ) );[m[41m[m
[32m+[m			[32mreturn toToggle;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mdefaultShowErrors: function() {[m[41m[m
[32m+[m			[32mfor ( var i = 0; this.errorList[i]; i++ ) {[m[41m[m
[32m+[m				[32mvar error = this.errorList[i];[m[41m[m
[32m+[m				[32mthis.settings.highlight && this.settings.highlight.call( this, error.element, this.settings.errorClass, this.settings.validClass );[m[41m[m
[32m+[m				[32mthis.showLabel( error.element, error.message );[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif( this.errorList.length ) {[m[41m[m
[32m+[m				[32mthis.toShow = this.toShow.add( this.containers );[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif (this.settings.success) {[m[41m[m
[32m+[m				[32mfor ( var i = 0; this.successList[i]; i++ ) {[m[41m[m
[32m+[m					[32mthis.showLabel( this.successList[i] );[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif (this.settings.unhighlight) {[m[41m[m
[32m+[m				[32mfor ( var i = 0, elements = this.validElements(); elements[i]; i++ ) {[m[41m[m
[32m+[m					[32mthis.settings.unhighlight.call( this, elements[i], this.settings.errorClass, this.settings.validClass );[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mthis.toHide = this.toHide.not( this.toShow );[m[41m[m
[32m+[m			[32mthis.hideErrors();[m[41m[m
[32m+[m			[32mthis.addWrapper( this.toShow ).show();[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mvalidElements: function() {[m[41m[m
[32m+[m			[32mreturn this.currentElements.not(this.invalidElements());[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32minvalidElements: function() {[m[41m[m
[32m+[m			[32mreturn $(this.errorList).map(function() {[m[41m[m
[32m+[m				[32mreturn this.element;[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mshowLabel: function(element, message) {[m[41m[m
[32m+[m			[32mvar label = this.errorsFor( element );[m[41m[m
[32m+[m			[32mif ( label.length ) {[m[41m[m
[32m+[m				[32m// refresh error/success class[m[41m[m
[32m+[m				[32mlabel.removeClass().addClass( this.settings.errorClass );[m[41m[m
[32m+[m[41m			[m
[32m+[m				[32m// check if we have a generated label, replace the message then[m[41m[m
[32m+[m				[32mlabel.attr("generated") && label.html(message);[m[41m[m
[32m+[m			[32m} else {[m[41m[m
[32m+[m				[32m// create label[m[41m[m
[32m+[m				[32mlabel = $("<" + this.settings.errorElement + "/>")[m[41m[m
[32m+[m					[32m.attr({"for":  this.idOrName(element), generated: true})[m[41m[m
[32m+[m					[32m.addClass(this.settings.errorClass)[m[41m[m
[32m+[m					[32m.html(message || "");[m[41m[m
[32m+[m				[32mif ( this.settings.wrapper ) {[m[41m[m
[32m+[m					[32m// make sure the element is visible, even in IE[m[41m[m
[32m+[m					[32m// actually showing the wrapped element is handled elsewhere[m[41m[m
[32m+[m					[32mlabel = label.hide().show().wrap("<" + this.settings.wrapper + "/>").parent();[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m				[32mif ( !this.labelContainer.append(label).length )[m[41m[m
[32m+[m					[32mthis.settings.errorPlacement[m[41m[m
[32m+[m						[32m? this.settings.errorPlacement(label, $(element) )[m[41m[m
[32m+[m						[32m: label.insertAfter(element);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif ( !message && this.settings.success ) {[m[41m[m
[32m+[m				[32mlabel.text("");[m[41m[m
[32m+[m				[32mtypeof this.settings.success == "string"[m[41m[m
[32m+[m					[32m? label.addClass( this.settings.success )[m[41m[m
[32m+[m					[32m: this.settings.success( label );[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mthis.toShow = this.toShow.add(label);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32merrorsFor: function(element) {[m[41m[m
[32m+[m			[32mvar name = this.idOrName(element);[m[41m[m
[32m+[m[41m    [m		[32mreturn this.errors().filter(function() {[m[41m[m
[32m+[m				[32mreturn $(this).attr('for') == name;[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32midOrName: function(element) {[m[41m[m
[32m+[m			[32mreturn this.groups[element.name] || (this.checkable(element) ? element.name : element.id || element.name);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mcheckable: function( element ) {[m[41m[m
[32m+[m			[32mreturn /radio|checkbox/i.test(element.type);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mfindByName: function( name ) {[m[41m[m
[32m+[m			[32m// select by name and filter by form for performance over form.find("[name=...]")[m[41m[m
[32m+[m			[32mvar form = this.currentForm;[m[41m[m
[32m+[m			[32mreturn $(document.getElementsByName(name)).map(function(index, element) {[m[41m[m
[32m+[m				[32mreturn element.form == form && element.name == name && element  || null;[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mgetLength: function(value, element) {[m[41m[m
[32m+[m			[32mswitch( element.nodeName.toLowerCase() ) {[m[41m[m
[32m+[m			[32mcase 'select':[m[41m[m
[32m+[m				[32mreturn $("option:selected", element).length;[m[41m[m
[32m+[m			[32mcase 'input':[m[41m[m
[32m+[m				[32mif( this.checkable( element) )[m[41m[m
[32m+[m					[32mreturn this.findByName(element.name).filter(':checked').length;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mreturn value.length;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m		[32mdepend: function(param, element) {[m[41m[m
[32m+[m			[32mreturn this.dependTypes[typeof param][m[41m[m
[32m+[m				[32m? this.dependTypes[typeof param](param, element)[m[41m[m
[32m+[m				[32m: true;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m		[32mdependTypes: {[m[41m[m
[32m+[m			[32m"boolean": function(param, element) {[m[41m[m
[32m+[m				[32mreturn param;[m[41m[m
[32m+[m			[32m},[m[41m[m
[32m+[m			[32m"string": function(param, element) {[m[41m[m
[32m+[m				[32mreturn !!$(param, element.form).length;[m[41m[m
[32m+[m			[32m},[m[41m[m
[32m+[m			[32m"function": function(param, element) {[m[41m[m
[32m+[m				[32mreturn param(element);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32moptional: function(element) {[m[41m[m
[32m+[m			[32mreturn !$.validator.methods.required.call(this, $.trim(element.value), element) && "dependency-mismatch";[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mstartRequest: function(element) {[m[41m[m
[32m+[m			[32mif (!this.pending[element.name]) {[m[41m[m
[32m+[m				[32mthis.pendingRequest++;[m[41m[m
[32m+[m				[32mthis.pending[element.name] = true;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mstopRequest: function(element, valid) {[m[41m[m
[32m+[m			[32mthis.pendingRequest--;[m[41m[m
[32m+[m			[32m// sometimes synchronization fails, make sure pendingRequest is never < 0[m[41m[m
[32m+[m			[32mif (this.pendingRequest < 0)[m[41m[m
[32m+[m				[32mthis.pendingRequest = 0;[m[41m[m
[32m+[m			[32mdelete this.pending[element.name];[m[41m[m
[32m+[m			[32mif ( valid && this.pendingRequest == 0 && this.formSubmitted && this.form() ) {[m[41m[m
[32m+[m				[32m$(this.currentForm).submit();[m[41m[m
[32m+[m				[32mthis.formSubmitted = false;[m[41m[m
[32m+[m			[32m} else if (!valid && this.pendingRequest == 0 && this.formSubmitted) {[m[41m[m
[32m+[m				[32m$(this.currentForm).triggerHandler("invalid-form", [this]);[m[41m[m
[32m+[m				[32mthis.formSubmitted = false;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mpreviousValue: function(element) {[m[41m[m
[32m+[m			[32mreturn $.data(element, "previousValue") || $.data(element, "previousValue", {[m[41m[m
[32m+[m				[32mold: null,[m[41m[m
[32m+[m				[32mvalid: true,[m[41m[m
[32m+[m				[32mmessage: this.defaultMessage( element, "remote" )[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mclassRuleSettings: {[m[41m[m
[32m+[m		[32mrequired: {required: true},[m[41m[m
[32m+[m		[32memail: {email: true},[m[41m[m
[32m+[m		[32murl: {url: true},[m[41m[m
[32m+[m		[32mdate: {date: true},[m[41m[m
[32m+[m		[32mdateISO: {dateISO: true},[m[41m[m
[32m+[m		[32mdateDE: {dateDE: true},[m[41m[m
[32m+[m		[32mnumber: {number: true},[m[41m[m
[32m+[m		[32mnumberDE: {numberDE: true},[m[41m[m
[32m+[m		[32mdigits: {digits: true},[m[41m[m
[32m+[m		[32mcreditcard: {creditcard: true}[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32maddClassRules: function(className, rules) {[m[41m[m
[32m+[m		[32m/// <summary>[m[41m[m
[32m+[m		[32m/// Add a compound class method - useful to refactor common combinations of rules into a single[m[41m[m
[32m+[m		[32m/// class.[m[41m[m
[32m+[m		[32m/// </summary>[m[41m[m
[32m+[m		[32m/// <param name="name" type="String">[m[41m[m
[32m+[m		[32m/// The name of the class rule to add[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m		[32m/// <param name="rules" type="Options">[m[41m[m
[32m+[m		[32m/// The compound rules[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mclassName.constructor == String ?[m[41m[m
[32m+[m			[32mthis.classRuleSettings[className] = rules :[m[41m[m
[32m+[m			[32m$.extend(this.classRuleSettings, className);[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mclassRules: function(element) {[m[41m[m
[32m+[m		[32mvar rules = {};[m[41m[m
[32m+[m		[32mvar classes = $(element).attr('class');[m[41m[m
[32m+[m		[32mclasses && $.each(classes.split(' '), function() {[m[41m[m
[32m+[m			[32mif (this in $.validator.classRuleSettings) {[m[41m[m
[32m+[m				[32m$.extend(rules, $.validator.classRuleSettings[this]);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m});[m[41m[m
[32m+[m		[32mreturn rules;[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mattributeRules: function(element) {[m[41m[m
[32m+[m		[32mvar rules = {};[m[41m[m
[32m+[m		[32mvar $element = $(element);[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mfor (var method in $.validator.methods) {[m[41m[m
[32m+[m			[32mvar value = $element.attr(method);[m[41m[m
[32m+[m			[32mif (value) {[m[41m[m
[32m+[m				[32mrules[method] = value;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// maxlength may be returned as -1, 2147483647 (IE) and 524288 (safari) for text inputs[m[41m[m
[32m+[m		[32mif (rules.maxlength && /-1|2147483647|524288/.test(rules.maxlength)) {[m[41m[m
[32m+[m			[32mdelete rules.maxlength;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mreturn rules;[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mmetadataRules: function(element) {[m[41m[m
[32m+[m		[32mif (!$.metadata) return {};[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mvar meta = $.data(element.form, 'validator').settings.meta;[m[41m[m
[32m+[m		[32mreturn meta ?[m[41m[m
[32m+[m			[32m$(element).metadata()[meta] :[m[41m[m
[32m+[m			[32m$(element).metadata();[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mstaticRules: function(element) {[m[41m[m
[32m+[m		[32mvar rules = {};[m[41m[m
[32m+[m		[32mvar validator = $.data(element.form, 'validator');[m[41m[m
[32m+[m		[32mif (validator.settings.rules) {[m[41m[m
[32m+[m			[32mrules = $.validator.normalizeRule(validator.settings.rules[element.name]) || {};[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mreturn rules;[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mnormalizeRules: function(rules, element) {[m[41m[m
[32m+[m		[32m// handle dependency check[m[41m[m
[32m+[m		[32m$.each(rules, function(prop, val) {[m[41m[m
[32m+[m			[32m// ignore rule when param is explicitly false, eg. required:false[m[41m[m
[32m+[m			[32mif (val === false) {[m[41m[m
[32m+[m				[32mdelete rules[prop];[m[41m[m
[32m+[m				[32mreturn;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif (val.param || val.depends) {[m[41m[m
[32m+[m				[32mvar keepRule = true;[m[41m[m
[32m+[m				[32mswitch (typeof val.depends) {[m[41m[m
[32m+[m					[32mcase "string":[m[41m[m
[32m+[m						[32mkeepRule = !!$(val.depends, element.form).length;[m[41m[m
[32m+[m						[32mbreak;[m[41m[m
[32m+[m					[32mcase "function":[m[41m[m
[32m+[m						[32mkeepRule = val.depends.call(element, element);[m[41m[m
[32m+[m						[32mbreak;[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m				[32mif (keepRule) {[m[41m[m
[32m+[m					[32mrules[prop] = val.param !== undefined ? val.param : true;[m[41m[m
[32m+[m				[32m} else {[m[41m[m
[32m+[m					[32mdelete rules[prop];[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m});[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// evaluate parameters[m[41m[m
[32m+[m		[32m$.each(rules, function(rule, parameter) {[m[41m[m
[32m+[m			[32mrules[rule] = $.isFunction(parameter) ? parameter(element) : parameter;[m[41m[m
[32m+[m		[32m});[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// clean number parameters[m[41m[m
[32m+[m		[32m$.each(['minlength', 'maxlength', 'min', 'max'], function() {[m[41m[m
[32m+[m			[32mif (rules[this]) {[m[41m[m
[32m+[m				[32mrules[this] = Number(rules[this]);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m});[m[41m[m
[32m+[m		[32m$.each(['rangelength', 'range'], function() {[m[41m[m
[32m+[m			[32mif (rules[this]) {[m[41m[m
[32m+[m				[32mrules[this] = [Number(rules[this][0]), Number(rules[this][1])];[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m});[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mif ($.validator.autoCreateRanges) {[m[41m[m
[32m+[m			[32m// auto-create ranges[m[41m[m
[32m+[m			[32mif (rules.min && rules.max) {[m[41m[m
[32m+[m				[32mrules.range = [rules.min, rules.max];[m[41m[m
[32m+[m				[32mdelete rules.min;[m[41m[m
[32m+[m				[32mdelete rules.max;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif (rules.minlength && rules.maxlength) {[m[41m[m
[32m+[m				[32mrules.rangelength = [rules.minlength, rules.maxlength];[m[41m[m
[32m+[m				[32mdelete rules.minlength;[m[41m[m
[32m+[m				[32mdelete rules.maxlength;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// To support custom messages in metadata ignore rule methods titled "messages"[m[41m[m
[32m+[m		[32mif (rules.messages) {[m[41m[m
[32m+[m			[32mdelete rules.messages;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mreturn rules;[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32m// Converts a simple string to a {string: true} rule, e.g., "required" to {required:true}[m[41m[m
[32m+[m	[32mnormalizeRule: function(data) {[m[41m[m
[32m+[m		[32mif( typeof data == "string" ) {[m[41m[m
[32m+[m			[32mvar transformed = {};[m[41m[m
[32m+[m			[32m$.each(data.split(/\s/), function() {[m[41m[m
[32m+[m				[32mtransformed[this] = true;[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m			[32mdata = transformed;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mreturn data;[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32m// http://docs.jquery.com/Plugins/Validation/Validator/addMethod[m[41m[m
[32m+[m	[32maddMethod: function(name, method, message) {[m[41m[m
[32m+[m		[32m/// <summary>[m[41m[m
[32m+[m		[32m/// Add a custom validation method. It must consist of a name (must be a legal javascript[m[41m [m
[32m+[m		[32m/// identifier), a javascript based function and a default string message.[m[41m[m
[32m+[m		[32m/// </summary>[m[41m[m
[32m+[m		[32m/// <param name="name" type="String">[m[41m[m
[32m+[m		[32m/// The name of the method, used to identify and referencing it, must be a valid javascript[m[41m[m
[32m+[m		[32m/// identifier[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m		[32m/// <param name="method" type="Function">[m[41m[m
[32m+[m		[32m/// The actual method implementation, returning true if an element is valid[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m		[32m/// <param name="message" type="String" optional="true">[m[41m[m
[32m+[m		[32m/// (Optional) The default message to display for this method. Can be a function created by[m[41m [m
[32m+[m		[32m/// jQuery.validator.format(value). When undefined, an already existing message is used[m[41m [m
[32m+[m		[32m/// (handy for localization), otherwise the field-specific messages have to be defined.[m[41m[m
[32m+[m		[32m/// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m$.validator.methods[name] = method;[m[41m[m
[32m+[m		[32m$.validator.messages[name] = message != undefined ? message : $.validator.messages[name];[m[41m[m
[32m+[m		[32mif (method.length < 3) {[m[41m[m
[32m+[m			[32m$.validator.addClassRules(name, $.validator.normalizeRule(name));[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m	[32m},[m[41m[m
[32m+[m[41m[m
[32m+[m	[32mmethods: {[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/required[m[41m[m
[32m+[m		[32mrequired: function(value, element, param) {[m[41m[m
[32m+[m			[32m// check if dependency is met[m[41m[m
[32m+[m			[32mif ( !this.depend(param, element) )[m[41m[m
[32m+[m				[32mreturn "dependency-mismatch";[m[41m[m
[32m+[m			[32mswitch( element.nodeName.toLowerCase() ) {[m[41m[m
[32m+[m			[32mcase 'select':[m[41m[m
[32m+[m				[32m// could be an array for select-multiple or a string, both are fine this way[m[41m[m
[32m+[m				[32mvar val = $(element).val();[m[41m[m
[32m+[m				[32mreturn val && val.length > 0;[m[41m[m
[32m+[m			[32mcase 'input':[m[41m[m
[32m+[m				[32mif ( this.checkable(element) )[m[41m[m
[32m+[m					[32mreturn this.getLength(value, element) > 0;[m[41m[m
[32m+[m			[32mdefault:[m[41m[m
[32m+[m				[32mreturn $.trim(value).length > 0;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/remote[m[41m[m
[32m+[m		[32mremote: function(value, element, param) {[m[41m[m
[32m+[m			[32mif ( this.optional(element) )[m[41m[m
[32m+[m				[32mreturn "dependency-mismatch";[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mvar previous = this.previousValue(element);[m[41m[m
[32m+[m			[32mif (!this.settings.messages[element.name] )[m[41m[m
[32m+[m				[32mthis.settings.messages[element.name] = {};[m[41m[m
[32m+[m			[32mprevious.originalMessage = this.settings.messages[element.name].remote;[m[41m[m
[32m+[m			[32mthis.settings.messages[element.name].remote = previous.message;[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mparam = typeof param == "string" && {url:param} || param;[m[41m [m
[32m+[m[41m			[m
[32m+[m			[32mif ( this.pending[element.name] ) {[m[41m[m
[32m+[m				[32mreturn "pending";[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mif ( previous.old === value ) {[m[41m[m
[32m+[m				[32mreturn previous.valid;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mprevious.old = value;[m[41m[m
[32m+[m			[32mvar validator = this;[m[41m[m
[32m+[m			[32mthis.startRequest(element);[m[41m[m
[32m+[m			[32mvar data = {};[m[41m[m
[32m+[m			[32mdata[element.name] = value;[m[41m[m
[32m+[m			[32m$.ajax($.extend(true, {[m[41m[m
[32m+[m				[32murl: param,[m[41m[m
[32m+[m				[32mmode: "abort",[m[41m[m
[32m+[m				[32mport: "validate" + element.name,[m[41m[m
[32m+[m				[32mdataType: "json",[m[41m[m
[32m+[m				[32mdata: data,[m[41m[m
[32m+[m				[32msuccess: function(response) {[m[41m[m
[32m+[m					[32mvalidator.settings.messages[element.name].remote = previous.originalMessage;[m[41m[m
[32m+[m					[32mvar valid = response === true;[m[41m[m
[32m+[m					[32mif ( valid ) {[m[41m[m
[32m+[m						[32mvar submitted = validator.formSubmitted;[m[41m[m
[32m+[m						[32mvalidator.prepareElement(element);[m[41m[m
[32m+[m						[32mvalidator.formSubmitted = submitted;[m[41m[m
[32m+[m						[32mvalidator.successList.push(element);[m[41m[m
[32m+[m						[32mvalidator.showErrors();[m[41m[m
[32m+[m					[32m} else {[m[41m[m
[32m+[m						[32mvar errors = {};[m[41m[m
[32m+[m						[32mvar message = response || validator.defaultMessage(element, "remote");[m[41m[m
[32m+[m						[32merrors[element.name] = previous.message = $.isFunction(message) ? message(value) : message;[m[41m[m
[32m+[m						[32mvalidator.showErrors(errors);[m[41m[m
[32m+[m					[32m}[m[41m[m
[32m+[m					[32mprevious.valid = valid;[m[41m[m
[32m+[m					[32mvalidator.stopRequest(element, valid);[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m}, param));[m[41m[m
[32m+[m			[32mreturn "pending";[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/minlength[m[41m[m
[32m+[m		[32mminlength: function(value, element, param) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || this.getLength($.trim(value), element) >= param;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/maxlength[m[41m[m
[32m+[m		[32mmaxlength: function(value, element, param) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || this.getLength($.trim(value), element) <= param;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/rangelength[m[41m[m
[32m+[m		[32mrangelength: function(value, element, param) {[m[41m[m
[32m+[m			[32mvar length = this.getLength($.trim(value), element);[m[41m[m
[32m+[m			[32mreturn this.optional(element) || ( length >= param[0] && length <= param[1] );[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/min[m[41m[m
[32m+[m		[32mmin: function( value, element, param ) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || value >= param;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/max[m[41m[m
[32m+[m		[32mmax: function( value, element, param ) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || value <= param;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/range[m[41m[m
[32m+[m		[32mrange: function( value, element, param ) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || ( value >= param[0] && value <= param[1] );[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/email[m[41m[m
[32m+[m		[32memail: function(value, element) {[m[41m[m
[32m+[m			[32m// contributed by Scott Gonzalez: http://projects.scottsplayground.com/email_address_validation/[m[41m[m
[32m+[m			[32mreturn this.optional(element) || /^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i.test(value);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/url[m[41m[m
[32m+[m		[32murl: function(value, element) {[m[41m[m
[32m+[m			[32m// contributed by Scott Gonzalez: http://projects.scottsplayground.com/iri/[m[41m[m
[32m+[m			[32mreturn this.optional(element) || /^(https?|ftp):\/\/(((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:)*@)?(((\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5]))|((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?)(:\d*)?)(\/((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)+(\/(([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)*)*)?)?(\?((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)|[\uE000-\uF8FF]|\/|\?)*)?(\#((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)|\/|\?)*)?$/i.test(value);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m        [m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/date[m[41m[m
[32m+[m		[32mdate: function(value, element) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || !/Invalid|NaN/.test(new Date(value));[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/dateISO[m[41m[m
[32m+[m		[32mdateISO: function(value, element) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || /^\d{4}[\/-]\d{1,2}[\/-]\d{1,2}$/.test(value);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/number[m[41m[m
[32m+[m		[32mnumber: function(value, element) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || /^-?(?:\d+|\d{1,3}(?:,\d{3})+)(?:\.\d+)?$/.test(value);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m	[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/digits[m[41m[m
[32m+[m		[32mdigits: function(value, element) {[m[41m[m
[32m+[m			[32mreturn this.optional(element) || /^\d+$/.test(value);[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/creditcard[m[41m[m
[32m+[m		[32m// based on http://en.wikipedia.org/wiki/Luhn[m[41m[m
[32m+[m		[32mcreditcard: function(value, element) {[m[41m[m
[32m+[m			[32mif ( this.optional(element) )[m[41m[m
[32m+[m				[32mreturn "dependency-mismatch";[m[41m[m
[32m+[m			[32m// accept only digits and dashes[m[41m[m
[32m+[m			[32mif (/[^0-9-]+/.test(value))[m[41m[m
[32m+[m				[32mreturn false;[m[41m[m
[32m+[m			[32mvar nCheck = 0,[m[41m[m
[32m+[m				[32mnDigit = 0,[m[41m[m
[32m+[m				[32mbEven = false;[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mvalue = value.replace(/\D/g, "");[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mfor (var n = value.length - 1; n >= 0; n--) {[m[41m[m
[32m+[m				[32mvar cDigit = value.charAt(n);[m[41m[m
[32m+[m				[32mvar nDigit = parseInt(cDigit, 10);[m[41m[m
[32m+[m				[32mif (bEven) {[m[41m[m
[32m+[m					[32mif ((nDigit *= 2) > 9)[m[41m[m
[32m+[m						[32mnDigit -= 9;[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m				[32mnCheck += nDigit;[m[41m[m
[32m+[m				[32mbEven = !bEven;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mreturn (nCheck % 10) == 0;[m[41m[m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/accept[m[41m[m
[32m+[m		[32maccept: function(value, element, param) {[m[41m[m
[32m+[m			[32mparam = typeof param == "string" ? param.replace(/,/g, '|') : "png|jpe?g|gif";[m[41m[m
[32m+[m			[32mreturn this.optional(element) || value.match(new RegExp(".(" + param + ")$", "i"));[m[41m [m
[32m+[m		[32m},[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m// http://docs.jquery.com/Plugins/Validation/Methods/equalTo[m[41m[m
[32m+[m		[32mequalTo: function(value, element, param) {[m[41m[m
[32m+[m			[32m// bind to the blur event of the target in order to revalidate whenever the target field is updated[m[41m[m
[32m+[m			[32m// TODO find a way to bind the event just once, avoiding the unbind-rebind overhead[m[41m[m
[32m+[m			[32mvar target = $(param).unbind(".validate-equalTo").bind("blur.validate-equalTo", function() {[m[41m[m
[32m+[m				[32m$(element).valid();[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m			[32mreturn value == target.val();[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m[32m});[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// deprecated, use $.validator.format instead[m[41m[m
[32m+[m[32m$.format = $.validator.format;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m})(jQuery);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// ajax mode: abort[m[41m[m
[32m+[m[32m// usage: $.ajax({ mode: "abort"[, port: "uniqueport"]});[m[41m[m
[32m+[m[32m// if mode:"abort" is used, the previous request on that port (port can be undefined) is aborted via XMLHttpRequest.abort()[m[41m [m
[32m+[m[32m;(function($) {[m[41m[m
[32m+[m	[32mvar pendingRequests = {};[m[41m[m
[32m+[m		[32m// Use a prefilter if available (1.5+)[m[41m[m
[32m+[m	[32mif ( $.ajaxPrefilter ) {[m[41m[m
[32m+[m		[32m$.ajaxPrefilter(function(settings, _, xhr) {[m[41m[m
[32m+[m		[32m    var port = settings.port;[m[41m[m
[32m+[m		[32m    if (settings.mode == "abort") {[m[41m[m
[32m+[m			[32m    if ( pendingRequests[port] ) {[m[41m[m
[32m+[m				[32m    pendingRequests[port].abort();[m[41m[m
[32m+[m			[32m    }				pendingRequests[port] = xhr;[m[41m[m
[32m+[m		[32m    }[m[41m[m
[32m+[m	[32m    });[m[41m[m
[32m+[m	[32m} else {[m[41m[m
[32m+[m		[32m// Proxy ajax[m[41m[m
[32m+[m		[32mvar ajax = $.ajax;[m[41m[m
[32m+[m		[32m$.ajax = function(settings) {[m[41m[m
[32m+[m			[32mvar mode = ( "mode" in settings ? settings : $.ajaxSettings ).mode,[m[41m[m
[32m+[m				[32mport = ( "port" in settings ? settings : $.ajaxSettings ).port;[m[41m[m
[32m+[m			[32mif (mode == "abort") {[m[41m[m
[32m+[m				[32mif ( pendingRequests[port] ) {[m[41m[m
[32m+[m					[32mpendingRequests[port].abort();[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m    return (pendingRequests[port] = ajax.apply(this, arguments));[m[41m[m
[32m+[m		[32m    }[m[41m[m
[32m+[m		[32m    return ajax.apply(this, arguments);[m[41m[m
[32m+[m	[32m    };[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m})(jQuery);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// provides cross-browser focusin and focusout events[m[41m[m
[32m+[m[32m// IE has native support, in other browsers, use event caputuring (neither bubbles)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// provides delegate(type: String, delegate: Selector, handler: Callback) plugin for easier event delegation[m[41m[m
[32m+[m[32m// handler is only called when $(event.target).is(delegate), in the scope of the jquery-object for event.target[m[41m [m
[32m+[m[32m;(function($) {[m[41m[m
[32m+[m	[32m// only implement if not provided by jQuery core (since 1.4)[m[41m[m
[32m+[m	[32m// TODO verify if jQuery 1.4's implementation is compatible with older jQuery special-event APIs[m[41m[m
[32m+[m	[32mif (!jQuery.event.special.focusin && !jQuery.event.special.focusout && document.addEventListener) {[m[41m[m
[32m+[m		[32m$.each({[m[41m[m
[32m+[m			[32mfocus: 'focusin',[m[41m[m
[32m+[m			[32mblur: 'focusout'[m[41m	[m
[32m+[m		[32m}, function( original, fix ){[m[41m[m
[32m+[m			[32m$.event.special[fix] = {[m[41m[m
[32m+[m				[32msetup:function() {[m[41m[m
[32m+[m					[32mthis.addEventListener( original, handler, true );[m[41m[m
[32m+[m				[32m},[m[41m[m
[32m+[m				[32mteardown:function() {[m[41m[m
[32m+[m					[32mthis.removeEventListener( original, handler, true );[m[41m[m
[32m+[m				[32m},[m[41m[m
[32m+[m				[32mhandler: function(e) {[m[41m[m
[32m+[m					[32marguments[0] = $.event.fix(e);[m[41m[m
[32m+[m					[32marguments[0].type = fix;[m[41m[m
[32m+[m					[32mreturn $.event.handle.apply(this, arguments);[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m};[m[41m[m
[32m+[m			[32mfunction handler(e) {[m[41m[m
[32m+[m				[32me = $.event.fix(e);[m[41m[m
[32m+[m				[32me.type = fix;[m[41m[m
[32m+[m				[32mreturn $.event.handle.call(this, e);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m});[m[41m[m
[32m+[m	[32m};[m[41m[m
[32m+[m	[32m$.extend($.fn, {[m[41m[m
[32m+[m		[32mvalidateDelegate: function(delegate, type, handler) {[m[41m[m
[32m+[m			[32mreturn this.bind(type, function(event) {[m[41m[m
[32m+[m				[32mvar target = $(event.target);[m[41m[m
[32m+[m				[32mif (target.is(delegate)) {[m[41m[m
[32m+[m					[32mreturn handler.apply(target, arguments);[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m});[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m	[32m});[m[41m[m
[32m+[m[32m})(jQuery);[m[41m[m
