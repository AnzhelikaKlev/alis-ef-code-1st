[31m-function toSelector( tokens ) {[m
[31m-	var i = 0,[m
[31m-		len = tokens.length,[m
[31m-		selector = "";[m
[31m-	for ( ; i < len; i++ ) {[m
[31m-		selector += tokens[i].value;[m
[31m-	}[m
[31m-	return selector;[m
[31m-}[m
