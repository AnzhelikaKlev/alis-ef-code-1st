[32m+[m[41m[m
[32m+[m[32m        return this.animate(props, speed, easing, callback);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.filter = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Reduce the set of matched elements to those that match the selector or pass the function's test.[m[41m[m
[32m+[m[32m        ///     &#10;1 - filter(selector)[m[41m [m
[32m+[m[32m        ///     &#10;2 - filter(function(index))[m[41m [m
[32m+[m[32m        ///     &#10;3 - filter(element)[m[41m [m
[32m+[m[32m        ///     &#10;4 - filter(jQuery object)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match the current set of elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(winnow(this, selector, true));[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.find = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the descendants of each element in the current set of matched elements, filtered by a selector, jQuery object, or element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - find(selector)[m[41m [m
[32m+[m[32m        ///     &#10;2 - find(jQuery object)[m[41m [m
[32m+[m[32m        ///     &#10;3 - find(element)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var i, ret, self,[m[41m[m
[32m+[m			[32mlen = this.length;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (typeof selector !== "string") {[m[41m[m
[32m+[m[32m            self = this;[m[41m[m
[32m+[m[32m            return this.pushStack(jQuery(selector).filter(function () {[m[41m[m
[32m+[m[32m                for (i = 0; i < len; i++) {[m[41m[m
[32m+[m[32m                    if (jQuery.contains(self[i], this)) {[m[41m[m
[32m+[m[32m                        return true;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ret = [];[m[41m[m
[32m+[m[32m        for (i = 0; i < len; i++) {[m[41m[m
[32m+[m[32m            jQuery.find(selector, this[i], ret);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Needed because $( selector, context ) becomes $( context ).find( selector )[m[41m[m
[32m+[m[32m        ret = this.pushStack(len > 1 ? jQuery.unique(ret) : ret);[m[41m[m
[32m+[m[32m        ret.selector = (this.selector ? this.selector + " " : "") + selector;[m[41m[m
[32m+[m[32m        return ret;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.finish = function (type) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Stop the currently-running animation, remove all queued animations, and complete all animations for the matched elements.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="type" type="String">[m[41m[m
[32m+[m[32m        ///     The name of the queue in which to stop animations.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (type !== false) {[m[41m[m
[32m+[m[32m            type = type || "fx";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return this.each(function () {[m[41m[m
[32m+[m[32m            var index,[m[41m[m
[32m+[m				[32mdata = jQuery._data(this),[m[41m[m
[32m+[m				[32mqueue = data[type + "queue"],[m[41m[m
[32m+[m				[32mhooks = data[type + "queueHooks"],[m[41m[m
[32m+[m				[32mtimers = jQuery.timers,[m[41m[m
[32m+[m				[32mlength = queue ? queue.length : 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // enable finishing flag on private data[m[41m[m
[32m+[m[32m            data.finish = true;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // empty the queue first[m[41m[m
[32m+[m[32m            jQuery.queue(this, type, []);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (hooks && hooks.cur && hooks.cur.finish) {[m[41m[m
[32m+[m[32m                hooks.cur.finish.call(this);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // look for any active animations, and finish them[m[41m[m
[32m+[m[32m            for (index = timers.length; index--;) {[m[41m[m
[32m+[m[32m                if (timers[index].elem === this && timers[index].queue === type) {[m[41m[m
[32m+[m[32m                    timers[index].anim.stop(true);[m[41m[m
[32m+[m[32m                    timers.splice(index, 1);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // look for any animations in the old queue and finish them[m[41m[m
[32m+[m[32m            for (index = 0; index < length; index++) {[m[41m[m
[32m+[m[32m                if (queue[index] && queue[index].finish) {[m[41m[m
[32m+[m[32m                    queue[index].finish.call(this);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // turn off finishing flag[m[41m[m
[32m+[m[32m            delete data.finish;[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.first = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Reduce the set of matched elements to the first in the set.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.eq(0);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.focus = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Bind an event handler to the "focus" JavaScript event, or trigger that event on an element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - focus(handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - focus(eventData, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;3 - focus()[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Object">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute each time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return arguments.length > 0 ?[m[41m[m
[32m+[m			[32mthis.on(name, null, data, fn) :[m[41m[m
[32m+[m			[32mthis.trigger(name);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.focusin = function (data, fn) {[m[41m[m
