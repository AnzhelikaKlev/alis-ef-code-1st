[32m+[m[32m    function getFunction(code, argNames) {[m[41m[m
[32m+[m[32m        var fn = window, parts = (code || "").split(".");[m[41m[m
[32m+[m[32m        while (fn && parts.length) {[m[41m[m
[32m+[m[32m            fn = fn[parts.shift()];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        if (typeof (fn) === "function") {[m[41m[m
[32m+[m[32m            return fn;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        argNames.push(code);[m[41m[m
[32m+[m[32m        return Function.constructor.apply(null, argNames);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function isMethodProxySafe(method) {[m[41m[m
[32m+[m[32m        return method === "GET" || method === "POST";[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function asyncOnBeforeSend(xhr, method) {[m[41m[m
[32m+[m[32m        if (!isMethodProxySafe(method)) {[m[41m[m
[32m+[m[32m            xhr.setRequestHeader("X-HTTP-Method-Override", method);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function asyncOnSuccess(element, data, contentType) {[m[41m[m
[32m+[m[32m        var mode;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (contentType.indexOf("application/x-javascript") !== -1) {  // jQuery already executes JavaScript for us[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        mode = (element.getAttribute("data-ajax-mode") || "").toUpperCase();[m[41m[m
[32m+[m[32m        $(element.getAttribute("data-ajax-update")).each(function (i, update) {[m[41m[m
[32m+[m[32m            var top;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            switch (mode) {[m[41m[m
[32m+[m[32m            case "BEFORE":[m[41m[m
[32m+[m[32m                top = update.firstChild;[m[41m[m
[32m+[m[32m                $("<div />").html(data).contents().each(function () {[m[41m[m
[32m+[m[32m                    update.insertBefore(this, top);[m[41m[m
[32m+[m[32m                });[m[41m[m
[32m+[m[32m                break;[m[41m[m
[32m+[m[32m            case "AFTER":[m[41m[m
[32m+[m[32m                $("<div />").html(data).contents().each(function () {[m[41m[m
[32m+[m[32m                    update.appendChild(this);[m[41m[m
[32m+[m[32m                });[m[41m[m
[32m+[m[32m                break;[m[41m[m
[32m+[m[32m            default:[m[41m[m
[32m+[m[32m                $(update).html(data);[m[41m[m
[32m+[m[32m                break;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function asyncRequest(element, options) {[m[41m[m
[32m+[m[32m        var confirm, loading, method, duration;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        confirm = element.getAttribute("data-ajax-confirm");[m[41m[m
[32m+[m[32m        if (confirm && !window.confirm(confirm)) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        loading = $(element.getAttribute("data-ajax-loading"));[m[41m[m
[32m+[m[32m        duration = element.getAttribute("data-ajax-loading-duration") || 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        $.extend(options, {[m[41m[m
[32m+[m[32m            type: element.getAttribute("data-ajax-method") || undefined,[m[41m[m
[32m+[m[32m            url: element.getAttribute("data-ajax-url") || undefined,[m[41m[m
[32m+[m[32m            beforeSend: function (xhr) {[m[41m[m
[32m+[m[32m                var result;[m[41m[m
[32m+[m[32m                asyncOnBeforeSend(xhr, method);[m[41m[m
[32m+[m[32m                result = getFunction(element.getAttribute("data-ajax-begin"), ["xhr"]).apply(this, arguments);[m[41m[m
[32m+[m[32m                if (result !== false) {[m[41m[m
[32m+[m[32m                    loading.show(duration);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                return result;[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m            complete: function () {[m[41m[m
[32m+[m[32m                loading.hide(duration);[m[41m[m
[32m+[m[32m                getFunction(element.getAttribute("data-ajax-complete"), ["xhr", "status"]).apply(this, arguments);[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m            success: function (data, status, xhr) {[m[41m[m
[32m+[m[32m                asyncOnSuccess(element, data, xhr.getResponseHeader("Content-Type") || "text/html");[m[41m[m
[32m+[m[32m                getFunction(element.getAttribute("data-ajax-success"), ["data", "status", "xhr"]).apply(this, arguments);[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m            error: getFunction(element.getAttribute("data-ajax-failure"), ["xhr", "status", "error"])[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        options.data.push({ name: "X-Requested-With", value: "XMLHttpRequest" });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        method = options.type.toUpperCase();[m[41m[m
[32m+[m[32m        if (!isMethodProxySafe(method)) {[m[41m[m
[32m+[m[32m            options.type = "POST";[m[41m[m
[32m+[m[32m            options.data.push({ name: "X-HTTP-Method-Override", value: method });[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        $.ajax(options);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function validate(form) {[m[41m[m
[32m+[m[32m        var validationInfo = $(form).data(data_validation);[m[41m[m
[32m+[m[32m        return !validationInfo || !validationInfo.validate || validationInfo.validate();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    $(document).on("click", "a[data-ajax=true]", function (evt) {[m[41m[m
[32m+[m[32m        evt.preventDefault();[m[41m[m
[32m+[m[32m        asyncRequest(this, {[m[41m[m
[32m+[m[32m            url: this.href,[m[41m[m
[32m+[m[32m            type: "GET",[m[41m[m
[32m+[m[32m            data: [][m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    $(document).on("click", "form[data-ajax=true] input[type=image]", function (evt) {[m[41m[m
[32m+[m[32m        var name = evt.target.name,[m[41m[m
[32m+[m[32m            $target = $(evt.target),[m[41m[m
[32m+[m[32m            form = $target.parents("form")[0],[m[41m[m
[32m+[m[32m            offset = $target.offset();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        $(form).data(data_click, [[m[41m[m
[32m+[m[32m            { name: name + ".x", value: Math.round(evt.pageX - offset.left) },[m[41m[m
[32m+[m[32m            { name: name + ".y", value: Math.round(evt.pageY - offset.top) }[m[41m[m
[32m+[m[32m        ]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        setTimeout(function () {[m[41m[m
[32m+[m[32m            $(form).removeData(data_click);[m[41m[m
[32m+[m[32m        }, 0);[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    $(document).on("click", "form[data-ajax=true] :submit", function (evt) {[m[41m[m
[32m+[m[32m        var name = evt.target.name,[m[41m[m
[32m+[m[32m            form = $(evt.target).parents("form")[0];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        $(form).data(data_click, name ? [{ name: name, value: evt.target.value }] : []);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        setTimeout(function () {[m[41m[m
[32m+[m[32m            $(form).removeData(data_click);[m[41m[m
[32m+[m[32m        }, 0);[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    $(document).on("submit", "form[data-ajax=true]", function (evt) {[m[41m[m
[32m+[m[32m        var clickInfo = $(this).data(data_click) || [];[m[41m[m
[32m+[m[32m        evt.preventDefault();[m[41m[m
[32m+[m[32m        if (!validate(this)) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        asyncRequest(this, {[m[41m[m
[32m+[m[32m            url: this.action,[m[41m[m
[32m+[m[32m            type: this.method || "GET",[m[41m[m
[32m+[m[32m            data: clickInfo.concat($(this).serializeArray())[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    });[m[41m[m
 }(jQuery));[m
