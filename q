[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.queue(type || "fx", []);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.click = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
