[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.offset = {};[m[41m[m
[32m+[m[32m    jQuery.param = function (a, traditional) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Create a serialized representation of an array or object, suitable for use in a URL query string or Ajax request.[m[41m[m
[32m+[m[32m        ///     &#10;1 - jQuery.param(obj)[m[41m [m
[32m+[m[32m        ///     &#10;2 - jQuery.param(obj, traditional)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="a" type="Object">[m[41m[m
[32m+[m[32m        ///     An array or object to serialize.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="traditional" type="Boolean">[m[41m[m
[32m+[m[32m        ///     A Boolean indicating whether to perform a traditional "shallow" serialization.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="String" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var prefix,[m[41m[m
[32m+[m[32m            s = [],[m[41m[m
[32m+[m[32m            add = function (key, value) {[m[41m[m
[32m+[m[32m                // If value is a function, invoke it and return its value[m[41m[m
[32m+[m[32m                value = jQuery.isFunction(value) ? value() : (value == null ? "" : value);[m[41m[m
[32m+[m[32m                s[s.length] = encodeURIComponent(key) + "=" + encodeURIComponent(value);[m[41m[m
[32m+[m[32m            };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Set traditional to true for jQuery <= 1.3.2 behavior.[m[41m[m
[32m+[m[32m        if (traditional === undefined) {[m[41m[m
[32m+[m[32m            traditional = jQuery.ajaxSettings && jQuery.ajaxSettings.traditional;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If an array was passed in, assume that it is an array of form elements.[m[41m[m
[32m+[m[32m        if (jQuery.isArray(a) || (a.jquery && !jQuery.isPlainObject(a))) {[m[41m[m
[32m+[m[32m            // Serialize the form elements[m[41m[m
[32m+[m[32m            jQuery.each(a, function () {[m[41m[m
[32m+[m[32m                add(this.name, this.value);[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            // If traditional, encode the "old" way (the way 1.3.2 or older[m[41m[m
[32m+[m[32m            // did it), otherwise encode params recursively.[m[41m[m
[32m+[m[32m            for (prefix in a) {[m[41m[m
[32m+[m[32m                buildParams(prefix, a[prefix], traditional, add);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Return the resulting serialization[m[41m[m
[32m+[m[32m        return s.join("&").replace(r20, "+");[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.parseHTML = function (data, context, keepScripts) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Parses a string into an array of DOM nodes.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="String">[m[41m[m
[32m+[m[32m        ///     HTML string to be parsed[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="context" domElement="true">[m[41m[m
[32m+[m[32m        ///     DOM element to serve as the context in which the HTML fragment will be created[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="keepScripts" type="Boolean">[m[41m[m
[32m+[m[32m        ///     A Boolean indicating whether to include scripts passed in the HTML string[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Array" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!data || typeof data !== "string") {[m[41m[m
[32m+[m[32m            return null;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        if (typeof context === "boolean") {[m[41m[m
[32m+[m[32m            keepScripts = context;[m[41m[m
[32m+[m[32m            context = false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        context = context || document;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var parsed = rsingleTag.exec(data),[m[41m[m
[32m+[m			[32mscripts = !keepScripts && [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Single tag[m[41m[m
[32m+[m[32m        if (parsed) {[m[41m[m
[32m+[m[32m            return [context.createElement(parsed[1])];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        parsed = jQuery.buildFragment([data], context, scripts);[m[41m[m
[32m+[m[32m        if (scripts) {[m[41m[m
[32m+[m[32m            jQuery(scripts).remove();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return jQuery.merge([], parsed.childNodes);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.parseJSON = function (data) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Takes a well-formed JSON string and returns the resulting JavaScript object.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="String">[m[41m[m
[32m+[m[32m        ///     The JSON string to parse.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Object" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Attempt to parse using the native JSON parser first[m[41m[m
[32m+[m[32m        if (window.JSON && window.JSON.parse) {[m[41m[m
[32m+[m[32m            return window.JSON.parse(data);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (data === null) {[m[41m[m
[32m+[m[32m            return data;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (typeof data === "string") {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Make sure leading/trailing whitespace is removed (IE can't handle it)[m[41m[m
[32m+[m[32m            data = jQuery.trim(data);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (data) {[m[41m[m
[32m+[m[32m                // Make sure the incoming data is actual JSON[m[41m[m
[32m+[m[32m                // Logic borrowed from http://json.org/json2.js[m[41m[m
[32m+[m[32m                if (rvalidchars.test(data.replace(rvalidescape, "@")[m[41m[m
[32m+[m					[32m.replace(rvalidtokens, "]")[m[41m[m
[32m+[m					[32m.replace(rvalidbraces, ""))) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    return (new Function("return " + data))();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        jQuery.error("Invalid JSON: " + data);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.parseXML = function (data) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Parses a string into an XML document.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="String">[m[41m[m
[32m+[m[32m        ///     a well-formed XML string to be parsed[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="XMLDocument" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var xml, tmp;[m[41m[m
[32m+[m[32m        if (!data || typeof data !== "string") {[m[41m[m
[32m+[m[32m            return null;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            if (window.DOMParser) { // Standard[m[41m[m
[32m+[m[32m                tmp = new DOMParser();[m[41m[m
[32m+[m[32m                xml = tmp.parseFromString(data, "text/xml");[m[41m[m
[32m+[m[32m            } else { // IE[m[41m[m
[32m+[m[32m                xml = new ActiveXObject("Microsoft.XMLDOM");[m[41m[m
[32m+[m[32m                xml.async = "false";[m[41m[m
[32m+[m[32m                xml.loadXML(data);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        } catch (e) {[m[41m[m
[32m+[m[32m            xml = undefined;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        if (!xml || !xml.documentElement || xml.getElementsByTagName("parsererror").length) {[m[41m[m
[32m+[m[32m            jQuery.error("Invalid XML: " + data);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return xml;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.post = function (url, data, callback, type) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Load data from the server using a HTTP POST request.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="url" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the URL to which the request is sent.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="String">[m[41m[m
[32m+[m[32m        ///     A plain object or string that is sent to the server with the request.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A callback function that is executed if the request succeeds.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="type" type="String">[m[41m[m
[32m+[m[32m        ///     The type of data expected from the server. Default: Intelligent Guess (xml, json, script, text, html).[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // shift arguments if data argument was omitted[m[41m[m
[32m+[m[32m        if (jQuery.isFunction(data)) {[m[41m[m
[32m+[m[32m            type = type || callback;[m[41m[m
[32m+[m[32m            callback = data;[m[41m[m
[32m+[m[32m            data = undefined;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.ajax({[m[41m[m
[32m+[m[32m            url: url,[m[41m[m
[32m+[m[32m            type: method,[m[41m[m
[32m+[m[32m            dataType: type,[m[41m[m
[32m+[m[32m            data: data,[m[41m[m
[32m+[m[32m            success: callback[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prop = function (elem, name, value) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret, hooks, notxml,[m[41m[m
[32m+[m			[32mnType = elem.nodeType;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // don't get/set properties on text, comment and attribute nodes[m[41m[m
[32m+[m[32m        if (!elem || nType === 3 || nType === 8 || nType === 2) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        notxml = nType !== 1 || !jQuery.isXMLDoc(elem);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (notxml) {[m[41m[m
[32m+[m[32m            // Fix name and attach hooks[m[41m[m
[32m+[m[32m            name = jQuery.propFix[name] || name;[m[41m[m
[32m+[m[32m            hooks = jQuery.propHooks[name];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (value !== undefined) {[m[41m[m
[32m+[m[32m            if (hooks && "set" in hooks && (ret = hooks.set(elem, value, name)) !== undefined) {[m[41m[m
[32m+[m[32m                return ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                return (elem[name] = value);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            if (hooks && "get" in hooks && (ret = hooks.get(elem, name)) !== null) {[m[41m[m
[32m+[m[32m                return ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                return elem[name];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.propFix = {[m[41m[m
[32m+[m[32m        "tabindex": 'tabIndex',[m[41m[m
[32m+[m[32m        "readonly": 'readOnly',[m[41m[m
[32m+[m[32m        "for": 'htmlFor',[m[41m[m
[32m+[m[32m        "class": 'className',[m[41m[m
[32m+[m[32m        "maxlength": 'maxLength',[m[41m[m
[32m+[m[32m        "cellspacing": 'cellSpacing',[m[41m[m
[32m+[m[32m        "cellpadding": 'cellPadding',[m[41m[m
[32m+[m[32m        "rowspan": 'rowSpan',[m[41m[m
[32m+[m[32m        "colspan": 'colSpan',[m[41m[m
[32m+[m[32m        "usemap": 'useMap',[m[41m[m
[32m+[m[32m        "frameborder": 'frameBorder',[m[41m[m
[32m+[m[32m        "contenteditable": 'contentEditable'[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.propHooks = {[m[41m[m
[32m+[m[32m        "tabIndex": {},[m[41m[m
[32m+[m[32m        "selected": {}[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.proxy = function (fn, context) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Takes a function and returns a new one that will always have a particular context.[m[41m[m
[32m+[m[32m        ///     &#10;1 - jQuery.proxy(function, context)[m[41m [m
[32m+[m[32m        ///     &#10;2 - jQuery.proxy(context, name)[m[41m [m
[32m+[m[32m        ///     &#10;3 - jQuery.proxy(function, context, additionalArguments)[m[41m [m
[32m+[m[32m        ///     &#10;4 - jQuery.proxy(context, name, additionalArguments)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     The function whose context will be changed.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="context" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     The object to which the context (this) of the function should be set.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="" type="Anything">[m[41m[m
[32m+[m[32m        ///     Any number of arguments to be passed to the function referenced in the function argument.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Function" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var args, proxy, tmp;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (typeof context === "string") {[m[41m[m
[32m+[m[32m            tmp = fn[context];[m[41m[m
[32m+[m[32m            context = fn;[m[41m[m
[32m+[m[32m            fn = tmp;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Quick check to determine if target is callable, in the spec[m[41m[m
[32m+[m[32m        // this throws a TypeError, but we will just return undefined.[m[41m[m
[32m+[m[32m        if (!jQuery.isFunction(fn)) {[m[41m[m
[32m+[m[32m            return undefined;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Simulated bind[m[41m[m
[32m+[m[32m        args = core_slice.call(arguments, 2);[m[41m[m
[32m+[m[32m        proxy = function () {[m[41m[m
[32m+[m[32m            return fn.apply(context || this, args.concat(core_slice.call(arguments)));[m[41m[m
[32m+[m[32m        };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Set the guid of unique handler to the same of original handler, so it can be removed[m[41m[m
[32m+[m[32m        proxy.guid = fn.guid = fn.guid || jQuery.guid++;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return proxy;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.queue = function (elem, type, data) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Show the queue of functions to be executed on the matched element.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - jQuery.queue(element, queueName)[m[41m[m
[32m+[m[32m        ///     &#10;2: Manipulate the queue of functions to be executed on the matched element.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - jQuery.queue(element, queueName, newQueue)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - jQuery.queue(element, queueName, callback())[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elem" domElement="true">[m[41m[m
[32m+[m[32m        ///     A DOM element where the array of queued functions is attached.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="type" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the name of the queue. Defaults to fx, the standard effects queue.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Array">[m[41m[m
[32m+[m[32m        ///     An array of functions to replace the current queue contents.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var queue;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (elem) {[m[41m[m
[32m+[m[32m            type = (type || "fx") + "queue";[m[41m[m
[32m+[m[32m            queue = jQuery._data(elem, type);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Speed up dequeue by getting out quickly if this is just a lookup[m[41m[m
[32m+[m[32m            if (data) {[m[41m[m
[32m+[m[32m                if (!queue || jQuery.isArray(data)) {[m[41m[m
[32m+[m[32m                    queue = jQuery._data(elem, type, jQuery.makeArray(data));[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    queue.push(data);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return queue || [];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.ready = function (wait) {[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Abort if there are pending holds or we're already ready[m[41m[m
[32m+[m[32m        if (wait === true ? --jQuery.readyWait : jQuery.isReady) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Make sure body exists, at least, in case IE gets a little overzealous (ticket #5443).[m[41m[m
[32m+[m[32m        if (!document.body) {[m[41m[m
[32m+[m[32m            return setTimeout(jQuery.ready);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Remember that the DOM is ready[m[41m[m
[32m+[m[32m        jQuery.isReady = true;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If a normal DOM Ready event fired, decrement, and wait if need be[m[41m[m
[32m+[m[32m        if (wait !== true && --jQuery.readyWait > 0) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If there are functions bound, to execute[m[41m[m
[32m+[m[32m        readyList.resolveWith(document, [jQuery]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Trigger any bound ready events[m[41m[m
[32m+[m[32m        if (jQuery.fn.trigger) {[m[41m[m
[32m+[m[32m            jQuery(document).trigger("ready").off("ready");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.readyWait = 0;[m[41m[m
[32m+[m[32m    jQuery.removeAttr = function (elem, value) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var name, propName,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mattrNames = value && value.match(core_rnotwhite);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (attrNames && elem.nodeType === 1) {[m[41m[m
[32m+[m[32m            while ((name = attrNames[i++])) {[m[41m[m
[32m+[m[32m                propName = jQuery.propFix[name] || name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Boolean attributes get special treatment (#10870)[m[41m[m
[32m+[m[32m                if (rboolean.test(name)) {[m[41m[m
[32m+[m[32m                    // Set corresponding property to false for boolean attributes[m[41m[m
[32m+[m[32m                    // Also clear defaultChecked/defaultSelected (if appropriate) for IE<8[m[41m[m
[32m+[m[32m                    if (!getSetAttribute && ruseDefault.test(name)) {[m[41m[m
[32m+[m[32m                        elem[jQuery.camelCase("default-" + name)] =[m[41m[m
[32m+[m							[32melem[propName] = false;[m[41m[m
[32m+[m[32m                    } else {[m[41m[m
[32m+[m[32m                        elem[propName] = false;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // See #9699 for explanation of this approach (setting first, then removal)[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    jQuery.attr(elem, name, "");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                elem.removeAttribute(getSetAttribute ? name : propName);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.removeData = function (elem, name) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Remove a previously-stored piece of data.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elem" domElement="true">[m[41m[m
[32m+[m[32m        ///     A DOM element from which to remove data.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="name" type="String">[m[41m[m
[32m+[m[32m        ///     A string naming the piece of data to remove.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return internalRemoveData(elem, name);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.removeEvent = function (elem, type, handle) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (elem.removeEventListener) {[m[41m[m
[32m+[m[32m            elem.removeEventListener(type, handle, false);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.sibling = function (n, elem) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var r = [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; n; n = n.nextSibling) {[m[41m[m
[32m+[m[32m            if (n.nodeType === 1 && n !== elem) {[m[41m[m
[32m+[m[32m                r.push(n);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return r;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.speed = function (speed, easing, fn) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var opt = speed && typeof speed === "object" ? jQuery.extend({}, speed) : {[m[41m[m
[32m+[m[32m            complete: fn || !fn && easing ||[m[41m[m
[32m+[m[32m                jQuery.isFunction(speed) && speed,[m[41m[m
[32m+[m[32m            duration: speed,[m[41m[m
[32m+[m[32m            easing: fn && easing || easing && !jQuery.isFunction(easing) && easing[m[41m[m
[32m+[m[32m        };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        opt.duration = jQuery.fx.off ? 0 : typeof opt.duration === "number" ? opt.duration :[m[41m[m
[32m+[m[32m            opt.duration in jQuery.fx.speeds ? jQuery.fx.speeds[opt.duration] : jQuery.fx.speeds._default;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // normalize opt.queue - true/undefined/null -> "fx"[m[41m[m
[32m+[m[32m        if (opt.queue == null || opt.queue === true) {[m[41m[m
[32m+[m[32m            opt.queue = "fx";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Queueing[m[41m[m
[32m+[m[32m        opt.old = opt.complete;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        opt.complete = function () {[m[41m[m
[32m+[m[32m            if (jQuery.isFunction(opt.old)) {[m[41m[m
[32m+[m[32m                opt.old.call(this);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (opt.queue) {[m[41m[m
[32m+[m[32m                jQuery.dequeue(this, opt.queue);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return opt;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.style = function (elem, name, value, extra) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Don't set styles on text and comment nodes[m[41m[m
[32m+[m[32m        if (!elem || elem.nodeType === 3 || elem.nodeType === 8 || !elem.style) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Make sure that we're working with the right name[m[41m[m
[32m+[m[32m        var ret, type, hooks,[m[41m[m
[32m+[m			[32morigName = jQuery.camelCase(name),[m[41m[m
[32m+[m			[32mstyle = elem.style;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        name = jQuery.cssProps[origName] || (jQuery.cssProps[origName] = vendorPropName(style, origName));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // gets hook for the prefixed version[m[41m[m
[32m+[m[32m        // followed by the unprefixed version[m[41m[m
[32m+[m[32m        hooks = jQuery.cssHooks[name] || jQuery.cssHooks[origName];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Check if we're setting a value[m[41m[m
[32m+[m[32m        if (value !== undefined) {[m[41m[m
[32m+[m[32m            type = typeof value;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // convert relative number strings (+= or -=) to relative numbers. #7345[m[41m[m
[32m+[m[32m            if (type === "string" && (ret = rrelNum.exec(value))) {[m[41m[m
[32m+[m[32m                value = (ret[1] + 1) * ret[2] + parseFloat(jQuery.css(elem, name));[m[41m[m
[32m+[m[32m                // Fixes bug #9237[m[41m[m
[32m+[m[32m                type = "number";[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Make sure that NaN and null values aren't set. See: #7116[m[41m[m
[32m+[m[32m            if (value == null || type === "number" && isNaN(value)) {[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // If a number was passed in, add 'px' to the (except for certain CSS properties)[m[41m[m
[32m+[m[32m            if (type === "number" && !jQuery.cssNumber[origName]) {[m[41m[m
[32m+[m[32m                value += "px";[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Fixes #8908, it can be done more correctly by specifing setters in cssHooks,[m[41m[m
[32m+[m[32m            // but it would mean to define eight (for every problematic property) identical functions[m[41m[m
[32m+[m[32m            if (!jQuery.support.clearCloneStyle && value === "" && name.indexOf("background") === 0) {[m[41m[m
[32m+[m[32m                style[name] = "inherit";[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // If a hook was provided, use that value, otherwise just set the specified value[m[41m[m
[32m+[m[32m            if (!hooks || !("set" in hooks) || (value = hooks.set(elem, value, extra)) !== undefined) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Wrapped to prevent IE from throwing errors when 'invalid' values are provided[m[41m[m
[32m+[m[32m                // Fixes bug #5509[m[41m[m
[32m+[m[32m                try {[m[41m[m
[32m+[m[32m                    style[name] = value;[m[41m[m
[32m+[m[32m                } catch (e) { }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            // If a hook was provided get the non-computed value from there[m[41m[m
[32m+[m[32m            if (hooks && "get" in hooks && (ret = hooks.get(elem, false, extra)) !== undefined) {[m[41m[m
[32m+[m[32m                return ret;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Otherwise just get the value from the style object[m[41m[m
[32m+[m[32m            return style[name];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.support = {[m[41m[m
