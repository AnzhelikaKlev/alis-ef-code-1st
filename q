[32m+[m[32m    jQuery.prototype.get = function (num) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Retrieve the DOM elements matched by the jQuery object.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="num" type="Number">[m[41m[m
[32m+[m[32m        ///     A zero-based integer indicating which element to retrieve.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Array" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return num == null ?[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// Return a 'clean' array[m[41m[m
[32m+[m			[32mthis.toArray() :[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// Return just the object[m[41m[m
[32m+[m			[32m(num < 0 ? this[this.length + num] : this[num]);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.has = function (target) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Reduce the set of matched elements to those that have a descendant that matches the selector or DOM element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - has(selector)[m[41m [m
[32m+[m[32m        ///     &#10;2 - has(contained)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="target" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var i,[m[41m[m
[32m+[m			[32mtargets = jQuery(target, this),[m[41m[m
[32m+[m			[32mlen = targets.length;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.filter(function () {[m[41m[m
[32m+[m[32m            for (i = 0; i < len; i++) {[m[41m[m
[32m+[m[32m                if (jQuery.contains(this, targets[i])) {[m[41m[m
[32m+[m[32m                    return true;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.hasClass = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Determine whether any of the matched elements are assigned the given class.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     The class name to search for.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var className = " " + selector + " ",[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32ml = this.length;[m[41m[m
[32m+[m[32m        for (; i < l; i++) {[m[41m[m
[32m+[m[32m            if (this[i].nodeType === 1 && (" " + this[i].className + " ").replace(rclass, " ").indexOf(className) >= 0) {[m[41m[m
[32m+[m[32m                return true;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return false;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.height = function (margin, value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Get the current computed height for the first element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - height()[m[41m[m
[32m+[m[32m        ///     &#10;2: Set the CSS height of every matched element.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - height(value)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - height(function(index, height))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="margin" type="Number">[m[41m[m
[32m+[m[32m        ///     An integer representing the number of pixels, or an integer with an optional unit of measure appended (as a string).[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var chainable = arguments.length && (defaultExtra || typeof margin !== "boolean"),[m[41m[m
[32m+[m[32m            extra = defaultExtra || (margin === true || value === true ? "margin" : "border");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, function (elem, type, value) {[m[41m[m
[32m+[m[32m            var doc;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (jQuery.isWindow(elem)) {[m[41m[m
[32m+[m[32m                // As of 5/8/2012 this will yield incorrect results for Mobile Safari, but there[m[41m[m
[32m+[m[32m                // isn't a whole lot we can do. See pull request at this URL for discussion:[m[41m[m
[32m+[m[32m                // https://github.com/jquery/jquery/pull/764[m[41m[m
[32m+[m[32m                return elem.document.documentElement["client" + name];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Get document width or height[m[41m[m
[32m+[m[32m            if (elem.nodeType === 9) {[m[41m[m
[32m+[m[32m                doc = elem.documentElement;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Either scroll[Width/Height] or offset[Width/Height] or client[Width/Height], whichever is greatest[m[41m[m
[32m+[m[32m                // unfortunately, this causes bug #3838 in IE6/8 only, but there is currently no good, small way to fix it.[m[41m[m
[32m+[m[32m                return Math.max([m[41m[m
[32m+[m[32m                    elem.body["scroll" + name], doc["scroll" + name],[m[41m[m
[32m+[m[32m                    elem.body["offset" + name], doc["offset" + name],[m[41m[m
[32m+[m[32m                    doc["client" + name][m[41m[m
[32m+[m[32m                );[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return value === undefined ?[m[41m[m
[32m+[m[32m                // Get width or height on the element, requesting but not forcing parseFloat[m[41m[m
[32m+[m[32m                jQuery.css(elem, type, extra) :[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Set width or height on the element[m[41m[m
[32m+[m[32m                jQuery.style(elem, type, value, extra);[m[41m[m
[32m+[m[32m        }, type, chainable ? margin : undefined, chainable, null);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.hide = function (speed, easing, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Hide the matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - hide()[m[41m [m
[32m+[m[32m        ///     &#10;2 - hide(duration, complete)[m[41m [m
[32m+[m[32m        ///     &#10;3 - hide(options)[m[41m [m
[32m+[m[32m        ///     &#10;4 - hide(duration, easing, complete)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="speed" type="">[m[41m[m
[32m+[m[32m        ///     A string or number determining how long the animation will run.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="easing" type="String">[m[41m[m
[32m+[m[32m        ///     A string indicating which easing function to use for the transition.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to call once the animation is complete.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return speed == null || typeof speed === "boolean" ?[m[41m[m
[32m+[m			[32mcssFn.apply(this, arguments) :[m[41m[m
[32m+[m			[32mthis.animate(genFx(name, true), speed, easing, callback);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.hover = function (fnOver, fnOut) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Bind two handlers to the matched elements, to be executed when the mouse pointer enters and leaves the elements.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - hover(handlerIn(eventObject), handlerOut(eventObject))[m[41m[m
[32m+[m[32m        ///     &#10;2: Bind a single handler to the matched elements, to be executed when the mouse pointer enters or leaves the elements.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - hover(handlerInOut(eventObject))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="fnOver" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute when the mouse pointer enters the element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fnOut" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute when the mouse pointer leaves the element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.mouseenter(fnOver).mouseleave(fnOut || fnOver);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.html = function (value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Get the HTML contents of the first element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - html()[m[41m[m
[32m+[m[32m        ///     &#10;2: Set the HTML contents of each element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - html(htmlString)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - html(function(index, oldhtml))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="value" type="String">[m[41m[m
[32m+[m[32m        ///     A string of HTML to set as the content of each matched element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, function (value) {[m[41m[m
[32m+[m[32m            var elem = this[0] || {},[m[41m[m
[32m+[m				[32mi = 0,[m[41m[m
[32m+[m				[32ml = this.length;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (value === undefined) {[m[41m[m
[32m+[m[32m                return elem.nodeType === 1 ?[m[41m[m
[32m+[m					[32melem.innerHTML.replace(rinlinejQuery, "") :[m[41m[m
[32m+[m					[32mundefined;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // See if we can take a shortcut and just use innerHTML[m[41m[m
[32m+[m[32m            if (typeof value === "string" && !rnoInnerhtml.test(value) &&[m[41m[m
[32m+[m				[32m(jQuery.support.htmlSerialize || !rnoshimcache.test(value)) &&[m[41m[m
[32m+[m				[32m(jQuery.support.leadingWhitespace || !rleadingWhitespace.test(value)) &&[m[41m[m
[32m+[m				[32m!wrapMap[(rtagName.exec(value) || ["", ""])[1].toLowerCase()]) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                value = value.replace(rxhtmlTag, "<$1></$2>");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                try {[m[41m[m
[32m+[m[32m                    for (; i < l; i++) {[m[41m[m
[32m+[m[32m                        // Remove element nodes and prevent memory leaks[m[41m[m
[32m+[m[32m                        elem = this[i] || {};[m[41m[m
[32m+[m[32m                        if (elem.nodeType === 1) {[m[41m[m
[32m+[m[32m                            jQuery.cleanData(getAll(elem, false));[m[41m[m
[32m+[m[32m                            elem.innerHTML = value;[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    elem = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // If using innerHTML throws an exception, use the fallback method[m[41m[m
[32m+[m[32m                } catch (e) { }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (elem) {[m[41m[m
[32m+[m[32m                this.empty().append(value);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }, null, value, arguments.length);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.index = function (elem) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Search for a given element from among the matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - index()[m[41m [m
[32m+[m[32m        ///     &#10;2 - index(selector)[m[41m [m
[32m+[m[32m        ///     &#10;3 - index(element)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elem" type="String">[m[41m[m
[32m+[m[32m        ///     A selector representing a jQuery collection in which to look for an element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Number" />[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // No argument, return index in parent[m[41m[m
[32m+[m[32m        if (!elem) {[m[41m[m
[32m+[m[32m            return (this[0] && this[0].parentNode) ? this.first().prevAll().length : -1;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // index in selector[m[41m[m
[32m+[m[32m        if (typeof elem === "string") {[m[41m[m
[32m+[m[32m            return jQuery.inArray(this[0], jQuery(elem));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Locate the position of the desired element[m[41m[m
[32m+[m[32m        return jQuery.inArray([m[41m[m
[32m+[m			[32m// If it receives a jQuery object, the first element is used[m[41m[m
[32m+[m			[32melem.jquery ? elem[0] : elem, this);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.init = function (selector, context, rootjQuery) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var match, elem;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // HANDLE: $(""), $(null), $(undefined), $(false)[m[41m[m
[32m+[m[32m        if (!selector) {[m[41m[m
[32m+[m[32m            return this;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Handle HTML strings[m[41m[m
[32m+[m[32m        if (typeof selector === "string") {[m[41m[m
[32m+[m[32m            if (selector.charAt(0) === "<" && selector.charAt(selector.length - 1) === ">" && selector.length >= 3) {[m[41m[m
[32m+[m[32m                // Assume that strings that start and end with <> are HTML and skip the regex check[m[41m[m
[32m+[m[32m                match = [null, selector, null];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                match = rquickExpr.exec(selector);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Match html or make sure no context is specified for #id[m[41m[m
[32m+[m[32m            if (match && (match[1] || !context)) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // HANDLE: $(html) -> $(array)[m[41m[m
[32m+[m[32m                if (match[1]) {[m[41m[m
[32m+[m[32m                    context = context instanceof jQuery ? context[0] : context;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // scripts is true for back-compat[m[41m[m
[32m+[m[32m                    jQuery.merge(this, jQuery.parseHTML([m[41m[m
[32m+[m						[32mmatch[1],[m[41m[m
[32m+[m						[32mcontext && context.nodeType ? context.ownerDocument || context : document,[m[41m[m
[32m+[m						[32mtrue[m[41m[m
[32m+[m					[32m));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // HANDLE: $(html, props)[m[41m[m
[32m+[m[32m                    if (rsingleTag.test(match[1]) && jQuery.isPlainObject(context)) {[m[41m[m
[32m+[m[32m                        for (match in context) {[m[41m[m
[32m+[m[32m                            // Properties of context are called as methods if possible[m[41m[m
[32m+[m[32m                            if (jQuery.isFunction(this[match])) {[m[41m[m
[32m+[m[32m                                this[match](context[match]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                                // ...and otherwise set as attributes[m[41m[m
[32m+[m[32m                            } else {[m[41m[m
[32m+[m[32m                                this.attr(match, context[match]);[m[41m[m
[32m+[m[32m                            }[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // HANDLE: $(#id)[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    elem = document.getElementById(match[2]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Check parentNode to catch when Blackberry 4.6 returns[m[41m[m
[32m+[m[32m                    // nodes that are no longer in the document #6963[m[41m[m
[32m+[m[32m                    if (elem && elem.parentNode) {[m[41m[m
[32m+[m[32m                        // Handle the case where IE and Opera return items[m[41m[m
[32m+[m[32m                        // by name instead of ID[m[41m[m
[32m+[m[32m                        if (elem.id !== match[2]) {[m[41m[m
[32m+[m[32m                            return rootjQuery.find(selector);[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        // Otherwise, we inject the element directly into the jQuery object[m[41m[m
[32m+[m[32m                        this.length = 1;[m[41m[m
[32m+[m[32m                        this[0] = elem;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    this.context = document;[m[41m[m
[32m+[m[32m                    this.selector = selector;[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // HANDLE: $(expr, $(...))[m[41m[m
[32m+[m[32m            } else if (!context || context.jquery) {[m[41m[m
[32m+[m[32m                return (context || rootjQuery).find(selector);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // HANDLE: $(expr, context)[m[41m[m
[32m+[m[32m                // (which is just equivalent to: $(context).find(expr)[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                return this.constructor(context).find(selector);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // HANDLE: $(DOMElement)[m[41m[m
[32m+[m[32m        } else if (selector.nodeType) {[m[41m[m
[32m+[m[32m            this.context = this[0] = selector;[m[41m[m
[32m+[m[32m            this.length = 1;[m[41m[m
[32m+[m[32m            return this;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // HANDLE: $(function)[m[41m[m
[32m+[m[32m            // Shortcut for document ready[m[41m[m
[32m+[m[32m        } else if (jQuery.isFunction(selector)) {[m[41m[m
[32m+[m[32m            return rootjQuery.ready(selector);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (selector.selector !== undefined) {[m[41m[m
[32m+[m[32m            this.selector = selector.selector;[m[41m[m
[32m+[m[32m            this.context = selector.context;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.makeArray(selector, this);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.innerHeight = function (margin, value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the current computed height for the first element in the set of matched elements, including padding but not border.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="Number" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var chainable = arguments.length && (defaultExtra || typeof margin !== "boolean"),[m[41m[m
[32m+[m[32m            extra = defaultExtra || (margin === true || value === true ? "margin" : "border");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, function (elem, type, value) {[m[41m[m
[32m+[m[32m            var doc;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (jQuery.isWindow(elem)) {[m[41m[m
[32m+[m[32m                // As of 5/8/2012 this will yield incorrect results for Mobile Safari, but there[m[41m[m
[32m+[m[32m                // isn't a whole lot we can do. See pull request at this URL for discussion:[m[41m[m
[32m+[m[32m                // https://github.com/jquery/jquery/pull/764[m[41m[m
[32m+[m[32m                return elem.document.documentElement["client" + name];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Get document width or height[m[41m[m
[32m+[m[32m            if (elem.nodeType === 9) {[m[41m[m
[32m+[m[32m                doc = elem.documentElement;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Either scroll[Width/Height] or offset[Width/Height] or client[Width/Height], whichever is greatest[m[41m[m
[32m+[m[32m                // unfortunately, this causes bug #3838 in IE6/8 only, but there is currently no good, small way to fix it.[m[41m[m
[32m+[m[32m                return Math.max([m[41m[m
[32m+[m[32m                    elem.body["scroll" + name], doc["scroll" + name],[m[41m[m
[32m+[m[32m                    elem.body["offset" + name], doc["offset" + name],[m[41m[m
[32m+[m[32m                    doc["client" + name][m[41m[m
[32m+[m[32m                );[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return value === undefined ?[m[41m[m
[32m+[m[32m                // Get width or height on the element, requesting but not forcing parseFloat[m[41m[m
[32m+[m[32m                jQuery.css(elem, type, extra) :[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Set width or height on the element[m[41m[m
[32m+[m[32m                jQuery.style(elem, type, value, extra);[m[41m[m
[32m+[m[32m        }, type, chainable ? margin : undefined, chainable, null);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.innerWidth = function (margin, value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the current computed width for the first element in the set of matched elements, including padding but not border.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="Number" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var chainable = arguments.length && (defaultExtra || typeof margin !== "boolean"),[m[41m[m
[32m+[m[32m            extra = defaultExtra || (margin === true || value === true ? "margin" : "border");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, function (elem, type, value) {[m[41m[m
[32m+[m[32m            var doc;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (jQuery.isWindow(elem)) {[m[41m[m
[32m+[m[32m                // As of 5/8/2012 this will yield incorrect results for Mobile Safari, but there[m[41m[m
[32m+[m[32m                // isn't a whole lot we can do. See pull request at this URL for discussion:[m[41m[m
[32m+[m[32m                // https://github.com/jquery/jquery/pull/764[m[41m[m
[32m+[m[32m                return elem.document.documentElement["client" + name];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Get document width or height[m[41m[m
[32m+[m[32m            if (elem.nodeType === 9) {[m[41m[m
[32m+[m[32m                doc = elem.documentElement;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Either scroll[Width/Height] or offset[Width/Height] or client[Width/Height], whichever is greatest[m[41m[m
[32m+[m[32m                // unfortunately, this causes bug #3838 in IE6/8 only, but there is currently no good, small way to fix it.[m[41m[m
[32m+[m[32m                return Math.max([m[41m[m
[32m+[m[32m                    elem.body["scroll" + name], doc["scroll" + name],[m[41m[m
[32m+[m[32m                    elem.body["offset" + name], doc["offset" + name],[m[41m[m
[32m+[m[32m                    doc["client" + name][m[41m[m
[32m+[m[32m                );[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return value === undefined ?[m[41m[m
[32m+[m[32m                // Get width or height on the element, requesting but not forcing parseFloat[m[41m[m
[32m+[m[32m                jQuery.css(elem, type, extra) :[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Set width or height on the element[m[41m[m
[32m+[m[32m                jQuery.style(elem, type, value, extra);[m[41m[m
[32m+[m[32m        }, type, chainable ? margin : undefined, chainable, null);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.insertAfter = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Insert every element in the set of matched elements after the target.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="jQuery">[m[41m[m
[32m+[m[32m        ///     A selector, element, HTML string, or jQuery object; the matched set of elements will be inserted after the element(s) specified by this parameter.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var elems,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mret = [],[m[41m[m
[32m+[m			[32minsert = jQuery(selector),[m[41m[m
[32m+[m			[32mlast = insert.length - 1;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; i <= last; i++) {[m[41m[m
[32m+[m[32m            elems = i === last ? this : this.clone(true);[m[41m[m
[32m+[m[32m            jQuery(insert[i])[original](elems);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Modern browsers can apply jQuery collections as arrays, but oldIE needs a .get()[m[41m[m
[32m+[m[32m            core_push.apply(ret, elems.get());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.insertBefore = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Insert every element in the set of matched elements before the target.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="jQuery">[m[41m[m
[32m+[m[32m        ///     A selector, element, HTML string, or jQuery object; the matched set of elements will be inserted before the element(s) specified by this parameter.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var elems,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mret = [],[m[41m[m
[32m+[m			[32minsert = jQuery(selector),[m[41m[m
[32m+[m			[32mlast = insert.length - 1;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; i <= last; i++) {[m[41m[m
[32m+[m[32m            elems = i === last ? this : this.clone(true);[m[41m[m
[32m+[m[32m            jQuery(insert[i])[original](elems);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Modern browsers can apply jQuery collections as arrays, but oldIE needs a .get()[m[41m[m
[32m+[m[32m            core_push.apply(ret, elems.get());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.is = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Check the current matched set of elements against a selector, element, or jQuery object and return true if at least one of these elements matches the given arguments.[m[41m[m
[32m+[m[32m        ///     &#10;1 - is(selector)[m[41m [m
[32m+[m[32m        ///     &#10;2 - is(function(index))[m[41m [m
[32m+[m[32m        ///     &#10;3 - is(jQuery object)[m[41m [m
[32m+[m[32m        ///     &#10;4 - is(element)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return !!selector && ([m[41m[m
[32m+[m			[32mtypeof selector === "string" ?[m[41m[m
[32m+[m				[32m// If this is a positional/relative selector, check membership in the returned set[m[41m[m
[32m+[m				[32m// so $("p:first").is("p:last") won't return true for a doc with two "p".[m[41m[m
[32m+[m				[32mrneedsContext.test(selector) ?[m[41m[m
[32m+[m					[32mjQuery(selector, this.context).index(this[0]) >= 0 :[m[41m[m
[32m+[m					[32mjQuery.filter(selector, this).length > 0 :[m[41m[m
[32m+[m				[32mthis.filter(selector).length > 0);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.keydown = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Bind an event handler to the "keydown" JavaScript event, or trigger that event on an element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - keydown(handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - keydown(eventData, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;3 - keydown()[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute each time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return arguments.length > 0 ?[m[41m[m
[32m+[m			[32mthis.on(name, null, data, fn) :[m[41m[m
[32m+[m			[32mthis.trigger(name);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.keypress = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Bind an event handler to the "keypress" JavaScript event, or trigger that event on an element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - keypress(handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - keypress(eventData, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;3 - keypress()[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute each time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return arguments.length > 0 ?[m[41m[m
[32m+[m			[32mthis.on(name, null, data, fn) :[m[41m[m
[32m+[m			[32mthis.trigger(name);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.keyup = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Bind an event handler to the "keyup" JavaScript event, or trigger that event on an element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - keyup(handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - keyup(eventData, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;3 - keyup()[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute each time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return arguments.length > 0 ?[m[41m[m
[32m+[m			[32mthis.on(name, null, data, fn) :[m[41m[m
[32m+[m			[32mthis.trigger(name);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.last = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Reduce the set of matched elements to the final one in the set.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.eq(-1);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.length = 0;[m[41m[m
[32m+[m[32m    jQuery.prototype.load = function (url, params, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Bind an event handler to the "load" JavaScript event.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - load(handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;    1.2 - load(eventData, handler(eventObject))[m[41m[m
[32m+[m[32m        ///     &#10;2: Load data from the server and place the returned HTML into the matched element.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - load(url, data, complete(responseText, textStatus, XMLHttpRequest))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="url" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the URL to which the request is sent.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="params" type="String">[m[41m[m
[32m+[m[32m        ///     A plain object or string that is sent to the server with the request.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A callback function that is executed when the request completes.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (typeof url !== "string" && _load) {[m[41m[m
[32m+[m[32m            return _load.apply(this, arguments);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var selector, response, type,[m[41m[m
[32m+[m[32m            self = this,[m[41m[m
[32m+[m[32m            off = url.indexOf(" ");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (off >= 0) {[m[41m[m
[32m+[m[32m            selector = url.slice(off, url.length);[m[41m[m
[32m+[m[32m            url = url.slice(0, off);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If it's a function[m[41m[m
[32m+[m[32m        if (jQuery.isFunction(params)) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // We assume that it's the callback[m[41m[m
[32m+[m[32m            callback = params;[m[41m[m
[32m+[m[32m            params = undefined;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Otherwise, build a param string[m[41m[m
[32m+[m[32m        } else if (params && typeof params === "object") {[m[41m[m
[32m+[m[32m            type = "POST";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If we have elements to modify, make the request[m[41m[m
[32m+[m[32m        if (self.length > 0) {[m[41m[m
[32m+[m[32m            jQuery.ajax({[m[41m[m
[32m+[m[32m                url: url,[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // if "type" variable is undefined, then "GET" method will be used[m[41m[m
[32m+[m[32m                type: type,[m[41m[m
[32m+[m[32m                dataType: "html",[m[41m[m
[32m+[m[32m                data: params[m[41m[m
[32m+[m[32m            }).done(function (responseText) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Save response for use in complete callback[m[41m[m
[32m+[m[32m                response = arguments;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                self.html(selector ?[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // If a selector was specified, locate the right elements in a dummy div[m[41m[m
[32m+[m[32m                    // Exclude scripts to avoid IE 'Permission Denied' errors[m[41m[m
[32m+[m[32m                    jQuery("<div>").append(jQuery.parseHTML(responseText)).find(selector) :[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Otherwise use the full result[m[41m[m
[32m+[m[32m                    responseText);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }).complete(callback && function (jqXHR, status) {[m[41m[m
[32m+[m[32m                self.each(callback, response || [jqXHR.responseText, status, jqXHR]);[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.map = function (callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Pass each element in the current matched set through a function, producing a new jQuery object containing the return values.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A function object that will be invoked for each element in the current set.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(jQuery.map(this, function (elem, i) {[m[41m[m
[32m+[m[32m            return callback.call(elem, i, elem);[m[41m[m
[32m+[m[32m        }));[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.mousedown = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Bind an event handler to the "mousedown" JavaScript event, or trigger that event on an element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - mousedown(handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - mousedown(eventData, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;3 - mousedown()[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute each time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return arguments.length > 0 ?[m[41m[m
[32m+[m			[32mthis.on(name, null, data, fn) :[m[41m[m
[32m+[m			[32mthis.trigger(name);[m[41m[m
[32m+[m[32m    };[m[41m[m
