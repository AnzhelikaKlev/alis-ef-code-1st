[32m+[m[32m    jQuery.prototype.css = function (name, value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Get the value of style properties for the first element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - css(propertyName)[m[41m [m
[32m+[m[32m        ///     &#10;    1.2 - css(propertyNames)[m[41m[m
[32m+[m[32m        ///     &#10;2: Set one or more CSS properties for the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - css(propertyName, value)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - css(propertyName, function(index, value))[m[41m [m
[32m+[m[32m        ///     &#10;    2.3 - css(properties)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="name" type="String">[m[41m[m
[32m+[m[32m        ///     A CSS property name.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="value" type="Number">[m[41m[m
[32m+[m[32m        ///     A value to set for the property.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, function (elem, name, value) {[m[41m[m
[32m+[m[32m            var len, styles,[m[41m[m
[32m+[m				[32mmap = {},[m[41m[m
[32m+[m				[32mi = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (jQuery.isArray(name)) {[m[41m[m
[32m+[m[32m                styles = getStyles(elem);[m[41m[m
[32m+[m[32m                len = name.length;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                for (; i < len; i++) {[m[41m[m
[32m+[m[32m                    map[name[i]] = jQuery.css(elem, name[i], false, styles);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return map;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return value !== undefined ?[m[41m[m
[32m+[m				[32mjQuery.style(elem, name, value) :[m[41m[m
[32m+[m				[32mjQuery.css(elem, name);[m[41m[m
[32m+[m[32m        }, name, value, arguments.length > 1);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.data = function (key, value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Store arbitrary data associated with the matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - data(key, value)[m[41m [m
[32m+[m[32m        ///     &#10;    1.2 - data(obj)[m[41m[m
[32m+[m[32m        ///     &#10;2: Return the value at the named data store for the first element in the jQuery collection, as set by data(name, value) or by an HTML5 data-* attribute.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - data(key)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - data()[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="key" type="String">[m[41m[m
[32m+[m[32m        ///     A string naming the piece of data to set.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="value" type="Object">[m[41m[m
[32m+[m[32m        ///     The new data value; it can be any Javascript type including Array or Object.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var attrs, name,[m[41m[m
[32m+[m			[32melem = this[0],[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mdata = null;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Gets all values[m[41m[m
[32m+[m[32m        if (key === undefined) {[m[41m[m
[32m+[m[32m            if (this.length) {[m[41m[m
[32m+[m[32m                data = jQuery.data(elem);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (elem.nodeType === 1 && !jQuery._data(elem, "parsedAttrs")) {[m[41m[m
[32m+[m[32m                    attrs = elem.attributes;[m[41m[m
[32m+[m[32m                    for (; i < attrs.length; i++) {[m[41m[m
[32m+[m[32m                        name = attrs[i].name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        if (!name.indexOf("data-")) {[m[41m[m
[32m+[m[32m                            name = jQuery.camelCase(name.slice(5));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                            dataAttr(elem, name, data[name]);[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    jQuery._data(elem, "parsedAttrs", true);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return data;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Sets multiple values[m[41m[m
[32m+[m[32m        if (typeof key === "object") {[m[41m[m
[32m+[m[32m            return this.each(function () {[m[41m[m
[32m+[m[32m                jQuery.data(this, key);[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, function (value) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (value === undefined) {[m[41m[m
[32m+[m[32m                // Try to fetch any internally stored data first[m[41m[m
[32m+[m[32m                return elem ? dataAttr(elem, key, jQuery.data(elem, key)) : null;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            this.each(function () {[m[41m[m
[32m+[m[32m                jQuery.data(this, key, value);[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[32m        }, null, value, arguments.length > 1, null, true);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.dblclick = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Bind an event handler to the "dblclick" JavaScript event, or trigger that event on an element.[m[41m[m
[32m+[m[32m        ///     &#10;1 - dblclick(handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - dblclick(eventData, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;3 - dblclick()[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Object">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute each time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return arguments.length > 0 ?[m[41m[m
[32m+[m			[32mthis.on(name, null, data, fn) :[m[41m[m
[32m+[m			[32mthis.trigger(name);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.delay = function (time, type) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Set a timer to delay execution of subsequent items in the queue.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="time" type="Number">[m[41m[m
[32m+[m[32m        ///     An integer indicating the number of milliseconds to delay execution of the next item in the queue.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="type" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the name of the queue. Defaults to fx, the standard effects queue.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        time = jQuery.fx ? jQuery.fx.speeds[time] || time : time;[m[41m[m
[32m+[m[32m        type = type || "fx";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.queue(type, function (next, hooks) {[m[41m[m
[32m+[m[32m            var timeout = setTimeout(next, time);[m[41m[m
[32m+[m[32m            hooks.stop = function () {[m[41m[m
[32m+[m[32m                clearTimeout(timeout);[m[41m[m
[32m+[m[32m            };[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.delegate = function (selector, types, data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Attach a handler to one or more events for all elements that match the selector, now or in the future, based on a specific set of root elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - delegate(selector, eventType, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - delegate(selector, eventType, eventData, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;3 - delegate(selector, events)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A selector to filter the elements that trigger the event.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="types" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing one or more space-separated JavaScript event types, such as "click" or "keydown," or custom event names.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Object">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute at the time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.on(types, selector, data, fn);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.dequeue = function (type) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Execute the next function on the queue for the matched elements.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="type" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the name of the queue. Defaults to fx, the standard effects queue.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.each(function () {[m[41m[m
[32m+[m[32m            jQuery.dequeue(this, type);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.detach = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Remove the set of matched elements from the DOM.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A selector expression that filters the set of matched elements to be removed.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.remove(selector, true);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.domManip = function (args, table, callback) {[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Flatten any nested arrays[m[41m[m
[32m+[m[32m        args = core_concat.apply([], args);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var first, node, hasScripts,[m[41m[m
[32m+[m			[32mscripts, doc, fragment,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32ml = this.length,[m[41m[m
[32m+[m			[32mset = this,[m[41m[m
[32m+[m			[32miNoClone = l - 1,[m[41m[m
[32m+[m			[32mvalue = args[0],[m[41m[m
[32m+[m			[32misFunction = jQuery.isFunction(value);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // We can't cloneNode fragments that contain checked, in WebKit[m[41m[m
[32m+[m[32m        if (isFunction || !(l <= 1 || typeof value !== "string" || jQuery.support.checkClone || !rchecked.test(value))) {[m[41m[m
[32m+[m[32m            return this.each(function (index) {[m[41m[m
[32m+[m[32m                var self = set.eq(index);[m[41m[m
[32m+[m[32m                if (isFunction) {[m[41m[m
[32m+[m[32m                    args[0] = value.call(this, index, table ? self.html() : undefined);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                self.domManip(args, table, callback);[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (l) {[m[41m[m
[32m+[m[32m            fragment = jQuery.buildFragment(args, this[0].ownerDocument, false, this);[m[41m[m
[32m+[m[32m            first = fragment.firstChild;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (fragment.childNodes.length === 1) {[m[41m[m
[32m+[m[32m                fragment = first;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (first) {[m[41m[m
[32m+[m[32m                table = table && jQuery.nodeName(first, "tr");[m[41m[m
[32m+[m[32m                scripts = jQuery.map(getAll(fragment, "script"), disableScript);[m[41m[m
[32m+[m[32m                hasScripts = scripts.length;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Use the original fragment for the last item instead of the first because it can end up[m[41m[m
[32m+[m[32m                // being emptied incorrectly in certain situations (#8070).[m[41m[m
[32m+[m[32m                for (; i < l; i++) {[m[41m[m
[32m+[m[32m                    node = fragment;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if (i !== iNoClone) {[m[41m[m
[32m+[m[32m                        node = jQuery.clone(node, true, true);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        // Keep references to cloned scripts for later restoration[m[41m[m
[32m+[m[32m                        if (hasScripts) {[m[41m[m
[32m+[m[32m                            jQuery.merge(scripts, getAll(node, "script"));[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    callback.call([m[41m[m
[32m+[m						[32mtable && jQuery.nodeName(this[i], "table") ?[m[41m[m
[32m+[m							[32mfindOrAppend(this[i], "tbody") :[m[41m[m
[32m+[m							[32mthis[i],[m[41m[m
[32m+[m						[32mnode,[m[41m[m
[32m+[m						[32mi[m[41m[m
[32m+[m					[32m);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (hasScripts) {[m[41m[m
[32m+[m[32m                    doc = scripts[scripts.length - 1].ownerDocument;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Reenable scripts[m[41m[m
[32m+[m[32m                    jQuery.map(scripts, restoreScript);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Evaluate executable scripts on first document insertion[m[41m[m
[32m+[m[32m                    for (i = 0; i < hasScripts; i++) {[m[41m[m
[32m+[m[32m                        node = scripts[i];[m[41m[m
[32m+[m[32m                        if (rscriptType.test(node.type || "") &&[m[41m[m
[32m+[m							[32m!jQuery._data(node, "globalEval") && jQuery.contains(doc, node)) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                            if (node.src) {[m[41m[m
[32m+[m[32m                                // Hope ajax is available...[m[41m[m
[32m+[m[32m                                jQuery.ajax({[m[41m[m
[32m+[m[32m                                    url: node.src,[m[41m[m
[32m+[m[32m                                    type: "GET",[m[41m[m
[32m+[m[32m                                    dataType: "script",[m[41m[m
[32m+[m[32m                                    async: false,[m[41m[m
[32m+[m[32m                                    global: false,[m[41m[m
[32m+[m[32m                                    "throws": true[m[41m[m
[32m+[m[32m                                });[m[41m[m
[32m+[m[32m                            } else {[m[41m[m
[32m+[m[32m                                jQuery.globalEval((node.text || node.textContent || node.innerHTML || "").replace(rcleanScript, ""));[m[41m[m
[32m+[m[32m                            }[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Fix #11809: Avoid leaking memory[m[41m[m
[32m+[m[32m                fragment = first = null;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.each = function (callback, args) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Iterate over a jQuery object, executing a function for each matched element.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute for each matched element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.each(this, callback, args);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.empty = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Remove all child nodes of the set of matched elements from the DOM.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var elem,[m[41m[m
[32m+[m			[32mi = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; (elem = this[i]) != null; i++) {[m[41m[m
[32m+[m[32m            // Remove element nodes and prevent memory leaks[m[41m[m
[32m+[m[32m            if (elem.nodeType === 1) {[m[41m[m
[32m+[m[32m                jQuery.cleanData(getAll(elem, false));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Remove any remaining nodes[m[41m[m
[32m+[m[32m            while (elem.firstChild) {[m[41m[m
[32m+[m[32m                elem.removeChild(elem.firstChild);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // If this is a select, ensure that it displays empty (#12336)[m[41m[m
[32m+[m[32m            // Support: IE<9[m[41m[m
[32m+[m[32m            if (elem.options && jQuery.nodeName(elem, "select")) {[m[41m[m
[32m+[m[32m                elem.options.length = 0;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.end = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     End the most recent filtering operation in the current chain and return the set of matched elements to its previous state.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.prevObject || this.constructor(null);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.eq = function (i) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Reduce the set of matched elements to the one at the specified index.[m[41m[m
[32m+[m[32m        ///     &#10;1 - eq(index)[m[41m [m
[32m+[m[32m        ///     &#10;2 - eq(-index)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="i" type="Number">[m[41m[m
[32m+[m[32m        ///     An integer indicating the 0-based position of the element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var len = this.length,[m[41m[m
[32m+[m			[32mj = +i + (i < 0 ? len : 0);[m[41m[m
[32m+[m[32m        return this.pushStack(j >= 0 && j < len ? [this[j]] : []);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.error = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Bind an event handler to the "error" JavaScript event.[m[41m[m
[32m+[m[32m        ///     &#10;1 - error(handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - error(eventData, handler(eventObject))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Object">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute each time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return arguments.length > 0 ?[m[41m[m
[32m+[m			[32mthis.on(name, null, data, fn) :[m[41m[m
[32m+[m			[32mthis.trigger(name);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.extend = function () {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var src, copyIsArray, copy, name, options, clone,[m[41m[m
[32m+[m[32m            target = arguments[0] || {},[m[41m[m
[32m+[m[32m            i = 1,[m[41m[m
[32m+[m[32m            length = arguments.length,[m[41m[m
[32m+[m[32m            deep = false;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Handle a deep copy situation[m[41m[m
[32m+[m[32m        if (typeof target === "boolean") {[m[41m[m
[32m+[m[32m            deep = target;[m[41m[m
[32m+[m[32m            target = arguments[1] || {};[m[41m[m
[32m+[m[32m            // skip the boolean and the target[m[41m[m
[32m+[m[32m            i = 2;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Handle case when target is a string or something (possible in deep copy)[m[41m[m
[32m+[m[32m        if (typeof target !== "object" && !jQuery.isFunction(target)) {[m[41m[m
[32m+[m[32m            target = {};[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // extend jQuery itself if only one argument is passed[m[41m[m
[32m+[m[32m        if (length === i) {[m[41m[m
[32m+[m[32m            target = this;[m[41m[m
[32m+[m[32m            --i;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; i < length; i++) {[m[41m[m
[32m+[m[32m            // Only deal with non-null/undefined values[m[41m[m
[32m+[m[32m            if ((options = arguments[i]) != null) {[m[41m[m
[32m+[m[32m                // Extend the base object[m[41m[m
[32m+[m[32m                for (name in options) {[m[41m[m
[32m+[m[32m                    src = target[name];[m[41m[m
[32m+[m[32m                    copy = options[name];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Prevent never-ending loop[m[41m[m
[32m+[m[32m                    if (target === copy) {[m[41m[m
[32m+[m[32m                        continue;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Recurse if we're merging plain objects or arrays[m[41m[m
[32m+[m[32m                    if (deep && copy && (jQuery.isPlainObject(copy) || (copyIsArray = jQuery.isArray(copy)))) {[m[41m[m
[32m+[m[32m                        if (copyIsArray) {[m[41m[m
[32m+[m[32m                            copyIsArray = false;[m[41m[m
[32m+[m[32m                            clone = src && jQuery.isArray(src) ? src : [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        } else {[m[41m[m
[32m+[m[32m                            clone = src && jQuery.isPlainObject(src) ? src : {};[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        // Never move original objects, clone them[m[41m[m
[32m+[m[32m                        target[name] = jQuery.extend(deep, clone, copy);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        // Don't bring in undefined values[m[41m[m
[32m+[m[32m                    } else if (copy !== undefined) {[m[41m[m
[32m+[m[32m                        target[name] = copy;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Return the modified object[m[41m[m
[32m+[m[32m        return target;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.fadeIn = function (speed, easing, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Display the matched elements by fading them to opaque.[m[41m[m
[32m+[m[32m        ///     &#10;1 - fadeIn(duration, complete)[m[41m [m
[32m+[m[32m        ///     &#10;2 - fadeIn(options)[m[41m [m
[32m+[m[32m        ///     &#10;3 - fadeIn(duration, easing, complete)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="speed" type="">[m[41m[m
[32m+[m[32m        ///     A string or number determining how long the animation will run.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="easing" type="String">[m[41m[m
[32m+[m[32m        ///     A string indicating which easing function to use for the transition.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to call once the animation is complete.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.animate(props, speed, easing, callback);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.fadeOut = function (speed, easing, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Hide the matched elements by fading them to transparent.[m[41m[m
[32m+[m[32m        ///     &#10;1 - fadeOut(duration, complete)[m[41m [m
