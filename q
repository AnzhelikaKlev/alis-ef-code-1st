[32m+[m[32m    jQuery.prototype.animate = function (prop, speed, easing, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Perform a custom animation of a set of CSS properties.[m[41m[m
[32m+[m[32m        ///     &#10;1 - animate(properties, duration, easing, complete)[m[41m [m
[32m+[m[32m        ///     &#10;2 - animate(properties, options)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="prop" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     An object of CSS properties and values that the animation will move toward.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="speed" type="">[m[41m[m
[32m+[m[32m        ///     A string or number determining how long the animation will run.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="easing" type="String">[m[41m[m
[32m+[m[32m        ///     A string indicating which easing function to use for the transition.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to call once the animation is complete.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var empty = jQuery.isEmptyObject(prop),[m[41m[m
[32m+[m			[32moptall = jQuery.speed(speed, easing, callback),[m[41m[m
[32m+[m			[32mdoAnimation = function () {[m[41m[m
[32m+[m			[32m    // Operate on a copy of prop so per-property easing won't be lost[m[41m[m
[32m+[m			[32m    var anim = Animation(this, jQuery.extend({}, prop), optall);[m[41m[m
[32m+[m			[32m    doAnimation.finish = function () {[m[41m[m
[32m+[m			[32m        anim.stop(true);[m[41m[m
[32m+[m			[32m    };[m[41m[m
[32m+[m			[32m    // Empty animations, or finishing resolves immediately[m[41m[m
[32m+[m			[32m    if (empty || jQuery._data(this, "finish")) {[m[41m[m
[32m+[m			[32m        anim.stop(true);[m[41m[m
[32m+[m			[32m    }[m[41m[m
[32m+[m			[32m};[m[41m[m
[32m+[m[32m        doAnimation.finish = doAnimation;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return empty || optall.queue === false ?[m[41m[m
[32m+[m			[32mthis.each(doAnimation) :[m[41m[m
[32m+[m			[32mthis.queue(optall.queue, doAnimation);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.append = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Insert content, specified by the parameter, to the end of each element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - append(content, content)[m[41m [m
[32m+[m[32m        ///     &#10;2 - append(function(index, html))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="" type="jQuery">[m[41m[m
[32m+[m[32m        ///     DOM element, HTML string, or jQuery object to insert at the end of each element in the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="" type="jQuery">[m[41m[m
[32m+[m[32m        ///     One or more additional DOM elements, arrays of elements, HTML strings, or jQuery objects to insert at the end of each element in the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.domManip(arguments, true, function (elem) {[m[41m[m
[32m+[m[32m            if (this.nodeType === 1 || this.nodeType === 11 || this.nodeType === 9) {[m[41m[m
[32m+[m[32m                this.appendChild(elem);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.appendTo = function (selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Insert every element in the set of matched elements to the end of the target.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="jQuery">[m[41m[m
[32m+[m[32m        ///     A selector, element, HTML string, or jQuery object; the matched set of elements will be inserted at the end of the element(s) specified by this parameter.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var elems,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mret = [],[m[41m[m
[32m+[m			[32minsert = jQuery(selector),[m[41m[m
[32m+[m			[32mlast = insert.length - 1;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; i <= last; i++) {[m[41m[m
[32m+[m[32m            elems = i === last ? this : this.clone(true);[m[41m[m
[32m+[m[32m            jQuery(insert[i])[original](elems);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Modern browsers can apply jQuery collections as arrays, but oldIE needs a .get()[m[41m[m
[32m+[m[32m            core_push.apply(ret, elems.get());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.attr = function (name, value) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Get the value of an attribute for the first element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - attr(attributeName)[m[41m[m
[32m+[m[32m        ///     &#10;2: Set one or more attributes for the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - attr(attributeName, value)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - attr(attributes)[m[41m [m
[32m+[m[32m        ///     &#10;    2.3 - attr(attributeName, function(index, attr))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="name" type="String">[m[41m[m
[32m+[m[32m        ///     The name of the attribute to set.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="value" type="Number">[m[41m[m
[32m+[m[32m        ///     A value to set for the attribute.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.access(this, jQuery.attr, name, value, arguments.length > 1);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.before = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Insert content, specified by the parameter, before each element in the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - before(content, content)[m[41m [m
[32m+[m[32m        ///     &#10;2 - before(function)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="" type="jQuery">[m[41m[m
[32m+[m[32m        ///     HTML string, DOM element, or jQuery object to insert before each element in the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="" type="jQuery">[m[41m[m
[32m+[m[32m        ///     One or more additional DOM elements, arrays of elements, HTML strings, or jQuery objects to insert before each element in the set of matched elements.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.domManip(arguments, false, function (elem) {[m[41m[m
[32m+[m[32m            if (this.parentNode) {[m[41m[m
[32m+[m[32m                this.parentNode.insertBefore(elem, this);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.bind = function (types, data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Attach a handler to an event for the elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - bind(eventType, eventData, handler(eventObject))[m[41m [m
[32m+[m[32m        ///     &#10;2 - bind(eventType, eventData, preventBubble)[m[41m [m
[32m+[m[32m        ///     &#10;3 - bind(events)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="types" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing one or more DOM event types, such as "click" or "submit," or custom event names.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Object">[m[41m[m
[32m+[m[32m        ///     An object containing data that will be passed to the event handler.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">[m[41m[m
[32m+[m[32m        ///     A function to execute each time the event is triggered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.on(types, null, data, fn);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.blur = function (data, fn) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
