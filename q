[32m+[m[32m    jQuery.prototype.clone = function (dataAndEvents, deepDataAndEvents) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Create a deep copy of the set of matched elements.[m[41m[m
[32m+[m[32m        ///     &#10;1 - clone(withDataAndEvents)[m[41m [m
[32m+[m[32m        ///     &#10;2 - clone(withDataAndEvents, deepWithDataAndEvents)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="dataAndEvents" type="Boolean">[m[41m[m
[32m+[m[32m        ///     A Boolean indicating whether event handlers and data should be copied along with the elements. The default value is false. *In jQuery 1.5.0 the default value was incorrectly true; it was changed back to false in 1.5.1 and up.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="deepDataAndEvents" type="Boolean">[m[41m[m
[32m+[m[32m        ///     A Boolean indicating whether event handlers and data for all children of the cloned element should be copied. By default its value matches the first argument's value (which defaults to false).[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        dataAndEvents = dataAndEvents == null ? false : dataAndEvents;[m[41m[m
[32m+[m[32m        deepDataAndEvents = deepDataAndEvents == null ? dataAndEvents : deepDataAndEvents;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.map(function () {[m[41m[m
[32m+[m[32m            return jQuery.clone(this, dataAndEvents, deepDataAndEvents);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.closest = function (selectors, context) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: For each element in the set, get the first element that matches the selector by testing the element itself and traversing up through its ancestors in the DOM tree.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - closest(selector)[m[41m [m
[32m+[m[32m        ///     &#10;    1.2 - closest(selector, context)[m[41m [m
[32m+[m[32m        ///     &#10;    1.3 - closest(jQuery object)[m[41m [m
[32m+[m[32m        ///     &#10;    1.4 - closest(element)[m[41m[m
[32m+[m[32m        ///     &#10;2: Get an array of all the elements and selectors matched against the current element up through the DOM tree.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - closest(selectors, context)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selectors" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression to match elements against.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="context" domElement="true">[m[41m[m
[32m+[m[32m        ///     A DOM element within which a matching element may be found. If no context is passed in then the context of the jQuery set will be used instead.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var cur,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32ml = this.length,[m[41m[m
[32m+[m			[32mret = [],[m[41m[m
[32m+[m			[32mpos = rneedsContext.test(selectors) || typeof selectors !== "string" ?[m[41m[m
[32m+[m				[32mjQuery(selectors, context || this.context) :[m[41m[m
[32m+[m				[32m0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; i < l; i++) {[m[41m[m
[32m+[m[32m            cur = this[i];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            while (cur && cur.ownerDocument && cur !== context && cur.nodeType !== 11) {[m[41m[m
[32m+[m[32m                if (pos ? pos.index(cur) > -1 : jQuery.find.matchesSelector(cur, selectors)) {[m[41m[m
[32m+[m[32m                    ret.push(cur);[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                cur = cur.parentNode;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return this.pushStack(ret.length > 1 ? jQuery.unique(ret) : ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.constructor = function (selector, context) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // The jQuery object is actually just the init constructor 'enhanced'[m[41m[m
[32m+[m[32m        return new jQuery.fn.init(selector, context, rootjQuery);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.prototype.contents = function (until, selector) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Get the children of each element in the set of matched elements, including text and comment nodes.[m[41m[m
