[32m+[m[32m        ///     &#10;    1.3 - $(elementArray)[m[41m [m
[32m+[m[32m        ///     &#10;    1.4 - $(object)[m[41m [m
[32m+[m[32m        ///     &#10;    1.5 - $(jQuery object)[m[41m [m
[32m+[m[32m        ///     &#10;    1.6 - $()[m[41m[m
[32m+[m[32m        ///     &#10;2: Creates DOM elements on the fly from the provided string of raw HTML.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - $(html, ownerDocument)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - $(html, attributes)[m[41m[m
[32m+[m[32m        ///     &#10;3: Binds a function to be executed when the DOM has finished loading.[m[41m[m
[32m+[m[32m        ///     &#10;    3.1 - $(callback)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="selector" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing a selector expression[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="context" type="jQuery">[m[41m[m
[32m+[m[32m        ///     A DOM Element, Document, or jQuery to use as context[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // The jQuery object is actually just the init constructor 'enhanced'[m[41m[m
[32m+[m[32m        return new jQuery.fn.init(selector, context, rootjQuery);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Animation = function Animation(elem, properties, options) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var result,[m[41m[m
[32m+[m[32m            stopped,[m[41m[m
[32m+[m[32m            index = 0,[m[41m[m
[32m+[m[32m            length = animationPrefilters.length,[m[41m[m
[32m+[m[32m            deferred = jQuery.Deferred().always(function () {[m[41m[m
[32m+[m[32m                // don't match elem in the :animated selector[m[41m[m
[32m+[m[32m                delete tick.elem;[m[41m[m
[32m+[m[32m            }),[m[41m[m
[32m+[m[32m            tick = function () {[m[41m[m
[32m+[m[32m                if (stopped) {[m[41m[m
[32m+[m[32m                    return false;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                var currentTime = fxNow || createFxNow(),[m[41m[m
[32m+[m[32m                    remaining = Math.max(0, animation.startTime + animation.duration - currentTime),[m[41m[m
[32m+[m[32m                    // archaic crash bug won't allow us to use 1 - ( 0.5 || 0 ) (#12497)[m[41m[m
[32m+[m[32m                    temp = remaining / animation.duration || 0,[m[41m[m
[32m+[m[32m                    percent = 1 - temp,[m[41m[m
[32m+[m[32m                    index = 0,[m[41m[m
[32m+[m[32m                    length = animation.tweens.length;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                for (; index < length ; index++) {[m[41m[m
[32m+[m[32m                    animation.tweens[index].run(percent);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                deferred.notifyWith(elem, [animation, percent, remaining]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (percent < 1 && length) {[m[41m[m
[32m+[m[32m                    return remaining;[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    deferred.resolveWith(elem, [animation]);[m[41m[m
[32m+[m[32m                    return false;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m            animation = deferred.promise({[m[41m[m
[32m+[m[32m                elem: elem,[m[41m[m
[32m+[m[32m                props: jQuery.extend({}, properties),[m[41m[m
[32m+[m[32m                opts: jQuery.extend(true, { specialEasing: {} }, options),[m[41m[m
[32m+[m[32m                originalProperties: properties,[m[41m[m
[32m+[m[32m                originalOptions: options,[m[41m[m
[32m+[m[32m                startTime: fxNow || createFxNow(),[m[41m[m
[32m+[m[32m                duration: options.duration,[m[41m[m
[32m+[m[32m                tweens: [],[m[41m[m
[32m+[m[32m                createTween: function (prop, end) {[m[41m[m
[32m+[m[32m                    var tween = jQuery.Tween(elem, animation.opts, prop, end,[m[41m[m
[32m+[m[32m                            animation.opts.specialEasing[prop] || animation.opts.easing);[m[41m[m
[32m+[m[32m                    animation.tweens.push(tween);[m[41m[m
[32m+[m[32m                    return tween;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                stop: function (gotoEnd) {[m[41m[m
[32m+[m[32m                    var index = 0,[m[41m[m
[32m+[m[32m                        // if we are going to the end, we want to run all the tweens[m[41m[m
[32m+[m[32m                        // otherwise we skip this part[m[41m[m
[32m+[m[32m                        length = gotoEnd ? animation.tweens.length : 0;[m[41m[m
[32m+[m[32m                    if (stopped) {[m[41m[m
[32m+[m[32m                        return this;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    stopped = true;[m[41m[m
[32m+[m[32m                    for (; index < length ; index++) {[m[41m[m
[32m+[m[32m                        animation.tweens[index].run(1);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // resolve when we played the last frame[m[41m[m
[32m+[m[32m                    // otherwise, reject[m[41m[m
[32m+[m[32m                    if (gotoEnd) {[m[41m[m
[32m+[m[32m                        deferred.resolveWith(elem, [animation, gotoEnd]);[m[41m[m
[32m+[m[32m                    } else {[m[41m[m
[32m+[m[32m                        deferred.rejectWith(elem, [animation, gotoEnd]);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }),[m[41m[m
[32m+[m[32m            props = animation.props;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        propFilter(props, animation.opts.specialEasing);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; index < length ; index++) {[m[41m[m
[32m+[m[32m            result = animationPrefilters[index].call(animation, elem, props, animation.opts);[m[41m[m
[32m+[m[32m            if (result) {[m[41m[m
[32m+[m[32m                return result;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        createTweens(animation, props);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (jQuery.isFunction(animation.opts.start)) {[m[41m[m
[32m+[m[32m            animation.opts.start.call(elem, animation);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        jQuery.fx.timer([m[41m[m
[32m+[m[32m            jQuery.extend(tick, {[m[41m[m
[32m+[m[32m                elem: elem,[m[41m[m
[32m+[m[32m                anim: animation,[m[41m[m
[32m+[m[32m                queue: animation.opts.queue[m[41m[m
[32m+[m[32m            })[m[41m[m
[32m+[m[32m        );[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // attach callbacks from options[m[41m[m
[32m+[m[32m        return animation.progress(animation.opts.progress)[m[41m[m
[32m+[m[32m            .done(animation.opts.done, animation.opts.complete)[m[41m[m
[32m+[m[32m            .fail(animation.opts.fail)[m[41m[m
[32m+[m[32m            .always(animation.opts.always);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Callbacks = function (options) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     A multi-purpose callbacks list object that provides a powerful way to manage callback lists.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="options" type="String">[m[41m[m
[32m+[m[32m        ///     An optional list of space-separated flags that change how the callback list behaves.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Callbacks" />[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Convert options from String-formatted to Object-formatted if needed[m[41m[m
[32m+[m[32m        // (we check in cache first)[m[41m[m
[32m+[m[32m        options = typeof options === "string" ?[m[41m[m
[32m+[m[32m            (optionsCache[options] || createOptions(options)) :[m[41m[m
[32m+[m[32m            jQuery.extend({}, options);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var // Flag to know if list is currently firing[m[41m[m
[32m+[m[32m            firing,[m[41m[m
[32m+[m[32m            // Last fire value (for non-forgettable lists)[m[41m[m
[32m+[m[32m            memory,[m[41m[m
[32m+[m[32m            // Flag to know if list was already fired[m[41m[m
[32m+[m[32m            fired,[m[41m[m
[32m+[m[32m            // End of the loop when firing[m[41m[m
[32m+[m[32m            firingLength,[m[41m[m
[32m+[m[32m            // Index of currently firing callback (modified by remove if needed)[m[41m[m
[32m+[m[32m            firingIndex,[m[41m[m
[32m+[m[32m            // First callback to fire (used internally by add and fireWith)[m[41m[m
[32m+[m[32m            firingStart,[m[41m[m
[32m+[m[32m            // Actual callback list[m[41m[m
[32m+[m[32m            list = [],[m[41m[m
[32m+[m[32m            // Stack of fire calls for repeatable lists[m[41m[m
[32m+[m[32m            stack = !options.once && [],[m[41m[m
[32m+[m[32m            // Fire callbacks[m[41m[m
[32m+[m[32m            fire = function (data) {[m[41m[m
[32m+[m[32m                memory = options.memory && data;[m[41m[m
[32m+[m[32m                fired = true;[m[41m[m
[32m+[m[32m                firingIndex = firingStart || 0;[m[41m[m
[32m+[m[32m                firingStart = 0;[m[41m[m
[32m+[m[32m                firingLength = list.length;[m[41m[m
[32m+[m[32m                firing = true;[m[41m[m
[32m+[m[32m                for (; list && firingIndex < firingLength; firingIndex++) {[m[41m[m
[32m+[m[32m                    if (list[firingIndex].apply(data[0], data[1]) === false && options.stopOnFalse) {[m[41m[m
[32m+[m[32m                        memory = false; // To prevent further calls using add[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                firing = false;[m[41m[m
[32m+[m[32m                if (list) {[m[41m[m
[32m+[m[32m                    if (stack) {[m[41m[m
[32m+[m[32m                        if (stack.length) {[m[41m[m
[32m+[m[32m                            fire(stack.shift());[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    } else if (memory) {[m[41m[m
[32m+[m[32m                        list = [];[m[41m[m
[32m+[m[32m                    } else {[m[41m[m
[32m+[m[32m                        self.disable();[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m            // Actual Callbacks object[m[41m[m
[32m+[m[32m            self = {[m[41m[m
[32m+[m[32m                // Add a callback or a collection of callbacks to the list[m[41m[m
[32m+[m[32m                add: function () {[m[41m[m
[32m+[m[32m                    if (list) {[m[41m[m
[32m+[m[32m                        // First, we save the current length[m[41m[m
[32m+[m[32m                        var start = list.length;[m[41m[m
[32m+[m[32m                        (function add(args) {[m[41m[m
[32m+[m[32m                            jQuery.each(args, function (_, arg) {[m[41m[m
[32m+[m[32m                                var type = jQuery.type(arg);[m[41m[m
[32m+[m[32m                                if (type === "function") {[m[41m[m
[32m+[m[32m                                    if (!options.unique || !self.has(arg)) {[m[41m[m
[32m+[m[32m                                        list.push(arg);[m[41m[m
[32m+[m[32m                                    }[m[41m[m
[32m+[m[32m                                } else if (arg && arg.length && type !== "string") {[m[41m[m
[32m+[m[32m                                    // Inspect recursively[m[41m[m
[32m+[m[32m                                    add(arg);[m[41m[m
[32m+[m[32m                                }[m[41m[m
[32m+[m[32m                            });[m[41m[m
[32m+[m[32m                        })(arguments);[m[41m[m
[32m+[m[32m                        // Do we need to add the callbacks to the[m[41m[m
[32m+[m[32m                        // current firing batch?[m[41m[m
[32m+[m[32m                        if (firing) {[m[41m[m
[32m+[m[32m                            firingLength = list.length;[m[41m[m
[32m+[m[32m                            // With memory, if we're not firing then[m[41m[m
[32m+[m[32m                            // we should call right away[m[41m[m
[32m+[m[32m                        } else if (memory) {[m[41m[m
[32m+[m[32m                            firingStart = start;[m[41m[m
[32m+[m[32m                            fire(memory);[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Remove a callback from the list[m[41m[m
[32m+[m[32m                remove: function () {[m[41m[m
[32m+[m[32m                    if (list) {[m[41m[m
[32m+[m[32m                        jQuery.each(arguments, function (_, arg) {[m[41m[m
[32m+[m[32m                            var index;[m[41m[m
[32m+[m[32m                            while ((index = jQuery.inArray(arg, list, index)) > -1) {[m[41m[m
[32m+[m[32m                                list.splice(index, 1);[m[41m[m
[32m+[m[32m                                // Handle firing indexes[m[41m[m
[32m+[m[32m                                if (firing) {[m[41m[m
[32m+[m[32m                                    if (index <= firingLength) {[m[41m[m
[32m+[m[32m                                        firingLength--;[m[41m[m
[32m+[m[32m                                    }[m[41m[m
[32m+[m[32m                                    if (index <= firingIndex) {[m[41m[m
[32m+[m[32m                                        firingIndex--;[m[41m[m
[32m+[m[32m                                    }[m[41m[m
[32m+[m[32m                                }[m[41m[m
[32m+[m[32m                            }[m[41m[m
[32m+[m[32m                        });[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Check if a given callback is in the list.[m[41m[m
[32m+[m[32m                // If no argument is given, return whether or not list has callbacks attached.[m[41m[m
[32m+[m[32m                has: function (fn) {[m[41m[m
[32m+[m[32m                    return fn ? jQuery.inArray(fn, list) > -1 : !!(list && list.length);[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Remove all callbacks from the list[m[41m[m
[32m+[m[32m                empty: function () {[m[41m[m
[32m+[m[32m                    list = [];[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Have the list do nothing anymore[m[41m[m
[32m+[m[32m                disable: function () {[m[41m[m
[32m+[m[32m                    list = stack = memory = undefined;[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Is it disabled?[m[41m[m
[32m+[m[32m                disabled: function () {[m[41m[m
[32m+[m[32m                    return !list;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Lock the list in its current state[m[41m[m
[32m+[m[32m                lock: function () {[m[41m[m
[32m+[m[32m                    stack = undefined;[m[41m[m
[32m+[m[32m                    if (!memory) {[m[41m[m
[32m+[m[32m                        self.disable();[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Is it locked?[m[41m[m
[32m+[m[32m                locked: function () {[m[41m[m
[32m+[m[32m                    return !stack;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Call all callbacks with the given context and arguments[m[41m[m
[32m+[m[32m                fireWith: function (context, args) {[m[41m[m
[32m+[m[32m                    args = args || [];[m[41m[m
[32m+[m[32m                    args = [context, args.slice ? args.slice() : args];[m[41m[m
[32m+[m[32m                    if (list && (!fired || stack)) {[m[41m[m
[32m+[m[32m                        if (firing) {[m[41m[m
[32m+[m[32m                            stack.push(args);[m[41m[m
[32m+[m[32m                        } else {[m[41m[m
[32m+[m[32m                            fire(args);[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // Call all the callbacks with the given arguments[m[41m[m
[32m+[m[32m                fire: function () {[m[41m[m
[32m+[m[32m                    self.fireWith(this, arguments);[m[41m[m
[32m+[m[32m                    return this;[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                // To know if the callbacks have already been called at least once[m[41m[m
[32m+[m[32m                fired: function () {[m[41m[m
[32m+[m[32m                    return !!fired;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return self;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Deferred = function (func) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     A constructor function that returns a chainable utility object with methods to register multiple callbacks into callback queues, invoke callback queues, and relay the success or failure state of any synchronous or asynchronous function.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="func" type="Function">[m[41m[m
[32m+[m[32m        ///     A function that is called just before the constructor returns.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Deferred" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var tuples = [[m[41m[m
[32m+[m				[32m// action, add listener, listener list, final state[m[41m[m
[32m+[m				[32m["resolve", "done", jQuery.Callbacks("once memory"), "resolved"],[m[41m[m
[32m+[m				[32m["reject", "fail", jQuery.Callbacks("once memory"), "rejected"],[m[41m[m
[32m+[m				[32m["notify", "progress", jQuery.Callbacks("memory")][m[41m[m
[32m+[m[32m        ],[m[41m[m
[32m+[m			[32mstate = "pending",[m[41m[m
[32m+[m			[32mpromise = {[m[41m[m
[32m+[m			[32m    state: function () {[m[41m[m
[32m+[m			[32m        return state;[m[41m[m
[32m+[m			[32m    },[m[41m[m
[32m+[m			[32m    always: function () {[m[41m[m
[32m+[m			[32m        deferred.done(arguments).fail(arguments);[m[41m[m
[32m+[m			[32m        return this;[m[41m[m
[32m+[m			[32m    },[m[41m[m
[32m+[m			[32m    then: function ( /* fnDone, fnFail, fnProgress */) {[m[41m[m
[32m+[m			[32m        var fns = arguments;[m[41m[m
[32m+[m			[32m        return jQuery.Deferred(function (newDefer) {[m[41m[m
[32m+[m			[32m            jQuery.each(tuples, function (i, tuple) {[m[41m[m
[32m+[m			[32m                var action = tuple[0],[m[41m[m
[32m+[m								[32mfn = jQuery.isFunction(fns[i]) && fns[i];[m[41m[m
[32m+[m			[32m                // deferred[ done | fail | progress ] for forwarding actions to newDefer[m[41m[m
[32m+[m			[32m                deferred[tuple[1]](function () {[m[41m[m
[32m+[m			[32m                    var returned = fn && fn.apply(this, arguments);[m[41m[m
[32m+[m			[32m                    if (returned && jQuery.isFunction(returned.promise)) {[m[41m[m
[32m+[m			[32m                        returned.promise()[m[41m[m
[32m+[m										[32m.done(newDefer.resolve)[m[41m[m
[32m+[m										[32m.fail(newDefer.reject)[m[41m[m
[32m+[m										[32m.progress(newDefer.notify);[m[41m[m
[32m+[m			[32m                    } else {[m[41m[m
[32m+[m			[32m                        newDefer[action + "With"](this === promise ? newDefer.promise() : this, fn ? [returned] : arguments);[m[41m[m
[32m+[m			[32m                    }[m[41m[m
[32m+[m			[32m                });[m[41m[m
[32m+[m			[32m            });[m[41m[m
[32m+[m			[32m            fns = null;[m[41m[m
[32m+[m			[32m        }).promise();[m[41m[m
[32m+[m			[32m    },[m[41m[m
[32m+[m			[32m    // Get a promise for this deferred[m[41m[m
[32m+[m			[32m    // If obj is provided, the promise aspect is added to the object[m[41m[m
[32m+[m			[32m    promise: function (obj) {[m[41m[m
[32m+[m			[32m        return obj != null ? jQuery.extend(obj, promise) : promise;[m[41m[m
[32m+[m			[32m    }[m[41m[m
[32m+[m			[32m},[m[41m[m
[32m+[m			[32mdeferred = {};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Keep pipe for back-compat[m[41m[m
[32m+[m[32m        promise.pipe = promise.then;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Add list-specific methods[m[41m[m
[32m+[m[32m        jQuery.each(tuples, function (i, tuple) {[m[41m[m
[32m+[m[32m            var list = tuple[2],[m[41m[m
[32m+[m				[32mstateString = tuple[3];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // promise[ done | fail | progress ] = list.add[m[41m[m
[32m+[m[32m            promise[tuple[1]] = list.add;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Handle state[m[41m[m
[32m+[m[32m            if (stateString) {[m[41m[m
[32m+[m[32m                list.add(function () {[m[41m[m
[32m+[m[32m                    // state = [ resolved | rejected ][m[41m[m
[32m+[m[32m                    state = stateString;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // [ reject_list | resolve_list ].disable; progress_list.lock[m[41m[m
[32m+[m[32m                }, tuples[i ^ 1][2].disable, tuples[2][2].lock);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // deferred[ resolve | reject | notify ][m[41m[m
[32m+[m[32m            deferred[tuple[0]] = function () {[m[41m[m
[32m+[m[32m                deferred[tuple[0] + "With"](this === deferred ? promise : this, arguments);[m[41m[m
[32m+[m[32m                return this;[m[41m[m
[32m+[m[32m            };[m[41m[m
[32m+[m[32m            deferred[tuple[0] + "With"] = list.fireWith;[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Make the deferred a promise[m[41m[m
[32m+[m[32m        promise.promise(deferred);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Call given func if any[m[41m[m
[32m+[m[32m        if (func) {[m[41m[m
[32m+[m[32m            func.call(deferred, deferred);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // All done![m[41m[m
[32m+[m[32m        return deferred;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Event = function (src, props) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Allow instantiation without the 'new' keyword[m[41m[m
[32m+[m[32m        if (!(this instanceof jQuery.Event)) {[m[41m[m
[32m+[m[32m            return new jQuery.Event(src, props);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Event object[m[41m[m
[32m+[m[32m        if (src && src.type) {[m[41m[m
[32m+[m[32m            this.originalEvent = src;[m[41m[m
[32m+[m[32m            this.type = src.type;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Events bubbling up the document may have been marked as prevented[m[41m[m
[32m+[m[32m            // by a handler lower down the tree; reflect the correct value.[m[41m[m
[32m+[m[32m            this.isDefaultPrevented = (src.defaultPrevented || src.returnValue === false ||[m[41m[m
[32m+[m[32m                src.getPreventDefault && src.getPreventDefault()) ? returnTrue : returnFalse;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Event type[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            this.type = src;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Put explicitly provided properties onto the event object[m[41m[m
[32m+[m[32m        if (props) {[m[41m[m
[32m+[m[32m            jQuery.extend(this, props);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Create a timestamp if incoming event doesn't have one[m[41m[m
[32m+[m[32m        this.timeStamp = src && src.timeStamp || jQuery.now();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Mark it as fixed[m[41m[m
[32m+[m[32m        this[jQuery.expando] = true;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.Tween = function Tween(elem, options, prop, end, easing) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return new Tween.prototype.init(elem, options, prop, end, easing);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery._data = function (elem, name, data) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return internalData(elem, name, data, true);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery._queueHooks = function (elem, type) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var key = type + "queueHooks";[m[41m[m
[32m+[m[32m        return jQuery._data(elem, key) || jQuery._data(elem, key, {[m[41m[m
[32m+[m[32m            empty: jQuery.Callbacks("once memory").add(function () {[m[41m[m
[32m+[m[32m                jQuery._removeData(elem, type + "queue");[m[41m[m
[32m+[m[32m                jQuery._removeData(elem, key);[m[41m[m
[32m+[m[32m            })[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery._removeData = function (elem, name) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return internalRemoveData(elem, name, true);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.acceptData = function (elem) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Do not set data on non-element because it will not be cleared (#8335).[m[41m[m
[32m+[m[32m        if (elem.nodeType && elem.nodeType !== 1 && elem.nodeType !== 9) {[m[41m[m
[32m+[m[32m            return false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var noData = elem.nodeName && jQuery.noData[elem.nodeName.toLowerCase()];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // nodes accept data unless otherwise specified; rejection can be conditional[m[41m[m
[32m+[m[32m        return !noData || noData !== true && elem.getAttribute("classid") === noData;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.access = function (elems, fn, key, value, chainable, emptyGet, raw) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var i = 0,[m[41m[m
[32m+[m			[32mlength = elems.length,[m[41m[m
[32m+[m			[32mbulk = key == null;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Sets many values[m[41m[m
[32m+[m[32m        if (jQuery.type(key) === "object") {[m[41m[m
[32m+[m[32m            chainable = true;[m[41m[m
[32m+[m[32m            for (i in key) {[m[41m[m
[32m+[m[32m                jQuery.access(elems, fn, i, key[i], true, emptyGet, raw);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Sets one value[m[41m[m
[32m+[m[32m        } else if (value !== undefined) {[m[41m[m
[32m+[m[32m            chainable = true;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (!jQuery.isFunction(value)) {[m[41m[m
[32m+[m[32m                raw = true;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (bulk) {[m[41m[m
[32m+[m[32m                // Bulk operations run against the entire set[m[41m[m
[32m+[m[32m                if (raw) {[m[41m[m
[32m+[m[32m                    fn.call(elems, value);[m[41m[m
[32m+[m[32m                    fn = null;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // ...except when executing function values[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    bulk = fn;[m[41m[m
[32m+[m[32m                    fn = function (elem, key, value) {[m[41m[m
[32m+[m[32m                        return bulk.call(jQuery(elem), value);[m[41m[m
[32m+[m[32m                    };[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (fn) {[m[41m[m
[32m+[m[32m                for (; i < length; i++) {[m[41m[m
[32m+[m[32m                    fn(elems[i], key, raw ? value : value.call(elems[i], i, fn(elems[i], key)));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return chainable ?[m[41m[m
[32m+[m[32m            elems :[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// Gets[m[41m[m
[32m+[m			[32mbulk ?[m[41m[m
[32m+[m				[32mfn.call(elems) :[m[41m[m
[32m+[m				[32mlength ? fn(elems[0], key) : emptyGet;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.active = 0;[m[41m[m
[32m+[m[32m    jQuery.ajax = function (url, options) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Perform an asynchronous HTTP (Ajax) request.[m[41m[m
[32m+[m[32m        ///     &#10;1 - jQuery.ajax(url, settings)[m[41m [m
[32m+[m[32m        ///     &#10;2 - jQuery.ajax(settings)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="url" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the URL to which the request is sent.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="options" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     A set of key/value pairs that configure the Ajax request. All settings are optional. A default can be set for any option with $.ajaxSetup(). See jQuery.ajax( settings ) below for a complete list of all settings.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If url is an object, simulate pre-1.5 signature[m[41m[m
[32m+[m[32m        if (typeof url === "object") {[m[41m[m
[32m+[m[32m            options = url;[m[41m[m
[32m+[m[32m            url = undefined;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Force options to be an object[m[41m[m
[32m+[m[32m        options = options || {};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var // Cross-domain detection vars[m[41m[m
[32m+[m			[32mparts,[m[41m[m
[32m+[m			[32m// Loop variable[m[41m[m
[32m+[m			[32mi,[m[41m[m
[32m+[m			[32m// URL without anti-cache param[m[41m[m
[32m+[m			[32mcacheURL,[m[41m[m
[32m+[m			[32m// Response headers as string[m[41m[m
[32m+[m			[32mresponseHeadersString,[m[41m[m
[32m+[m			[32m// timeout handle[m[41m[m
[32m+[m			[32mtimeoutTimer,[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// To know if global events are to be dispatched[m[41m[m
[32m+[m			[32mfireGlobals,[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mtransport,[m[41m[m
[32m+[m			[32m// Response headers[m[41m[m
[32m+[m			[32mresponseHeaders,[m[41m[m
[32m+[m			[32m// Create the final options object[m[41m[m
[32m+[m			[32ms = jQuery.ajaxSetup({}, options),[m[41m[m
[32m+[m			[32m// Callbacks context[m[41m[m
[32m+[m			[32mcallbackContext = s.context || s,[m[41m[m
[32m+[m			[32m// Context for global events is callbackContext if it is a DOM node or jQuery collection[m[41m[m
[32m+[m			[32mglobalEventContext = s.context && (callbackContext.nodeType || callbackContext.jquery) ?[m[41m[m
[32m+[m				[32mjQuery(callbackContext) :[m[41m[m
[32m+[m				[32mjQuery.event,[m[41m[m
[32m+[m			[32m// Deferreds[m[41m[m
[32m+[m			[32mdeferred = jQuery.Deferred(),[m[41m[m
[32m+[m			[32mcompleteDeferred = jQuery.Callbacks("once memory"),[m[41m[m
[32m+[m			[32m// Status-dependent callbacks[m[41m[m
[32m+[m			[32mstatusCode = s.statusCode || {},[m[41m[m
[32m+[m			[32m// Headers (they are sent all at once)[m[41m[m
[32m+[m			[32mrequestHeaders = {},[m[41m[m
[32m+[m			[32mrequestHeadersNames = {},[m[41m[m
[32m+[m			[32m// The jqXHR state[m[41m[m
[32m+[m			[32mstate = 0,[m[41m[m
[32m+[m			[32m// Default abort message[m[41m[m
[32m+[m			[32mstrAbort = "canceled",[m[41m[m
[32m+[m			[32m// Fake xhr[m[41m[m
[32m+[m			[32mjqXHR = {[m[41m[m
[32m+[m			[32m    readyState: 0,[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m    // Builds headers hashtable if needed[m[41m[m
[32m+[m			[32m    getResponseHeader: function (key) {[m[41m[m
[32m+[m			[32m        var match;[m[41m[m
[32m+[m			[32m        if (state === 2) {[m[41m[m
[32m+[m			[32m            if (!responseHeaders) {[m[41m[m
[32m+[m			[32m                responseHeaders = {};[m[41m[m
[32m+[m			[32m                while ((match = rheaders.exec(responseHeadersString))) {[m[41m[m
[32m+[m			[32m                    responseHeaders[match[1].toLowerCase()] = match[2];[m[41m[m
[32m+[m			[32m                }[m[41m[m
[32m+[m			[32m            }[m[41m[m
[32m+[m			[32m            match = responseHeaders[key.toLowerCase()];[m[41m[m
[32m+[m			[32m        }[m[41m[m
[32m+[m			[32m        return match == null ? null : match;[m[41m[m
[32m+[m			[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m    // Raw string[m[41m[m
[32m+[m			[32m    getAllResponseHeaders: function () {[m[41m[m
[32m+[m			[32m        return state === 2 ? responseHeadersString : null;[m[41m[m
[32m+[m			[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m    // Caches the header[m[41m[m
[32m+[m			[32m    setRequestHeader: function (name, value) {[m[41m[m
[32m+[m			[32m        var lname = name.toLowerCase();[m[41m[m
[32m+[m			[32m        if (!state) {[m[41m[m
[32m+[m			[32m            name = requestHeadersNames[lname] = requestHeadersNames[lname] || name;[m[41m[m
[32m+[m			[32m            requestHeaders[name] = value;[m[41m[m
[32m+[m			[32m        }[m[41m[m
[32m+[m			[32m        return this;[m[41m[m
[32m+[m			[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m    // Overrides response content-type header[m[41m[m
[32m+[m			[32m    overrideMimeType: function (type) {[m[41m[m
[32m+[m			[32m        if (!state) {[m[41m[m
[32m+[m			[32m            s.mimeType = type;[m[41m[m
[32m+[m			[32m        }[m[41m[m
[32m+[m			[32m        return this;[m[41m[m
[32m+[m			[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m    // Status-dependent callbacks[m[41m[m
[32m+[m			[32m    statusCode: function (map) {[m[41m[m
[32m+[m			[32m        var code;[m[41m[m
[32m+[m			[32m        if (map) {[m[41m[m
[32m+[m			[32m            if (state < 2) {[m[41m[m
[32m+[m			[32m                for (code in map) {[m[41m[m
[32m+[m			[32m                    // Lazy-add the new callback in a way that preserves old ones[m[41m[m
[32m+[m			[32m                    statusCode[code] = [statusCode[code], map[code]];[m[41m[m
[32m+[m			[32m                }[m[41m[m
[32m+[m			[32m            } else {[m[41m[m
[32m+[m			[32m                // Execute the appropriate callbacks[m[41m[m
[32m+[m			[32m                jqXHR.always(map[jqXHR.status]);[m[41m[m
[32m+[m			[32m            }[m[41m[m
[32m+[m			[32m        }[m[41m[m
[32m+[m			[32m        return this;[m[41m[m
[32m+[m			[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m    // Cancel the request[m[41m[m
[32m+[m			[32m    abort: function (statusText) {[m[41m[m
[32m+[m			[32m        var finalText = statusText || strAbort;[m[41m[m
[32m+[m			[32m        if (transport) {[m[41m[m
[32m+[m			[32m            transport.abort(finalText);[m[41m[m
[32m+[m			[32m        }[m[41m[m
[32m+[m			[32m        done(0, finalText);[m[41m[m
[32m+[m			[32m        return this;[m[41m[m
[32m+[m			[32m    }[m[41m[m
[32m+[m			[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Attach deferreds[m[41m[m
[32m+[m[32m        deferred.promise(jqXHR).complete = completeDeferred.add;[m[41m[m
[32m+[m[32m        jqXHR.success = jqXHR.done;[m[41m[m
[32m+[m[32m        jqXHR.error = jqXHR.fail;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Remove hash character (#7531: and string promotion)[m[41m[m
[32m+[m[32m        // Add protocol if not provided (#5866: IE7 issue with protocol-less urls)[m[41m[m
[32m+[m[32m        // Handle falsy url in the settings object (#10093: consistency with old signature)[m[41m[m
[32m+[m[32m        // We also use the url parameter if available[m[41m[m
[32m+[m[32m        s.url = ((url || s.url || ajaxLocation) + "").replace(rhash, "").replace(rprotocol, ajaxLocParts[1] + "//");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Alias method option to type as per ticket #12004[m[41m[m
[32m+[m[32m        s.type = options.method || options.type || s.method || s.type;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Extract dataTypes list[m[41m[m
[32m+[m[32m        s.dataTypes = jQuery.trim(s.dataType || "*").toLowerCase().match(core_rnotwhite) || [""];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // A cross-domain request is in order when we have a protocol:host:port mismatch[m[41m[m
[32m+[m[32m        if (s.crossDomain == null) {[m[41m[m
[32m+[m[32m            parts = rurl.exec(s.url.toLowerCase());[m[41m[m
[32m+[m[32m            s.crossDomain = !!(parts &&[m[41m[m
[32m+[m				[32m(parts[1] !== ajaxLocParts[1] || parts[2] !== ajaxLocParts[2] ||[m[41m[m
[32m+[m					[32m(parts[3] || (parts[1] === "http:" ? 80 : 443)) !=[m[41m[m
[32m+[m						[32m(ajaxLocParts[3] || (ajaxLocParts[1] === "http:" ? 80 : 443)))[m[41m[m
[32m+[m			[32m);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Convert data if not already a string[m[41m[m
[32m+[m[32m        if (s.data && s.processData && typeof s.data !== "string") {[m[41m[m
[32m+[m[32m            s.data = jQuery.param(s.data, s.traditional);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Apply prefilters[m[41m[m
[32m+[m[32m        inspectPrefiltersOrTransports(prefilters, s, options, jqXHR);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If request was aborted inside a prefilter, stop there[m[41m[m
[32m+[m[32m        if (state === 2) {[m[41m[m
[32m+[m[32m            return jqXHR;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // We can fire global events as of now if asked to[m[41m[m
[32m+[m[32m        fireGlobals = s.global;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Watch for a new set of requests[m[41m[m
[32m+[m[32m        if (fireGlobals && jQuery.active++ === 0) {[m[41m[m
[32m+[m[32m            jQuery.event.trigger("ajaxStart");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Uppercase the type[m[41m[m
[32m+[m[32m        s.type = s.type.toUpperCase();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Determine if request has content[m[41m[m
[32m+[m[32m        s.hasContent = !rnoContent.test(s.type);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Save the URL in case we're toying with the If-Modified-Since[m[41m[m
[32m+[m[32m        // and/or If-None-Match header later on[m[41m[m
[32m+[m[32m        cacheURL = s.url;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // More options handling for requests with no content[m[41m[m
[32m+[m[32m        if (!s.hasContent) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // If data is available, append data to url[m[41m[m
[32m+[m[32m            if (s.data) {[m[41m[m
[32m+[m[32m                cacheURL = (s.url += (ajax_rquery.test(cacheURL) ? "&" : "?") + s.data);[m[41m[m
[32m+[m[32m                // #9682: remove data so that it's not used in an eventual retry[m[41m[m
[32m+[m[32m                delete s.data;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Add anti-cache in url if needed[m[41m[m
[32m+[m[32m            if (s.cache === false) {[m[41m[m
[32m+[m[32m                s.url = rts.test(cacheURL) ?[m[41m[m
[32m+[m[41m[m
[32m+[m					[32m// If there is already a '_' parameter, set its value[m[41m[m
[32m+[m					[32mcacheURL.replace(rts, "$1_=" + ajax_nonce++) :[m[41m[m
[32m+[m[41m[m
[32m+[m					[32m// Otherwise add one to the end[m[41m[m
[32m+[m					[32mcacheURL + (ajax_rquery.test(cacheURL) ? "&" : "?") + "_=" + ajax_nonce++;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Set the If-Modified-Since and/or If-None-Match header, if in ifModified mode.[m[41m[m
[32m+[m[32m        if (s.ifModified) {[m[41m[m
[32m+[m[32m            if (jQuery.lastModified[cacheURL]) {[m[41m[m
[32m+[m[32m                jqXHR.setRequestHeader("If-Modified-Since", jQuery.lastModified[cacheURL]);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            if (jQuery.etag[cacheURL]) {[m[41m[m
[32m+[m[32m                jqXHR.setRequestHeader("If-None-Match", jQuery.etag[cacheURL]);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Set the correct header, if data is being sent[m[41m[m
[32m+[m[32m        if (s.data && s.hasContent && s.contentType !== false || options.contentType) {[m[41m[m
[32m+[m[32m            jqXHR.setRequestHeader("Content-Type", s.contentType);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Set the Accepts header for the server, depending on the dataType[m[41m[m
[32m+[m[32m        jqXHR.setRequestHeader([m[41m[m
[32m+[m			[32m"Accept",[m[41m[m
[32m+[m			[32ms.dataTypes[0] && s.accepts[s.dataTypes[0]] ?[m[41m[m
[32m+[m				[32ms.accepts[s.dataTypes[0]] + (s.dataTypes[0] !== "*" ? ", " + allTypes + "; q=0.01" : "") :[m[41m[m
[32m+[m				[32ms.accepts["*"][m[41m[m
[32m+[m		[32m);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Check for headers option[m[41m[m
[32m+[m[32m        for (i in s.headers) {[m[41m[m
[32m+[m[32m            jqXHR.setRequestHeader(i, s.headers[i]);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Allow custom headers/mimetypes and early abort[m[41m[m
[32m+[m[32m        if (s.beforeSend && (s.beforeSend.call(callbackContext, jqXHR, s) === false || state === 2)) {[m[41m[m
[32m+[m[32m            // Abort if not done already and return[m[41m[m
[32m+[m[32m            return jqXHR.abort();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // aborting is no longer a cancellation[m[41m[m
[32m+[m[32m        strAbort = "abort";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Install callbacks on deferreds[m[41m[m
[32m+[m[32m        for (i in { success: 1, error: 1, complete: 1 }) {[m[41m[m
[32m+[m[32m            jqXHR[i](s[i]);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Get transport[m[41m[m
[32m+[m[32m        transport = inspectPrefiltersOrTransports(transports, s, options, jqXHR);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If no transport, we auto-abort[m[41m[m
[32m+[m[32m        if (!transport) {[m[41m[m
[32m+[m[32m            done(-1, "No Transport");[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            jqXHR.readyState = 1;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Send global event[m[41m[m
[32m+[m[32m            if (fireGlobals) {[m[41m[m
[32m+[m[32m                globalEventContext.trigger("ajaxSend", [jqXHR, s]);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            // Timeout[m[41m[m
[32m+[m[32m            if (s.async && s.timeout > 0) {[m[41m[m
[32m+[m[32m                timeoutTimer = setTimeout(function () {[m[41m[m
[32m+[m[32m                    jqXHR.abort("timeout");[m[41m[m
[32m+[m[32m                }, s.timeout);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            try {[m[41m[m
[32m+[m[32m                state = 1;[m[41m[m
[32m+[m[32m                transport.send(requestHeaders, done);[m[41m[m
[32m+[m[32m            } catch (e) {[m[41m[m
[32m+[m[32m                // Propagate exception as error if not done[m[41m[m
[32m+[m[32m                if (state < 2) {[m[41m[m
[32m+[m[32m                    done(-1, e);[m[41m[m
[32m+[m[32m                    // Simply rethrow otherwise[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    throw e;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Callback for when everything is done[m[41m[m
[32m+[m[32m        function done(status, nativeStatusText, responses, headers) {[m[41m[m
[32m+[m[32m            var isSuccess, success, error, response, modified,[m[41m[m
[32m+[m				[32mstatusText = nativeStatusText;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Called once[m[41m[m
[32m+[m[32m            if (state === 2) {[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // State is "done" now[m[41m[m
[32m+[m[32m            state = 2;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Clear timeout if it exists[m[41m[m
[32m+[m[32m            if (timeoutTimer) {[m[41m[m
[32m+[m[32m                clearTimeout(timeoutTimer);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Dereference transport for early garbage collection[m[41m[m
[32m+[m[32m            // (no matter how long the jqXHR object will be used)[m[41m[m
[32m+[m[32m            transport = undefined;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Cache response headers[m[41m[m
[32m+[m[32m            responseHeadersString = headers || "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Set readyState[m[41m[m
[32m+[m[32m            jqXHR.readyState = status > 0 ? 4 : 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Get response data[m[41m[m
[32m+[m[32m            if (responses) {[m[41m[m
[32m+[m[32m                response = ajaxHandleResponses(s, jqXHR, responses);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // If successful, handle type chaining[m[41m[m
[32m+[m[32m            if (status >= 200 && status < 300 || status === 304) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Set the If-Modified-Since and/or If-None-Match header, if in ifModified mode.[m[41m[m
[32m+[m[32m                if (s.ifModified) {[m[41m[m
[32m+[m[32m                    modified = jqXHR.getResponseHeader("Last-Modified");[m[41m[m
[32m+[m[32m                    if (modified) {[m[41m[m
[32m+[m[32m                        jQuery.lastModified[cacheURL] = modified;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    modified = jqXHR.getResponseHeader("etag");[m[41m[m
[32m+[m[32m                    if (modified) {[m[41m[m
[32m+[m[32m                        jQuery.etag[cacheURL] = modified;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // if no content[m[41m[m
[32m+[m[32m                if (status === 204) {[m[41m[m
[32m+[m[32m                    isSuccess = true;[m[41m[m
[32m+[m[32m                    statusText = "nocontent";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // if not modified[m[41m[m
[32m+[m[32m                } else if (status === 304) {[m[41m[m
[32m+[m[32m                    isSuccess = true;[m[41m[m
[32m+[m[32m                    statusText = "notmodified";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // If we have data, let's convert it[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    isSuccess = ajaxConvert(s, response);[m[41m[m
[32m+[m[32m                    statusText = isSuccess.state;[m[41m[m
[32m+[m[32m                    success = isSuccess.data;[m[41m[m
[32m+[m[32m                    error = isSuccess.error;[m[41m[m
[32m+[m[32m                    isSuccess = !error;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                // We extract error from statusText[m[41m[m
[32m+[m[32m                // then normalize statusText and status for non-aborts[m[41m[m
[32m+[m[32m                error = statusText;[m[41m[m
[32m+[m[32m                if (status || !statusText) {[m[41m[m
[32m+[m[32m                    statusText = "error";[m[41m[m
[32m+[m[32m                    if (status < 0) {[m[41m[m
[32m+[m[32m                        status = 0;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Set data for the fake xhr object[m[41m[m
[32m+[m[32m            jqXHR.status = status;[m[41m[m
[32m+[m[32m            jqXHR.statusText = (nativeStatusText || statusText) + "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Success/Error[m[41m[m
[32m+[m[32m            if (isSuccess) {[m[41m[m
[32m+[m[32m                deferred.resolveWith(callbackContext, [success, statusText, jqXHR]);[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                deferred.rejectWith(callbackContext, [jqXHR, statusText, error]);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Status-dependent callbacks[m[41m[m
[32m+[m[32m            jqXHR.statusCode(statusCode);[m[41m[m
[32m+[m[32m            statusCode = undefined;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (fireGlobals) {[m[41m[m
[32m+[m[32m                globalEventContext.trigger(isSuccess ? "ajaxSuccess" : "ajaxError",[m[41m[m
[32m+[m					[32m[jqXHR, s, isSuccess ? success : error]);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Complete[m[41m[m
[32m+[m[32m            completeDeferred.fireWith(callbackContext, [jqXHR, statusText]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (fireGlobals) {[m[41m[m
[32m+[m[32m                globalEventContext.trigger("ajaxComplete", [jqXHR, s]);[m[41m[m
[32m+[m[32m                // Handle the global AJAX counter[m[41m[m
[32m+[m[32m                if (!(--jQuery.active)) {[m[41m[m
[32m+[m[32m                    jQuery.event.trigger("ajaxStop");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jqXHR;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.ajaxPrefilter = function (dataTypeExpression, func) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Handle custom Ajax options or modify existing options before each request is sent and before they are processed by $.ajax().[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="dataTypeExpression" type="String">[m[41m[m
[32m+[m[32m        ///     An optional string containing one or more space-separated dataTypes[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="func" type="Function">[m[41m[m
[32m+[m[32m        ///     A handler to set default values for future Ajax requests.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="undefined" />[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (typeof dataTypeExpression !== "string") {[m[41m[m
[32m+[m[32m            func = dataTypeExpression;[m[41m[m
[32m+[m[32m            dataTypeExpression = "*";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var dataType,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mdataTypes = dataTypeExpression.toLowerCase().match(core_rnotwhite) || [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (jQuery.isFunction(func)) {[m[41m[m
[32m+[m[32m            // For each dataType in the dataTypeExpression[m[41m[m
[32m+[m[32m            while ((dataType = dataTypes[i++])) {[m[41m[m
[32m+[m[32m                // Prepend if requested[m[41m[m
[32m+[m[32m                if (dataType[0] === "+") {[m[41m[m
[32m+[m[32m                    dataType = dataType.slice(1) || "*";[m[41m[m
[32m+[m[32m                    (structure[dataType] = structure[dataType] || []).unshift(func);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Otherwise append[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    (structure[dataType] = structure[dataType] || []).push(func);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.ajaxSettings = {[m[41m[m
[32m+[m[32m        "url": 'http://localhost:25812/',[m[41m[m
[32m+[m[32m        "type": 'GET',[m[41m[m
[32m+[m[32m        "isLocal": false,[m[41m[m
[32m+[m[32m        "global": true,[m[41m[m
[32m+[m[32m        "processData": true,[m[41m[m
[32m+[m[32m        "async": true,[m[41m[m
[32m+[m[32m        "contentType": 'application/x-www-form-urlencoded; charset=UTF-8',[m[41m[m
[32m+[m[32m        "accepts": {},[m[41m[m
[32m+[m[32m        "contents": {},[m[41m[m
[32m+[m[32m        "responseFields": {},[m[41m[m
[32m+[m[32m        "converters": {},[m[41m[m
[32m+[m[32m        "flatOptions": {},[m[41m[m
[32m+[m[32m        "jsonp": 'callback'[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.ajaxSetup = function (target, settings) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Set default values for future Ajax requests.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="target" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     A set of key/value pairs that configure the default Ajax request. All options are optional.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return settings ?[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// Building a settings object[m[41m[m
[32m+[m			[32majaxExtend(ajaxExtend(target, jQuery.ajaxSettings), settings) :[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// Extending ajaxSettings[m[41m[m
[32m+[m			[32majaxExtend(jQuery.ajaxSettings, target);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.ajaxTransport = function (dataTypeExpression, func) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Creates an object that handles the actual transmission of Ajax data.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="dataTypeExpression" type="String">[m[41m[m
[32m+[m[32m        ///     A string identifying the data type to use[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="func" type="Function">[m[41m[m
[32m+[m[32m        ///     A handler to return the new transport object to use with the data type provided in the first argument.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="undefined" />[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (typeof dataTypeExpression !== "string") {[m[41m[m
[32m+[m[32m            func = dataTypeExpression;[m[41m[m
[32m+[m[32m            dataTypeExpression = "*";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var dataType,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mdataTypes = dataTypeExpression.toLowerCase().match(core_rnotwhite) || [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (jQuery.isFunction(func)) {[m[41m[m
[32m+[m[32m            // For each dataType in the dataTypeExpression[m[41m[m
[32m+[m[32m            while ((dataType = dataTypes[i++])) {[m[41m[m
[32m+[m[32m                // Prepend if requested[m[41m[m
[32m+[m[32m                if (dataType[0] === "+") {[m[41m[m
[32m+[m[32m                    dataType = dataType.slice(1) || "*";[m[41m[m
[32m+[m[32m                    (structure[dataType] = structure[dataType] || []).unshift(func);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Otherwise append[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    (structure[dataType] = structure[dataType] || []).push(func);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.attr = function (elem, name, value) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var hooks, notxml, ret,[m[41m[m
[32m+[m			[32mnType = elem.nodeType;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // don't get/set attributes on text, comment and attribute nodes[m[41m[m
[32m+[m[32m        if (!elem || nType === 3 || nType === 8 || nType === 2) {[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Fallback to prop when attributes are not supported[m[41m[m
[32m+[m[32m        if (typeof elem.getAttribute === core_strundefined) {[m[41m[m
[32m+[m[32m            return jQuery.prop(elem, name, value);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        notxml = nType !== 1 || !jQuery.isXMLDoc(elem);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // All attributes are lowercase[m[41m[m
[32m+[m[32m        // Grab necessary hook if one is defined[m[41m[m
[32m+[m[32m        if (notxml) {[m[41m[m
[32m+[m[32m            name = name.toLowerCase();[m[41m[m
[32m+[m[32m            hooks = jQuery.attrHooks[name] || (rboolean.test(name) ? boolHook : nodeHook);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (value !== undefined) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (value === null) {[m[41m[m
[32m+[m[32m                jQuery.removeAttr(elem, name);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            } else if (hooks && notxml && "set" in hooks && (ret = hooks.set(elem, value, name)) !== undefined) {[m[41m[m
[32m+[m[32m                return ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                elem.setAttribute(name, value + "");[m[41m[m
[32m+[m[32m                return value;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        } else if (hooks && notxml && "get" in hooks && (ret = hooks.get(elem, name)) !== null) {[m[41m[m
[32m+[m[32m            return ret;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // In IE9+, Flash objects don't have .getAttribute (#12945)[m[41m[m
[32m+[m[32m            // Support: IE9+[m[41m[m
[32m+[m[32m            if (typeof elem.getAttribute !== core_strundefined) {[m[41m[m
[32m+[m[32m                ret = elem.getAttribute(name);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Non-existent attributes return null, we normalize to undefined[m[41m[m
[32m+[m[32m            return ret == null ?[m[41m[m
[32m+[m[32m                undefined :[m[41m[m
[32m+[m				[32mret;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.attrHooks = { "type": {} };[m[41m[m
[32m+[m[32m    jQuery.buildFragment = function (elems, context, scripts, selection) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var j, elem, contains,[m[41m[m
[32m+[m			[32mtmp, tag, tbody, wrap,[m[41m[m
[32m+[m			[32ml = elems.length,[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m// Ensure a safe fragment[m[41m[m
[32m+[m			[32msafe = createSafeFragment(context),[m[41m[m
[32m+[m[41m[m
[32m+[m			[32mnodes = [],[m[41m[m
[32m+[m			[32mi = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; i < l; i++) {[m[41m[m
[32m+[m[32m            elem = elems[i];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (elem || elem === 0) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Add nodes directly[m[41m[m
[32m+[m[32m                if (jQuery.type(elem) === "object") {[m[41m[m
[32m+[m[32m                    jQuery.merge(nodes, elem.nodeType ? [elem] : elem);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Convert non-html into a text node[m[41m[m
[32m+[m[32m                } else if (!rhtml.test(elem)) {[m[41m[m
[32m+[m[32m                    nodes.push(context.createTextNode(elem));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Convert html into DOM nodes[m[41m[m
[32m+[m[32m                } else {[m[41m[m
[32m+[m[32m                    tmp = tmp || safe.appendChild(context.createElement("div"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Deserialize a standard representation[m[41m[m
[32m+[m[32m                    tag = (rtagName.exec(elem) || ["", ""])[1].toLowerCase();[m[41m[m
[32m+[m[32m                    wrap = wrapMap[tag] || wrapMap._default;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    tmp.innerHTML = wrap[1] + elem.replace(rxhtmlTag, "<$1></$2>") + wrap[2];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Descend through wrappers to the right content[m[41m[m
[32m+[m[32m                    j = wrap[0];[m[41m[m
[32m+[m[32m                    while (j--) {[m[41m[m
[32m+[m[32m                        tmp = tmp.lastChild;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Manually add leading whitespace removed by IE[m[41m[m
[32m+[m[32m                    if (!jQuery.support.leadingWhitespace && rleadingWhitespace.test(elem)) {[m[41m[m
[32m+[m[32m                        nodes.push(context.createTextNode(rleadingWhitespace.exec(elem)[0]));[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Remove IE's autoinserted <tbody> from table fragments[m[41m[m
[32m+[m[32m                    if (!jQuery.support.tbody) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        // String was a <table>, *may* have spurious <tbody>[m[41m[m
[32m+[m[32m                        elem = tag === "table" && !rtbody.test(elem) ?[m[41m[m
[32m+[m							[32mtmp.firstChild :[m[41m[m
[32m+[m[41m[m
[32m+[m							[32m// String was a bare <thead> or <tfoot>[m[41m[m
[32m+[m							[32mwrap[1] === "<table>" && !rtbody.test(elem) ?[m[41m[m
[32m+[m[32m                            tmp :[m[41m[m
[32m+[m								[32m0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        j = elem && elem.childNodes.length;[m[41m[m
[32m+[m[32m                        while (j--) {[m[41m[m
[32m+[m[32m                            if (jQuery.nodeName((tbody = elem.childNodes[j]), "tbody") && !tbody.childNodes.length) {[m[41m[m
[32m+[m[32m                                elem.removeChild(tbody);[m[41m[m
[32m+[m[32m                            }[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    jQuery.merge(nodes, tmp.childNodes);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Fix #12392 for WebKit and IE > 9[m[41m[m
[32m+[m[32m                    tmp.textContent = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Fix #12392 for oldIE[m[41m[m
[32m+[m[32m                    while (tmp.firstChild) {[m[41m[m
[32m+[m[32m                        tmp.removeChild(tmp.firstChild);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Remember the top-level container for proper cleanup[m[41m[m
[32m+[m[32m                    tmp = safe.lastChild;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Fix #11356: Clear elements from fragment[m[41m[m
[32m+[m[32m        if (tmp) {[m[41m[m
[32m+[m[32m            safe.removeChild(tmp);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Reset defaultChecked for any radios and checkboxes[m[41m[m
[32m+[m[32m        // about to be appended to the DOM in IE 6/7 (#8060)[m[41m[m
[32m+[m[32m        if (!jQuery.support.appendChecked) {[m[41m[m
[32m+[m[32m            jQuery.grep(getAll(nodes, "input"), fixDefaultChecked);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        i = 0;[m[41m[m
[32m+[m[32m        while ((elem = nodes[i++])) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // #4087 - If origin and destination elements are the same, and this is[m[41m[m
[32m+[m[32m            // that element, do not do anything[m[41m[m
[32m+[m[32m            if (selection && jQuery.inArray(elem, selection) !== -1) {[m[41m[m
[32m+[m[32m                continue;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            contains = jQuery.contains(elem.ownerDocument, elem);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Append to fragment[m[41m[m
[32m+[m[32m            tmp = getAll(safe.appendChild(elem), "script");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Preserve script evaluation history[m[41m[m
[32m+[m[32m            if (contains) {[m[41m[m
[32m+[m[32m                setGlobalEval(tmp);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Capture executables[m[41m[m
[32m+[m[32m            if (scripts) {[m[41m[m
[32m+[m[32m                j = 0;[m[41m[m
[32m+[m[32m                while ((elem = tmp[j++])) {[m[41m[m
[32m+[m[32m                    if (rscriptType.test(elem.type || "")) {[m[41m[m
[32m+[m[32m                        scripts.push(elem);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        tmp = null;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return safe;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.cache = {};[m[41m[m
[32m+[m[32m    jQuery.camelCase = function (string) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return string.replace(rmsPrefix, "ms-").replace(rdashAlpha, fcamelCase);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.cleanData = function (elems, /* internal */ acceptData) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var elem, type, id, data,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32minternalKey = jQuery.expando,[m[41m[m
[32m+[m			[32mcache = jQuery.cache,[m[41m[m
[32m+[m			[32mdeleteExpando = jQuery.support.deleteExpando,[m[41m[m
[32m+[m			[32mspecial = jQuery.event.special;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; (elem = elems[i]) != null; i++) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (acceptData || jQuery.acceptData(elem)) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                id = elem[internalKey];[m[41m[m
[32m+[m[32m                data = id && cache[id];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (data) {[m[41m[m
[32m+[m[32m                    if (data.events) {[m[41m[m
[32m+[m[32m                        for (type in data.events) {[m[41m[m
[32m+[m[32m                            if (special[type]) {[m[41m[m
[32m+[m[32m                                jQuery.event.remove(elem, type);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                                // This is a shortcut to avoid jQuery.event.remove's overhead[m[41m[m
[32m+[m[32m                            } else {[m[41m[m
[32m+[m[32m                                jQuery.removeEvent(elem, type, data.handle);[m[41m[m
[32m+[m[32m                            }[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Remove cache only if it was not already removed by jQuery.event.remove[m[41m[m
[32m+[m[32m                    if (cache[id]) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        delete cache[id];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        // IE does not allow us to delete expando properties from nodes,[m[41m[m
[32m+[m[32m                        // nor does it have a removeAttribute function on Document nodes;[m[41m[m
[32m+[m[32m                        // we must handle all of these cases[m[41m[m
[32m+[m[32m                        if (deleteExpando) {[m[41m[m
[32m+[m[32m                            delete elem[internalKey];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        } else if (typeof elem.removeAttribute !== core_strundefined) {[m[41m[m
[32m+[m[32m                            elem.removeAttribute(internalKey);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        } else {[m[41m[m
[32m+[m[32m                            elem[internalKey] = null;[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        core_deletedIds.push(id);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.clone = function (elem, dataAndEvents, deepDataAndEvents) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var destElements, node, clone, i, srcElements,[m[41m[m
[32m+[m			[32minPage = jQuery.contains(elem.ownerDocument, elem);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (jQuery.support.html5Clone || jQuery.isXMLDoc(elem) || !rnoshimcache.test("<" + elem.nodeName + ">")) {[m[41m[m
[32m+[m[32m            clone = elem.cloneNode(true);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // IE<=8 does not properly clone detached, unknown element nodes[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            fragmentDiv.innerHTML = elem.outerHTML;[m[41m[m
[32m+[m[32m            fragmentDiv.removeChild(clone = fragmentDiv.firstChild);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if ((!jQuery.support.noCloneEvent || !jQuery.support.noCloneChecked) &&[m[41m[m
[32m+[m				[32m(elem.nodeType === 1 || elem.nodeType === 11) && !jQuery.isXMLDoc(elem)) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // We eschew Sizzle here for performance reasons: http://jsperf.com/getall-vs-sizzle/2[m[41m[m
[32m+[m[32m            destElements = getAll(clone);[m[41m[m
[32m+[m[32m            srcElements = getAll(elem);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Fix all IE cloning issues[m[41m[m
[32m+[m[32m            for (i = 0; (node = srcElements[i]) != null; ++i) {[m[41m[m
[32m+[m[32m                // Ensure that the destination node is not null; Fixes #9587[m[41m[m
[32m+[m[32m                if (destElements[i]) {[m[41m[m
[32m+[m[32m                    fixCloneNodeIssues(node, destElements[i]);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Copy the events from the original to the clone[m[41m[m
[32m+[m[32m        if (dataAndEvents) {[m[41m[m
[32m+[m[32m            if (deepDataAndEvents) {[m[41m[m
[32m+[m[32m                srcElements = srcElements || getAll(elem);[m[41m[m
[32m+[m[32m                destElements = destElements || getAll(clone);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                for (i = 0; (node = srcElements[i]) != null; i++) {[m[41m[m
[32m+[m[32m                    cloneCopyEvent(node, destElements[i]);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                cloneCopyEvent(elem, clone);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Preserve script evaluation history[m[41m[m
[32m+[m[32m        destElements = getAll(clone, "script");[m[41m[m
[32m+[m[32m        if (destElements.length > 0) {[m[41m[m
[32m+[m[32m            setGlobalEval(destElements, !inPage && getAll(elem, "script"));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        destElements = srcElements = node = null;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Return the cloned set[m[41m[m
[32m+[m[32m        return clone;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.contains = function (context, elem) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Check to see if a DOM element is a descendant of another DOM element.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="context" domElement="true">[m[41m[m
[32m+[m[32m        ///     The DOM element that may contain the other element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="elem" domElement="true">[m[41m[m
[32m+[m[32m        ///     The DOM element that may be contained by (a descendant of) the other element.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Set document vars if needed[m[41m[m
[32m+[m[32m        if ((context.ownerDocument || context) !== document) {[m[41m[m
[32m+[m[32m            setDocument(context);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return contains(context, elem);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.css = function (elem, name, extra, styles) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var num, val, hooks,[m[41m[m
[32m+[m			[32morigName = jQuery.camelCase(name);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Make sure that we're working with the right name[m[41m[m
[32m+[m[32m        name = jQuery.cssProps[origName] || (jQuery.cssProps[origName] = vendorPropName(elem.style, origName));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // gets hook for the prefixed version[m[41m[m
[32m+[m[32m        // followed by the unprefixed version[m[41m[m
[32m+[m[32m        hooks = jQuery.cssHooks[name] || jQuery.cssHooks[origName];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If a hook was provided get the computed value from there[m[41m[m
[32m+[m[32m        if (hooks && "get" in hooks) {[m[41m[m
[32m+[m[32m            val = hooks.get(elem, true, extra);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Otherwise, if a way to get the computed value exists, use that[m[41m[m
[32m+[m[32m        if (val === undefined) {[m[41m[m
[32m+[m[32m            val = curCSS(elem, name, styles);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        //convert "normal" to computed value[m[41m[m
[32m+[m[32m        if (val === "normal" && name in cssNormalTransform) {[m[41m[m
[32m+[m[32m            val = cssNormalTransform[name];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Return, converting to number if forced or a qualifier was provided and val looks numeric[m[41m[m
[32m+[m[32m        if (extra === "" || extra) {[m[41m[m
[32m+[m[32m            num = parseFloat(val);[m[41m[m
[32m+[m[32m            return extra === true || jQuery.isNumeric(num) ? num || 0 : val;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return val;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.cssHooks = {[m[41m[m
[32m+[m[32m        "opacity": {},[m[41m[m
[32m+[m[32m        "height": {},[m[41m[m
[32m+[m[32m        "width": {},[m[41m[m
[32m+[m[32m        "margin": {},[m[41m[m
[32m+[m[32m        "padding": {},[m[41m[m
[32m+[m[32m        "borderWidth": {}[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.cssNumber = {[m[41m[m
[32m+[m[32m        "columnCount": true,[m[41m[m
[32m+[m[32m        "fillOpacity": true,[m[41m[m
[32m+[m[32m        "fontWeight": true,[m[41m[m
[32m+[m[32m        "lineHeight": true,[m[41m[m
[32m+[m[32m        "opacity": true,[m[41m[m
[32m+[m[32m        "orphans": true,[m[41m[m
[32m+[m[32m        "widows": true,[m[41m[m
[32m+[m[32m        "zIndex": true,[m[41m[m
[32m+[m[32m        "zoom": true[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.cssProps = {[m[41m[m
[32m+[m[32m        "float": 'cssFloat',[m[41m[m
[32m+[m[32m        "display": 'display',[m[41m[m
[32m+[m[32m        "visibility": 'visibility'[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.data = function (elem, name, data) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     1: Store arbitrary data associated with the specified element. Returns the value that was set.[m[41m[m
[32m+[m[32m        ///     &#10;    1.1 - jQuery.data(element, key, value)[m[41m[m
[32m+[m[32m        ///     &#10;2: Returns value at named data store for the element, as set by jQuery.data(element, name, value), or the full data store for the element.[m[41m[m
[32m+[m[32m        ///     &#10;    2.1 - jQuery.data(element, key)[m[41m [m
[32m+[m[32m        ///     &#10;    2.2 - jQuery.data(element)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elem" domElement="true">[m[41m[m
[32m+[m[32m        ///     The DOM element to associate with the data.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="name" type="String">[m[41m[m
[32m+[m[32m        ///     A string naming the piece of data to set.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="Object">[m[41m[m
[32m+[m[32m        ///     The new data value.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Object" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return internalData(elem, name, data);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.dequeue = function (elem, type) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Execute the next function on the queue for the matched element.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elem" domElement="true">[m[41m[m
[32m+[m[32m        ///     A DOM element from which to remove and execute a queued function.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="type" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the name of the queue. Defaults to fx, the standard effects queue.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="undefined" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        type = type || "fx";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var queue = jQuery.queue(elem, type),[m[41m[m
[32m+[m			[32mstartLength = queue.length,[m[41m[m
[32m+[m			[32mfn = queue.shift(),[m[41m[m
[32m+[m			[32mhooks = jQuery._queueHooks(elem, type),[m[41m[m
[32m+[m			[32mnext = function () {[m[41m[m
[32m+[m			[32m    jQuery.dequeue(elem, type);[m[41m[m
[32m+[m			[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If the fx queue is dequeued, always remove the progress sentinel[m[41m[m
[32m+[m[32m        if (fn === "inprogress") {[m[41m[m
[32m+[m[32m            fn = queue.shift();[m[41m[m
[32m+[m[32m            startLength--;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        hooks.cur = fn;[m[41m[m
[32m+[m[32m        if (fn) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Add a progress sentinel to prevent the fx queue from being[m[41m[m
[32m+[m[32m            // automatically dequeued[m[41m[m
[32m+[m[32m            if (type === "fx") {[m[41m[m
[32m+[m[32m                queue.unshift("inprogress");[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // clear up the last queue stop function[m[41m[m
[32m+[m[32m            delete hooks.stop;[m[41m[m
[32m+[m[32m            fn.call(elem, next, hooks);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!startLength && hooks) {[m[41m[m
[32m+[m[32m            hooks.empty.fire();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.dir = function (elem, dir, until) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var matched = [],[m[41m[m
[32m+[m			[32mcur = elem[dir];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        while (cur && cur.nodeType !== 9 && (until === undefined || cur.nodeType !== 1 || !jQuery(cur).is(until))) {[m[41m[m
[32m+[m[32m            if (cur.nodeType === 1) {[m[41m[m
[32m+[m[32m                matched.push(cur);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            cur = cur[dir];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return matched;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.each = function (obj, callback, args) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     A generic iterator function, which can be used to seamlessly iterate over both objects and arrays. Arrays and array-like objects with a length property (such as a function's arguments object) are iterated by numeric index, from 0 to length-1. Other objects are iterated via their named properties.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="obj" type="Object">[m[41m[m
[32m+[m[32m        ///     The object or array to iterate over.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     The function that will be executed on every object.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Object" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var value,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mlength = obj.length,[m[41m[m
[32m+[m			[32misArray = isArraylike(obj);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (args) {[m[41m[m
[32m+[m[32m            if (isArray) {[m[41m[m
[32m+[m[32m                for (; i < length; i++) {[m[41m[m
[32m+[m[32m                    value = callback.apply(obj[i], args);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if (value === false) {[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                for (i in obj) {[m[41m[m
[32m+[m[32m                    value = callback.apply(obj[i], args);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if (value === false) {[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // A special, fast, case for the most common use of each[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            if (isArray) {[m[41m[m
[32m+[m[32m                for (; i < length; i++) {[m[41m[m
[32m+[m[32m                    value = callback.call(obj[i], i, obj[i]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if (value === false) {[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                for (i in obj) {[m[41m[m
[32m+[m[32m                    value = callback.call(obj[i], i, obj[i]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if (value === false) {[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return obj;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.easing = {};[m[41m[m
[32m+[m[32m    jQuery.error = function (msg) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Takes a string and throws an exception containing it.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="msg" type="String">[m[41m[m
[32m+[m[32m        ///     The message to send out.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        throw new Error(msg);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.etag = {};[m[41m[m
[32m+[m[32m    jQuery.event = {[m[41m[m
[32m+[m[32m        "global": {},[m[41m[m
[32m+[m[32m        "props": ['altKey', 'bubbles', 'cancelable', 'ctrlKey', 'currentTarget', 'eventPhase', 'metaKey', 'relatedTarget', 'shiftKey', 'target', 'timeStamp', 'view', 'which'],[m[41m[m
[32m+[m[32m        "fixHooks": {},[m[41m[m
[32m+[m[32m        "keyHooks": {},[m[41m[m
[32m+[m[32m        "mouseHooks": {},[m[41m[m
[32m+[m[32m        "special": {},[m[41m[m
[32m+[m[32m        "triggered": {}[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.expr = {[m[41m[m
[32m+[m[32m        "cacheLength": 50,[m[41m[m
[32m+[m[32m        "match": {},[m[41m[m
[32m+[m[32m        "find": {},[m[41m[m
[32m+[m[32m        "relative": {},[m[41m[m
[32m+[m[32m        "preFilter": {},[m[41m[m
[32m+[m[32m        "filter": {},[m[41m[m
[32m+[m[32m        "pseudos": {},[m[41m[m
[32m+[m[32m        "filters": {},[m[41m[m
[32m+[m[32m        "setFilters": {},[m[41m[m
[32m+[m[32m        "attrHandle": {},[m[41m[m
[32m+[m[32m        ":": {}[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.extend = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Merge the contents of two or more objects together into the first object.[m[41m[m
[32m+[m[32m        ///     &#10;1 - jQuery.extend(target, object1, objectN)[m[41m [m
[32m+[m[32m        ///     &#10;2 - jQuery.extend(deep, target, object1, objectN)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="" type="Boolean">[m[41m[m
[32m+[m[32m        ///     If true, the merge becomes recursive (aka. deep copy).[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="" type="Object">[m[41m[m
[32m+[m[32m        ///     The object to extend. It will receive the new properties.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="" type="Object">[m[41m[m
[32m+[m[32m        ///     An object containing additional properties to merge in.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="" type="Object">[m[41m[m
[32m+[m[32m        ///     Additional objects containing properties to merge in.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Object" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var src, copyIsArray, copy, name, options, clone,[m[41m[m
[32m+[m[32m            target = arguments[0] || {},[m[41m[m
[32m+[m[32m            i = 1,[m[41m[m
[32m+[m[32m            length = arguments.length,[m[41m[m
[32m+[m[32m            deep = false;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Handle a deep copy situation[m[41m[m
[32m+[m[32m        if (typeof target === "boolean") {[m[41m[m
[32m+[m[32m            deep = target;[m[41m[m
[32m+[m[32m            target = arguments[1] || {};[m[41m[m
[32m+[m[32m            // skip the boolean and the target[m[41m[m
[32m+[m[32m            i = 2;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Handle case when target is a string or something (possible in deep copy)[m[41m[m
[32m+[m[32m        if (typeof target !== "object" && !jQuery.isFunction(target)) {[m[41m[m
[32m+[m[32m            target = {};[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // extend jQuery itself if only one argument is passed[m[41m[m
[32m+[m[32m        if (length === i) {[m[41m[m
[32m+[m[32m            target = this;[m[41m[m
[32m+[m[32m            --i;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (; i < length; i++) {[m[41m[m
[32m+[m[32m            // Only deal with non-null/undefined values[m[41m[m
[32m+[m[32m            if ((options = arguments[i]) != null) {[m[41m[m
[32m+[m[32m                // Extend the base object[m[41m[m
[32m+[m[32m                for (name in options) {[m[41m[m
[32m+[m[32m                    src = target[name];[m[41m[m
[32m+[m[32m                    copy = options[name];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Prevent never-ending loop[m[41m[m
[32m+[m[32m                    if (target === copy) {[m[41m[m
[32m+[m[32m                        continue;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Recurse if we're merging plain objects or arrays[m[41m[m
[32m+[m[32m                    if (deep && copy && (jQuery.isPlainObject(copy) || (copyIsArray = jQuery.isArray(copy)))) {[m[41m[m
[32m+[m[32m                        if (copyIsArray) {[m[41m[m
[32m+[m[32m                            copyIsArray = false;[m[41m[m
[32m+[m[32m                            clone = src && jQuery.isArray(src) ? src : [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        } else {[m[41m[m
[32m+[m[32m                            clone = src && jQuery.isPlainObject(src) ? src : {};[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        // Never move original objects, clone them[m[41m[m
[32m+[m[32m                        target[name] = jQuery.extend(deep, clone, copy);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        // Don't bring in undefined values[m[41m[m
[32m+[m[32m                    } else if (copy !== undefined) {[m[41m[m
[32m+[m[32m                        target[name] = copy;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Return the modified object[m[41m[m
[32m+[m[32m        return target;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.filter = function (expr, elems, not) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (not) {[m[41m[m
[32m+[m[32m            expr = ":not(" + expr + ")";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return elems.length === 1 ?[m[41m[m
[32m+[m			[32mjQuery.find.matchesSelector(elems[0], expr) ? [elems[0]] : [] :[m[41m[m
[32m+[m			[32mjQuery.find.matches(expr, elems);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.find = function Sizzle(selector, context, results, seed) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var match, elem, m, nodeType,[m[41m[m
[32m+[m[32m            // QSA vars[m[41m[m
[32m+[m[32m            i, groups, old, nid, newContext, newSelector;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if ((context ? context.ownerDocument || context : preferredDoc) !== document) {[m[41m[m
[32m+[m[32m            setDocument(context);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        context = context || document;[m[41m[m
[32m+[m[32m        results = results || [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!selector || typeof selector !== "string") {[m[41m[m
[32m+[m[32m            return results;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if ((nodeType = context.nodeType) !== 1 && nodeType !== 9) {[m[41m[m
[32m+[m[32m            return [];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!documentIsXML && !seed) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Shortcuts[m[41m[m
[32m+[m[32m            if ((match = rquickExpr.exec(selector))) {[m[41m[m
[32m+[m[32m                // Speed-up: Sizzle("#ID")[m[41m[m
[32m+[m[32m                if ((m = match[1])) {[m[41m[m
[32m+[m[32m                    if (nodeType === 9) {[m[41m[m
[32m+[m[32m                        elem = context.getElementById(m);[m[41m[m
[32m+[m[32m                        // Check parentNode to catch when Blackberry 4.6 returns[m[41m[m
[32m+[m[32m                        // nodes that are no longer in the document #6963[m[41m[m
[32m+[m[32m                        if (elem && elem.parentNode) {[m[41m[m
[32m+[m[32m                            // Handle the case where IE, Opera, and Webkit return items[m[41m[m
[32m+[m[32m                            // by name instead of ID[m[41m[m
[32m+[m[32m                            if (elem.id === m) {[m[41m[m
[32m+[m[32m                                results.push(elem);[m[41m[m
[32m+[m[32m                                return results;[m[41m[m
[32m+[m[32m                            }[m[41m[m
[32m+[m[32m                        } else {[m[41m[m
[32m+[m[32m                            return results;[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    } else {[m[41m[m
[32m+[m[32m                        // Context is not a document[m[41m[m
[32m+[m[32m                        if (context.ownerDocument && (elem = context.ownerDocument.getElementById(m)) &&[m[41m[m
[32m+[m[32m                            contains(context, elem) && elem.id === m) {[m[41m[m
[32m+[m[32m                            results.push(elem);[m[41m[m
[32m+[m[32m                            return results;[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Speed-up: Sizzle("TAG")[m[41m[m
[32m+[m[32m                } else if (match[2]) {[m[41m[m
[32m+[m[32m                    push.apply(results, slice.call(context.getElementsByTagName(selector), 0));[m[41m[m
[32m+[m[32m                    return results;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Speed-up: Sizzle(".CLASS")[m[41m[m
[32m+[m[32m                } else if ((m = match[3]) && support.getByClassName && context.getElementsByClassName) {[m[41m[m
[32m+[m[32m                    push.apply(results, slice.call(context.getElementsByClassName(m), 0));[m[41m[m
[32m+[m[32m                    return results;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // QSA path[m[41m[m
[32m+[m[32m            if (support.qsa && !rbuggyQSA.test(selector)) {[m[41m[m
[32m+[m[32m                old = true;[m[41m[m
[32m+[m[32m                nid = expando;[m[41m[m
[32m+[m[32m                newContext = context;[m[41m[m
[32m+[m[32m                newSelector = nodeType === 9 && selector;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // qSA works strangely on Element-rooted queries[m[41m[m
[32m+[m[32m                // We can work around this by specifying an extra ID on the root[m[41m[m
[32m+[m[32m                // and working up from there (Thanks to Andrew Dupont for the technique)[m[41m[m
[32m+[m[32m                // IE 8 doesn't work on object elements[m[41m[m
[32m+[m[32m                if (nodeType === 1 && context.nodeName.toLowerCase() !== "object") {[m[41m[m
[32m+[m[32m                    groups = tokenize(selector);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if ((old = context.getAttribute("id"))) {[m[41m[m
[32m+[m[32m                        nid = old.replace(rescape, "\\$&");[m[41m[m
[32m+[m[32m                    } else {[m[41m[m
[32m+[m[32m                        context.setAttribute("id", nid);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    nid = "[id='" + nid + "'] ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    i = groups.length;[m[41m[m
[32m+[m[32m                    while (i--) {[m[41m[m
[32m+[m[32m                        groups[i] = nid + toSelector(groups[i]);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    newContext = rsibling.test(selector) && context.parentNode || context;[m[41m[m
[32m+[m[32m                    newSelector = groups.join(",");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (newSelector) {[m[41m[m
[32m+[m[32m                    try {[m[41m[m
[32m+[m[32m                        push.apply(results, slice.call(newContext.querySelectorAll([m[41m[m
[32m+[m[32m                            newSelector[m[41m[m
[32m+[m[32m                        ), 0));[m[41m[m
[32m+[m[32m                        return results;[m[41m[m
[32m+[m[32m                    } catch (qsaError) {[m[41m[m
[32m+[m[32m                    } finally {[m[41m[m
[32m+[m[32m                        if (!old) {[m[41m[m
[32m+[m[32m                            context.removeAttribute("id");[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // All others[m[41m[m
[32m+[m[32m        return select(selector.replace(rtrim, "$1"), context, results, seed);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.fn = {[m[41m[m
[32m+[m[32m        "jquery": '2.0.0',[m[41m[m
[32m+[m[32m        "selector": '',[m[41m[m
[32m+[m[32m        "length": 0[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.fx = function (elem, options, prop, end, easing, unit) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        this.elem = elem;[m[41m[m
[32m+[m[32m        this.prop = prop;[m[41m[m
[32m+[m[32m        this.easing = easing || "swing";[m[41m[m
[32m+[m[32m        this.options = options;[m[41m[m
[32m+[m[32m        this.start = this.now = this.cur();[m[41m[m
[32m+[m[32m        this.end = end;[m[41m[m
[32m+[m[32m        this.unit = unit || (jQuery.cssNumber[prop] ? "" : "px");[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.get = function (url, data, callback, type) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Load data from the server using a HTTP GET request.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="url" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the URL to which the request is sent.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="String">[m[41m[m
[32m+[m[32m        ///     A plain object or string that is sent to the server with the request.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A callback function that is executed if the request succeeds.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="type" type="String">[m[41m[m
[32m+[m[32m        ///     The type of data expected from the server. Default: Intelligent Guess (xml, json, script, or html).[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // shift arguments if data argument was omitted[m[41m[m
[32m+[m[32m        if (jQuery.isFunction(data)) {[m[41m[m
[32m+[m[32m            type = type || callback;[m[41m[m
[32m+[m[32m            callback = data;[m[41m[m
[32m+[m[32m            data = undefined;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.ajax({[m[41m[m
[32m+[m[32m            url: url,[m[41m[m
[32m+[m[32m            type: method,[m[41m[m
[32m+[m[32m            dataType: type,[m[41m[m
[32m+[m[32m            data: data,[m[41m[m
[32m+[m[32m            success: callback[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.getJSON = function (url, data, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Load JSON-encoded data from the server using a GET HTTP request.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="url" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the URL to which the request is sent.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="data" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     A plain object or string that is sent to the server with the request.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A callback function that is executed if the request succeeds.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.get(url, data, callback, "json");[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.getScript = function (url, callback) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Load a JavaScript file from the server using a GET HTTP request, then execute it.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="url" type="String">[m[41m[m
[32m+[m[32m        ///     A string containing the URL to which the request is sent.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     A callback function that is executed if the request succeeds.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.get(url, undefined, callback, "script");[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.globalEval = function (data) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Execute some JavaScript code globally.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="data" type="String">[m[41m[m
[32m+[m[32m        ///     The JavaScript code to execute.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (data && jQuery.trim(data)) {[m[41m[m
[32m+[m[32m            // We use execScript on Internet Explorer[m[41m[m
[32m+[m[32m            // We use an anonymous function so that context is window[m[41m[m
[32m+[m[32m            // rather than jQuery in Firefox[m[41m[m
[32m+[m[32m            (window.execScript || function (data) {[m[41m[m
[32m+[m[32m                window["eval"].call(window, data);[m[41m[m
[32m+[m[32m            })(data);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.grep = function (elems, callback, inv) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Finds the elements of an array which satisfy a filter function. The original array is not affected.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elems" type="Array">[m[41m[m
[32m+[m[32m        ///     The array to search through.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     The function to process each item against.  The first argument to the function is the item, and the second argument is the index.  The function should return a Boolean value.  this will be the global window object.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="inv" type="Boolean">[m[41m[m
[32m+[m[32m        ///     If "invert" is false, or not provided, then the function returns an array consisting of all elements for which "callback" returns true.  If "invert" is true, then the function returns an array consisting of all elements for which "callback" returns false.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Array" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var retVal,[m[41m[m
[32m+[m			[32mret = [],[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mlength = elems.length;[m[41m[m
[32m+[m[32m        inv = !!inv;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Go through the array, only saving the items[m[41m[m
[32m+[m[32m        // that pass the validator function[m[41m[m
[32m+[m[32m        for (; i < length; i++) {[m[41m[m
[32m+[m[32m            retVal = !!callback(elems[i], i);[m[41m[m
[32m+[m[32m            if (inv !== retVal) {[m[41m[m
[32m+[m[32m                ret.push(elems[i]);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return ret;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.guid = 1;[m[41m[m
[32m+[m[32m    jQuery.hasData = function (elem) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Determine whether an element has any jQuery data associated with it.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elem" domElement="true">[m[41m[m
[32m+[m[32m        ///     A DOM element to be checked for data.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        elem = elem.nodeType ? jQuery.cache[elem[jQuery.expando]] : elem[jQuery.expando];[m[41m[m
[32m+[m[32m        return !!elem && !isEmptyDataObject(elem);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.holdReady = function (hold) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Holds or releases the execution of jQuery's ready event.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="hold" type="Boolean">[m[41m[m
[32m+[m[32m        ///     Indicates whether the ready hold is being requested or released[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="undefined" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (hold) {[m[41m[m
[32m+[m[32m            jQuery.readyWait++;[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            jQuery.ready(true);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.inArray = function (elem, arr, i) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Search for a specified value within an array and return its index (or -1 if not found).[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elem" type="Anything">[m[41m[m
[32m+[m[32m        ///     The value to search for.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="arr" type="Array">[m[41m[m
[32m+[m[32m        ///     An array through which to search.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="i" type="Number">[m[41m[m
[32m+[m[32m        ///     The index of the array at which to begin the search. The default is 0, which will search the whole array.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Number" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var len;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (arr) {[m[41m[m
[32m+[m[32m            if (core_indexOf) {[m[41m[m
[32m+[m[32m                return core_indexOf.call(arr, elem, i);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            len = arr.length;[m[41m[m
[32m+[m[32m            i = i ? i < 0 ? Math.max(0, len + i) : i : 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (; i < len; i++) {[m[41m[m
[32m+[m[32m                // Skip accessing in sparse arrays[m[41m[m
[32m+[m[32m                if (i in arr && arr[i] === elem) {[m[41m[m
[32m+[m[32m                    return i;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return -1;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.isEmptyObject = function (obj) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Check to see if an object is empty (contains no enumerable properties).[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="obj" type="Object">[m[41m[m
[32m+[m[32m        ///     The object that will be checked to see if it's empty.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var name;[m[41m[m
[32m+[m[32m        for (name in obj) {[m[41m[m
[32m+[m[32m            return false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return true;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.isFunction = function (obj) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Determine if the argument passed is a Javascript function object.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="obj" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     Object to test whether or not it is a function.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery.type(obj) === "function";[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.isNumeric = function (obj) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Determines whether its argument is a number.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="obj" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     The value to be tested.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return !isNaN(parseFloat(obj)) && isFinite(obj);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.isPlainObject = function (obj) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Check to see if an object is a plain object (created using "{}" or "new Object").[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="obj" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     The object that will be checked to see if it's a plain object.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Must be an Object.[m[41m[m
[32m+[m[32m        // Because of IE, we also have to check the presence of the constructor property.[m[41m[m
[32m+[m[32m        // Make sure that DOM nodes and window objects don't pass through, as well[m[41m[m
[32m+[m[32m        if (!obj || jQuery.type(obj) !== "object" || obj.nodeType || jQuery.isWindow(obj)) {[m[41m[m
[32m+[m[32m            return false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            // Not own constructor property must be Object[m[41m[m
[32m+[m[32m            if (obj.constructor &&[m[41m[m
[32m+[m				[32m!core_hasOwn.call(obj, "constructor") &&[m[41m[m
[32m+[m				[32m!core_hasOwn.call(obj.constructor.prototype, "isPrototypeOf")) {[m[41m[m
[32m+[m[32m                return false;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        } catch (e) {[m[41m[m
[32m+[m[32m            // IE8,9 Will throw exceptions on certain host objects #9897[m[41m[m
[32m+[m[32m            return false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Own properties are enumerated firstly, so to speed up,[m[41m[m
[32m+[m[32m        // if last one is own, then all properties are own.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var key;[m[41m[m
[32m+[m[32m        for (key in obj) { }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return key === undefined || core_hasOwn.call(obj, key);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.isReady = true;[m[41m[m
[32m+[m[32m    jQuery.isWindow = function (obj) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Determine whether the argument is a window.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="obj" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     Object to test whether or not it is a window.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return obj != null && obj == obj.window;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.isXMLDoc = function (elem) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Check to see if a DOM node is within an XML document (or is an XML document).[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elem" domElement="true">[m[41m[m
[32m+[m[32m        ///     The DOM node that will be checked to see if it's in an XML document.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Boolean" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // documentElement is verified for cases where it doesn't yet exist[m[41m[m
[32m+[m[32m        // (such as loading iframes in IE - #4833)[m[41m[m
[32m+[m[32m        var documentElement = elem && (elem.ownerDocument || elem).documentElement;[m[41m[m
[32m+[m[32m        return documentElement ? documentElement.nodeName !== "HTML" : false;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.lastModified = {};[m[41m[m
[32m+[m[32m    jQuery.makeArray = function (arr, results) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Convert an array-like object into a true JavaScript array.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="arr" type="PlainObject">[m[41m[m
[32m+[m[32m        ///     Any object to turn into a native Array.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Array" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var ret = results || [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (arr != null) {[m[41m[m
[32m+[m[32m            if (isArraylike(Object(arr))) {[m[41m[m
[32m+[m[32m                jQuery.merge(ret,[m[41m[m
[32m+[m					[32mtypeof arr === "string" ?[m[41m[m
[32m+[m					[32m[arr] : arr[m[41m[m
[32m+[m				[32m);[m[41m[m
[32m+[m[32m            } else {[m[41m[m
[32m+[m[32m                core_push.call(ret, arr);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return ret;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.map = function (elems, callback, arg) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Translate all items in an array or object to new array of items.[m[41m[m
[32m+[m[32m        ///     &#10;1 - jQuery.map(array, callback(elementOfArray, indexInArray))[m[41m [m
[32m+[m[32m        ///     &#10;2 - jQuery.map(arrayOrObject, callback( value, indexOrKey ))[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="elems" type="Array">[m[41m[m
[32m+[m[32m        ///     The Array to translate.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="callback" type="Function">[m[41m[m
[32m+[m[32m        ///     The function to process each item against.  The first argument to the function is the array item, the second argument is the index in array The function can return any value. Within the function, this refers to the global (window) object.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Array" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var value,[m[41m[m
[32m+[m			[32mi = 0,[m[41m[m
[32m+[m			[32mlength = elems.length,[m[41m[m
[32m+[m			[32misArray = isArraylike(elems),[m[41m[m
[32m+[m			[32mret = [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Go through the array, translating each of the items to their[m[41m[m
[32m+[m[32m        if (isArray) {[m[41m[m
[32m+[m[32m            for (; i < length; i++) {[m[41m[m
[32m+[m[32m                value = callback(elems[i], i, arg);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (value != null) {[m[41m[m
[32m+[m[32m                    ret[ret.length] = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Go through every key on the object,[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            for (i in elems) {[m[41m[m
[32m+[m[32m                value = callback(elems[i], i, arg);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (value != null) {[m[41m[m
[32m+[m[32m                    ret[ret.length] = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Flatten any nested arrays[m[41m[m
[32m+[m[32m        return core_concat.apply([], ret);[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.merge = function (first, second) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Merge the contents of two arrays together into the first array.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="first" type="Array">[m[41m[m
[32m+[m[32m        ///     The first array to merge, the elements of second added.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <param name="second" type="Array">[m[41m[m
[32m+[m[32m        ///     The second array to merge into the first, unaltered.[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Array" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        var l = second.length,[m[41m[m
[32m+[m			[32mi = first.length,[m[41m[m
[32m+[m			[32mj = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (typeof l === "number") {[m[41m[m
[32m+[m[32m            for (; j < l; j++) {[m[41m[m
[32m+[m[32m                first[i++] = second[j];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            while (second[j] !== undefined) {[m[41m[m
[32m+[m[32m                first[i++] = second[j++];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        first.length = i;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return first;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.noConflict = function (deep) {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///     Relinquish jQuery's control of the $ variable.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="deep" type="Boolean">[m[41m[m
[32m+[m[32m        ///     A Boolean indicating whether to remove all jQuery variables from the global scope (including jQuery itself).[m[41m[m
[32m+[m[32m        /// </param>[m[41m[m
[32m+[m[32m        /// <returns type="Object" />[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (window.$ === jQuery) {[m[41m[m
[32m+[m[32m            window.$ = _$;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (deep && window.jQuery === jQuery) {[m[41m[m
[32m+[m[32m            window.jQuery = _jQuery;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return jQuery;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.noData = {[m[41m[m
[32m+[m[32m        "embed": true,[m[41m[m
[32m+[m[32m        "object": 'clsid:D27CDB6E-AE6D-11cf-96B8-444553540000',[m[41m[m
[32m+[m[32m        "applet": true[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.nodeName = function (elem, name) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return elem.nodeName && elem.nodeName.toLowerCase() === name.toLowerCase();[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m    jQuery.noop = function () {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
