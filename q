[32m+[m[32m/*jslint white: true, browser: true, onevar: true, undef: true, nomen: true, eqeqeq: true, plusplus: true, bitwise: true, regexp: true, newcap: true, immed: true, strict: false */[m[41m[m
[32m+[m[32m/*global document: false, jQuery: false */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m(function ($) {[m[41m[m
[32m+[m[32m    var $jQval = $.validator,[m[41m[m
[32m+[m[32m        adapters,[m[41m[m
[32m+[m[32m        data_validation = "unobtrusiveValidation";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function setValidationValues(options, ruleName, value) {[m[41m[m
[32m+[m[32m        options.rules[ruleName] = value;[m[41m[m
[32m+[m[32m        if (options.message) {[m[41m[m
[32m+[m[32m            options.messages[ruleName] = options.message;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function splitAndTrim(value) {[m[41m[m
[32m+[m[32m        return value.replace(/^\s+|\s+$/g, "").split(/\s*,\s*/g);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function getModelPrefix(fieldName) {[m[41m[m
[32m+[m[32m        return fieldName.substr(0, fieldName.lastIndexOf(".") + 1);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function appendModelPrefix(value, prefix) {[m[41m[m
[32m+[m[32m        if (value.indexOf("*.") === 0) {[m[41m[m
[32m+[m[32m            value = value.replace("*.", prefix);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return value;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function onError(error, inputElement) {  // 'this' is the form element[m[41m[m
[32m+[m[32m        var container = $(this).find("[data-valmsg-for='" + inputElement[0].name + "']"),[m[41m[m
[32m+[m[32m            replace = $.parseJSON(container.attr("data-valmsg-replace")) !== false;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        container.removeClass("field-validation-valid").addClass("field-validation-error");[m[41m[m
[32m+[m[32m        error.data("unobtrusiveContainer", container);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (replace) {[m[41m[m
[32m+[m[32m            container.empty();[m[41m[m
[32m+[m[32m            error.removeClass("input-validation-error").appendTo(container);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        else {[m[41m[m
[32m+[m[32m            error.hide();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function onErrors(form, validator) {  // 'this' is the form element[m[41m[m
[32m+[m[32m        var container = $(this).find("[data-valmsg-summary=true]"),[m[41m[m
[32m+[m[32m            list = container.find("ul");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (list && list.length && validator.errorList.length) {[m[41m[m
[32m+[m[32m            list.empty();[m[41m[m
[32m+[m[32m            container.addClass("validation-summary-errors").removeClass("validation-summary-valid");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            $.each(validator.errorList, function () {[m[41m[m
[32m+[m[32m                $("<li />").html(this.message).appendTo(list);[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function onSuccess(error) {  // 'this' is the form element[m[41m[m
[32m+[m[32m        var container = error.data("unobtrusiveContainer"),[m[41m[m
[32m+[m[32m            replace = $.parseJSON(container.attr("data-valmsg-replace"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (container) {[m[41m[m
[32m+[m[32m            container.addClass("field-validation-valid").removeClass("field-validation-error");[m[41m[m
[32m+[m[32m            error.removeData("unobtrusiveContainer");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (replace) {[m[41m[m
[32m+[m[32m                container.empty();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function validationInfo(form) {[m[41m[m
[32m+[m[32m        var $form = $(form),[m[41m[m
[32m+[m[32m            result = $form.data(data_validation);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (!result) {[m[41m[m
[32m+[m[32m            result = {[m[41m[m
[32m+[m[32m                options: {  // options structure passed to jQuery Validate's validate() method[m[41m[m
[32m+[m[32m                    errorClass: "input-validation-error",[m[41m[m
[32m+[m[32m                    errorElement: "span",[m[41m[m
[32m+[m[32m                    errorPlacement: $.proxy(onError, form),[m[41m[m
[32m+[m[32m                    invalidHandler: $.proxy(onErrors, form),[m[41m[m
[32m+[m[32m                    messages: {},[m[41m[m
[32m+[m[32m                    rules: {},[m[41m[m
[32m+[m[32m                    success: $.proxy(onSuccess, form)[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                attachValidation: function () {[m[41m[m
[32m+[m[32m                    $form.validate(this.options);[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                validate: function () {  // a validation function that is called by unobtrusive Ajax[m[41m[m
[32m+[m[32m                    $form.validate();[m[41m[m
[32m+[m[32m                    return $form.valid();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            };[m[41m[m
[32m+[m[32m            $form.data(data_validation, result);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return result;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    $jQval.unobtrusive = {[m[41m[m
[32m+[m[32m        adapters: [],[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        parseElement: function (element, skipAttach) {[m[41m[m
[32m+[m[32m            /// <summary>[m[41m[m
[32m+[m[32m            /// Parses a single HTML element for unobtrusive validation attributes.[m[41m[m
[32m+[m[32m            /// </summary>[m[41m[m
[32m+[m[32m            /// <param name="element" domElement="true">The HTML element to be parsed.</param>[m[41m[m
[32m+[m[32m            /// <param name="skipAttach" type="Boolean">[Optional] true to skip attaching the[m[41m[m
[32m+[m[32m            /// validation to the form. If parsing just this single element, you should specify true.[m[41m[m
[32m+[m[32m            /// If parsing several elements, you should specify false, and manually attach the validation[m[41m[m
[32m+[m[32m            /// to the form when you are finished. The default is false.</param>[m[41m[m
[32m+[m[32m            var $element = $(element),[m[41m[m
[32m+[m[32m                form = $element.parents("form")[0],[m[41m[m
[32m+[m[32m                valInfo, rules, messages;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (!form) {  // Cannot do client-side validation without a form[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            valInfo = validationInfo(form);[m[41m[m
[32m+[m[32m            valInfo.options.rules[element.name] = rules = {};[m[41m[m
[32m+[m[32m            valInfo.options.messages[element.name] = messages = {};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            $.each(this.adapters, function () {[m[41m[m
[32m+[m[32m                var prefix = "data-val-" + this.name,[m[41m[m
[32m+[m[32m                    message = $element.attr(prefix),[m[41m[m
[32m+[m[32m                    paramValues = {};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (message !== undefined) {  // Compare against undefined, because an empty message is legal (and falsy)[m[41m[m
[32m+[m[32m                    prefix += "-";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    $.each(this.params, function () {[m[41m[m
[32m+[m[32m                        paramValues[this] = $element.attr(prefix + this);[m[41m[m
[32m+[m[32m                    });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    this.adapt({[m[41m[m
[32m+[m[32m                        element: element,[m[41m[m
[32m+[m[32m                        form: form,[m[41m[m
[32m+[m[32m                        message: message,[m[41m[m
[32m+[m[32m                        params: paramValues,[m[41m[m
[32m+[m[32m                        rules: rules,[m[41m[m
[32m+[m[32m                        messages: messages[m[41m[m
[32m+[m[32m                    });[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            jQuery.extend(rules, { "__dummy__": true });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (!skipAttach) {[m[41m[m
[32m+[m[32m                valInfo.attachValidation();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        parse: function (selector) {[m[41m[m
[32m+[m[32m            /// <summary>[m[41m[m
[32m+[m[32m            /// Parses all the HTML elements in the specified selector. It looks for input elements decorated[m[41m[m
[32m+[m[32m            /// with the [data-val=true] attribute value and enables validation according to the data-val-*[m[41m[m
[32m+[m[32m            /// attribute values.[m[41m[m
[32m+[m[32m            /// </summary>[m[41m[m
[32m+[m[32m            /// <param name="selector" type="String">Any valid jQuery selector.</param>[m[41m[m
[32m+[m[32m            $(selector).find(":input[data-val=true]").each(function () {[m[41m[m
[32m+[m[32m                $jQval.unobtrusive.parseElement(this, true);[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            $("form").each(function () {[m[41m[m
[32m+[m[32m                var info = validationInfo(this);[m[41m[m
[32m+[m[32m                if (info) {[m[41m[m
[32m+[m[32m                    info.attachValidation();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            });[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    adapters = $jQval.unobtrusive.adapters;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    adapters.add = function (adapterName, params, fn) {[m[41m[m
[32m+[m[32m        /// <summary>Adds a new adapter to convert unobtrusive HTML into a jQuery Validate validation.</summary>[m[41m[m
[32m+[m[32m        /// <param name="adapterName" type="String">The name of the adapter to be added. This matches the name used[m[41m[m
[32m+[m[32m        /// in the data-val-nnnn HTML attribute (where nnnn is the adapter name).</param>[m[41m[m
[32m+[m[32m        /// <param name="params" type="Array" optional="true">[Optional] An array of parameter names (strings) that will[m[41m[m
[32m+[m[32m        /// be extracted from the data-val-nnnn-mmmm HTML attributes (where nnnn is the adapter name, and[m[41m[m
[32m+[m[32m        /// mmmm is the parameter name).</param>[m[41m[m
[32m+[m[32m        /// <param name="fn" type="Function">The function to call, which adapts the values from the HTML[m[41m[m
[32m+[m[32m        /// attributes into jQuery Validate rules and/or messages.</param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery.validator.unobtrusive.adapters" />[m[41m[m
[32m+[m[32m        if (!fn) {  // Called with no params, just a function[m[41m[m
[32m+[m[32m            fn = params;[m[41m[m
[32m+[m[32m            params = [];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        this.push({ name: adapterName, params: params, adapt: fn });[m[41m[m
[32m+[m[32m        return this;[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    adapters.addBool = function (adapterName, ruleName) {[m[41m[m
[32m+[m[32m        /// <summary>Adds a new adapter to convert unobtrusive HTML into a jQuery Validate validation, where[m[41m[m
[32m+[m[32m        /// the jQuery Validate validation rule has no parameter values.</summary>[m[41m[m
[32m+[m[32m        /// <param name="adapterName" type="String">The name of the adapter to be added. This matches the name used[m[41m[m
[32m+[m[32m        /// in the data-val-nnnn HTML attribute (where nnnn is the adapter name).</param>[m[41m[m
[32m+[m[32m        /// <param name="ruleName" type="String" optional="true">[Optional] The name of the jQuery Validate rule. If not provided, the value[m[41m[m
[32m+[m[32m        /// of adapterName will be used instead.</param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery.validator.unobtrusive.adapters" />[m[41m[m
[32m+[m[32m        return this.add(adapterName, function (options) {[m[41m[m
[32m+[m[32m            setValidationValues(options, ruleName || adapterName, true);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    adapters.addMinMax = function (adapterName, minRuleName, maxRuleName, minMaxRuleName, minAttribute, maxAttribute) {[m[41m[m
[32m+[m[32m        /// <summary>Adds a new adapter to convert unobtrusive HTML into a jQuery Validate validation, where[m[41m[m
[32m+[m[32m        /// the jQuery Validate validation has three potential rules (one for min-only, one for max-only, and[m[41m[m
[32m+[m[32m        /// one for min-and-max). The HTML parameters are expected to be named -min and -max.</summary>[m[41m[m
[32m+[m[32m        /// <param name="adapterName" type="String">The name of the adapter to be added. This matches the name used[m[41m[m
[32m+[m[32m        /// in the data-val-nnnn HTML attribute (where nnnn is the adapter name).</param>[m[41m[m
[32m+[m[32m        /// <param name="minRuleName" type="String">The name of the jQuery Validate rule to be used when you only[m[41m[m
[32m+[m[32m        /// have a minimum value.</param>[m[41m[m
[32m+[m[32m        /// <param name="maxRuleName" type="String">The name of the jQuery Validate rule to be used when you only[m[41m[m
[32m+[m[32m        /// have a maximum value.</param>[m[41m[m
[32m+[m[32m        /// <param name="minMaxRuleName" type="String">The name of the jQuery Validate rule to be used when you[m[41m[m
[32m+[m[32m        /// have both a minimum and maximum value.</param>[m[41m[m
[32m+[m[32m        /// <param name="minAttribute" type="String" optional="true">[Optional] The name of the HTML attribute that[m[41m[m
[32m+[m[32m        /// contains the minimum value. The default is "min".</param>[m[41m[m
[32m+[m[32m        /// <param name="maxAttribute" type="String" optional="true">[Optional] The name of the HTML attribute that[m[41m[m
[32m+[m[32m        /// contains the maximum value. The default is "max".</param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery.validator.unobtrusive.adapters" />[m[41m[m
[32m+[m[32m        return this.add(adapterName, [minAttribute || "min", maxAttribute || "max"], function (options) {[m[41m[m
[32m+[m[32m            var min = options.params.min,[m[41m[m
[32m+[m[32m                max = options.params.max;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (min && max) {[m[41m[m
[32m+[m[32m                setValidationValues(options, minMaxRuleName, [min, max]);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else if (min) {[m[41m[m
[32m+[m[32m                setValidationValues(options, minRuleName, min);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else if (max) {[m[41m[m
[32m+[m[32m                setValidationValues(options, maxRuleName, max);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    adapters.addSingleVal = function (adapterName, attribute, ruleName) {[m[41m[m
[32m+[m[32m        /// <summary>Adds a new adapter to convert unobtrusive HTML into a jQuery Validate validation, where[m[41m[m
[32m+[m[32m        /// the jQuery Validate validation rule has a single value.</summary>[m[41m[m
[32m+[m[32m        /// <param name="adapterName" type="String">The name of the adapter to be added. This matches the name used[m[41m[m
[32m+[m[32m        /// in the data-val-nnnn HTML attribute(where nnnn is the adapter name).</param>[m[41m[m
[32m+[m[32m        /// <param name="attribute" type="String">[Optional] The name of the HTML attribute that contains the value.[m[41m[m
[32m+[m[32m        /// The default is "val".</param>[m[41m[m
[32m+[m[32m        /// <param name="ruleName" type="String" optional="true">[Optional] The name of the jQuery Validate rule. If not provided, the value[m[41m[m
[32m+[m[32m        /// of adapterName will be used instead.</param>[m[41m[m
[32m+[m[32m        /// <returns type="jQuery.validator.unobtrusive.adapters" />[m[41m[m
[32m+[m[32m        return this.add(adapterName, [attribute || "val"], function (options) {[m[41m[m
[32m+[m[32m            setValidationValues(options, ruleName || adapterName, options.params[attribute]);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    $jQval.addMethod("__dummy__", function (value, element, params) {[m[41m[m
[32m+[m[32m        return true;[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    $jQval.addMethod("regex", function (value, element, params) {[m[41m[m
[32m+[m[32m        var match;[m[41m[m
[32m+[m[32m        if (this.optional(element)) {[m[41m[m
[32m+[m[32m            return true;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        match = new RegExp(params).exec(value);[m[41m[m
[32m+[m[32m        return (match && (match.index === 0) && (match[0].length === value.length));[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    adapters.addSingleVal("accept", "exts").addSingleVal("regex", "pattern");[m[41m[m
[32m+[m[32m    adapters.addBool("creditcard").addBool("date").addBool("digits").addBool("email").addBool("number").addBool("url");[m[41m[m
[32m+[m[32m    adapters.addMinMax("length", "minlength", "maxlength", "rangelength").addMinMax("range", "min", "max", "range");[m[41m[m
[32m+[m[32m    adapters.add("equalto", ["other"], function (options) {[m[41m[m
[32m+[m[32m        var prefix = getModelPrefix(options.element.name),[m[41m[m
[32m+[m[32m            other = options.params.other,[m[41m[m
[32m+[m[32m            fullOtherName = appendModelPrefix(other, prefix),[m[41m[m
[32m+[m[32m            element = $(options.form).find(":input[name=" + fullOtherName + "]")[0];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        setValidationValues(options, "equalTo", element);[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[32m    adapters.add("required", function (options) {[m[41m[m
[32m+[m[32m        // jQuery Validate equates "required" with "mandatory" for checkbox elements[m[41m[m
[32m+[m[32m        if (options.element.tagName.toUpperCase() !== "INPUT" || options.element.type.toUpperCase() !== "CHECKBOX") {[m[41m[m
[32m+[m[32m            setValidationValues(options, "required", true);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[32m    adapters.add("remote", ["url", "type", "additionalfields"], function (options) {[m[41m[m
[32m+[m[32m        var value = {[m[41m[m
[32m+[m[32m            url: options.params.url,[m[41m[m
[32m+[m[32m            type: options.params.type || "GET",[m[41m[m
[32m+[m[32m            data: {}[m[41m[m
[32m+[m[32m        },[m[41m[m
[32m+[m[32m            prefix = getModelPrefix(options.element.name);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        $.each(splitAndTrim(options.params.additionalfields || options.element.name), function (i, fieldName) {[m[41m[m
[32m+[m[32m            var paramName = appendModelPrefix(fieldName, prefix);[m[41m[m
[32m+[m[32m            value.data[paramName] = function () {[m[41m[m
[32m+[m[32m                return $(options.form).find(":input[name='" + paramName + "']").val();[m[41m[m
[32m+[m[32m            };[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        setValidationValues(options, "remote", value);[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    $(function () {[m[41m[m
[32m+[m[32m        $jQval.unobtrusive.parse(document);[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[32m}(jQuery));[m
